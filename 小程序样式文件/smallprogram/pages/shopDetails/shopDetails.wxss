/* pages/shopDetails/shopDetails.wxss */
.life_details_wrap{
	width: 100%;
    background-color: #efefef;
    padding-top: 100rpx;
    padding-bottom: 150rpx;
    font-size: 0;
}
.life_shop_details_content {
	padding-bottom: 25rpx;
	background-color: #FFFFFF;
}
.life_shop_name {
    font-size: 34rpx;
    color: #222222;
    padding: 30rpx 3%;
}
.life_shop_name .chang{
	float: right;
	color: #FF7800;
	display: inline-block;
	border: 1rpx solid #ff7800;
    padding: 5rpx 15rpx;
}
.life_shop_type{
	overflow: hidden;
	padding: 0 3%;
}
.life_shop_details_content image {
	display: inline-block;
    height: 39rpx;
    width: 27rpx;
}
.life_shop_type image{
	float: left;
    width: 30rpx;
    height: 28rpx;
    margin-right: 5rpx;
}
.life_shop_address, .life_shop_phone, .life_shop_time {
	font-size: 30rpx;
}
.life_shop_address{
	position: relative;
	display: -webkit-box;
	-webkit-box-orient: vertical;
	-webkit-line-clamp: 1;
	overflow: hidden;
	background-color: #efefef;
	height: 70rpx;
	margin-top: 20rpx;
	padding-left: 3%;
	line-height: 70rpx;
	padding-right: 140rpx;
}
.life_shop_address .address_yellow_logo{
  position: relative;
  top: 8rpx;
}
.life_shop_address .phone_btn{
	position: absolute;
	right: 0;
	top: 0;
	background-color: #d9333f;
	width: 140rpx;
	height: 100%;
}
.life_shop_address .phone_btn image{
    width: 41rpx;
    height: 41rpx;
    display: block;
    margin-top: 15rpx;
    margin-left: 50rpx;
}
.life_shop_goods_introduce {
	padding: 30rpx 3%;
	margin-top: 20rpx;
	background-color: #FFFFFF;
	border-bottom: 1rpx solid #f2f2f2;
}
.life_look_more {
	font-size: 25rpx;
	text-align: center;
	line-height: 70rpx;
	background-color: #FFFFFF;
	color: #222222;
	margin-bottom: 20rpx;
}
.life_look_more image{
	display: inline-block;
  width: 25rpx;
  height: 13rpx;
  margin-left:0.2rem;
}
.life_shop_goods_bottom {
	background-color: #FFFFFF;
    margin-bottom: 20rpx;
    padding: 30rpx 3%;
}
.good_list_right_address .black_text{
	color: #222222;
	font-size: 28rpx;
}
.good_list_right_address .red_text{
	color: #d9333f;
	margin-right: 20rpx;
}
.home_good_recommend_title image{
    height: 39rpx;
    width: 29rpx;
    display: inline-block;
    margin-right: 20rpx;
    position: relative;
    top: -12rpx;
}
.home_good_recommend_title,.home_good_recommend_list ul{
	border: none;
	padding-left: 0;
}
.life_shop_goods_bottom .introduce {
  font-size: 24rpx;
}
.home_good_recommend_title strong{
  font-size: 34rpx;
  position:relative;
  top:-15rpx;

}
.good_list_right_deduction{
	color: #222222;
}
.good_list_right_deduction .red_text{
	color: #d9333f;
  font-size: 30rpx;
}
.good_list_right_deduction .gray_text{
	color: #afafaf;
	margin-left: 20rpx;
}
.home_good_recommend_title .business_hours{
	font-size: 19rpx;
	float: right;
	color: #222222;
}
.full_eva_content .details_eva_title{
    padding: 28rpx 3%;
    border-bottom: 1rpx solid #f5f5f5;
}
.full_eva_content .details_eva_title .span1{
	font-size: 34rpx;
}
.full_eva_content .details_eva_title .span2{
  font-size: 20rpx;
  margin-left: 20rpx;
	color: #bababa;
}
.full_eva_content .details_eva_title image{
	float: right;
  width: 18rpx;
  height: 30rpx;
  margin-top:7rpx;
}
.life_shop_btm{
	overflow: hidden;
	width: 100%;
	position: fixed;
	bottom: 0;
	height: 110rpx;
	min-width: 320px;
	max-width: 750px;
	background-color: #ffffff;
	z-index: 999;
	border-top: 1rpx solid #f4f4f4;
}
.life_shop_btm .shop_btm_left,.life_shop_btm .shop_btm_right{
	width: 50%;
	float: left;
}
.life_shop_btm .shop_btm_right{
	background-color: #ff5858;
	color: white;
	font-size: 42rpx;
	line-height: 110rpx;
	text-align: center;
}
.life_shop_btm .shop_btm_share,.life_shop_btm .shop_btm_coloect{
	width: 50%;
	float: left;
}
.shop_btm_left .shop_btm_share{
	border-right: 1rpx solid #f4f4f4;
	box-sizing: border-box;
}
.shop_btm_left image{
    width: 56rpx;
    height: 60rpx;
	margin: 0 auto;
	margin-top: 10rpx;
}
.shop_btm_left view{
	text-align: center;
	font-size: 24rpx;
	color: #d2d2d2;
}
/*弹出框*/
.phone_alert_gray{
	width: 100%;
	height: 100%;
	max-width: 750px;
	min-width: 320px;
	background-color: rgba(0,0,0,0.5);
	position: fixed;
	top: 0;
	z-index: 9999;
	display: none;
}
.phone_alert{
	width: 4.80rpx;
	background-color: white;
	position: absolute;
	left: 50%;
    margin-left: -2.40rpx;
    top: 50%;
    margin-top: -1.40rpx;
}
.phone_alert .phone_num{
    width: 100%;
    height: 1.85rpx;
    overflow: hidden;
    line-height: 1.85rpx;
    font-size: 0.36rpx;
    text-align: center;
}
.phone_btn{
	width: 100%;
	overflow: hidden;
}
.phone_btn span{
	width: 50%;
	float: left;
	text-align: center;
	line-height: 80rpx;
  font-size: 36rpx;
	color: white;
}
.phone_btn .phone_cancel{
	background-color: #a3a3a3;
}
.phone_btn .phone_sure{
	background-color: #d9333f;
}
.phone_btn .phone_sure a{
	display: inline-block;
	width: 100%;
	height: 100%;
	color: white;
}
.phone_alert_gray2{
	width: 100%;
	height: 100%;
	background-color: transparent;
	position: absolute;
	top: 0;
	left: 0;
}
.home_good_recommend_list{
	width: 100%;
}
.home_good_recommend_list .good_list_left_img{
	float: left;
	width: 160rpx;
    height: 160rpx;
}
.home_good_recommend_list .good_list_left_img image{
	width: 100%;
	height: 100%;
}
.home_good_recommend_list .good_list_right_text{
	width: 100%;
	height: 160rpx;
    padding-left: 180rpx;
	box-sizing: border-box;
}
.home_good_recommend_list ul li{
	margin-top: 20rpx;
}
.good_list_right_text .right_text_goods_name,.good_list_right_text .good_list_right_address,.good_list_right_text .good_list_right_deduction{
	height: 55rpx;
	width: 100%;
	line-height: 55rpx;
	font-size: 30rpx;
	overflow: hidden;
}
.good_list_right_text .good_list_right_deduction{
	font-size: 28rpx;
}
.home_good_recommend_list ul{
	margin:10rpx 0;
  border-top: 1rpx dashed #d7d7d7;
}
.home_good_recommend_list ul {
    border-top: 1rpx dashed transparent;
    margin-top:0;
}
/*评价样式*/
.full_eva_content{
	width: 100%;
	background-color: white;
	padding-bottom: 100rpx;
	margin-top: 20rpx;
	border-top: 1rpx solid #dadada;
}
.full_eva_content .full_content_title{
	width: 100%;
	display: flex;
	justify-content: space-around;
	padding: 30rpx 0;
	border-bottom: 1rpx solid #dadada;
}
.full_content_title span{
	width: 22%;
	display: inline-block;
	border: 2rpx solid #cccccc;
	text-align: center;
	font-size: 24rpx;
	color: #484545;
	line-height: 55rpx;
	border-radius: 10rpx;
}
.full_eva_center{
	width: 100%;
}
.full_eva_ul li{
	padding: 20rpx 4%;
	box-sizing: border-box;
	border-bottom: 1rpx solid #f5f5f5;
	overflow: hidden;
  display:block;
}
/* .full_eva_ul li:nth-last-child(1){
	border-bottom: none;
} */
.full_eva_ul li .eva_left_photo,.full_eva_ul li .eva_right_details{
	float: left;
}
.eva_left_photo{
	width: 20%;
}
.eva_right_details{
	width: 80%;
	padding-top: 20rpx;
}
.eva_left_photo image{
	border-radius: 50%;
  width: 94rpx;
  height: 94rpx;
}
.full_eva_star,.full_eva_img{
	overflow: hidden;
}
.full_eva_star image{
	float: left;
	width: 20rpx;
	height: 20rpx;
}
.full_eva_img image{
	float: left;
	width: 133rpx;
	height: 133rpx;
	margin-right: 22rpx;
	margin-bottom: 20rpx;
}
.full_eva_user{
	overflow: hidden;
}
.full_eva_user span{
	font-size: 30rpx;
	color: #484545;
}
.full_eva_user .span_name{
	float: left;
}
.full_eva_user .span_time{
	float: right;
	font-size: 22rpx;
	color: #cccccc;
}
.eva_right_details view{
	margin-bottom: 15rpx;
}
.full_eva_ul .full_eva_remark{
	font-size: 30rpx;
	color: #484545;
	line-height: 40rpx;
  /* display: inline-block; */
}
.full_eva_ul .spend_time{
	font-size: 22rpx;
	color: #cccccc;
}
/*评价头部选中状态*/
.full_content_title .span_click{
	border-color: #ffa100;
	color: #ffa100;
}