body{
    width: 100%;
    min-width: 320px;
    max-width: 640px;
    margin: 0 auto;
    font-family: "微软雅黑";
}
html{
    background-color:#ebebeb;
}
img{
  border:0;
  display: block;
}
.clearfix:after {
visibility: hidden;
display: block;
font-size: 0;
content: " ";
clear: both;
height: 0;
}
.tihuoquan-box{
    width: 100%;
    height: 100%;
    background-color: #eaeaea;
}
.main-top{
    width: 100%;
    height: 39px;
    background-color: #5f7c95;
    font-family:"微软雅黑";
}
.main-top img{
    position: absolute;
    float: left;
    width: 10px;
    margin-top: 10px;
    margin-left: 13px;

}
.main-top p{
    width: 100%;
    letter-spacing: 2px;
    font-size: 14px;
    color: #fff;
    text-align: center;
    line-height:39px;
}
.dian-tit{
    width: 100%;
    padding: 8px 19px;
    background-color: #fff;
    border-bottom:1px solid #cfcfcf;
}

.number{
    width: 100%;
    background-color: #fff;
    border-top:1px solid #cfcfcf;
}
.number p{
    padding: 0 15px;
    /*color: #888588;*/
    font-size: 13px;
    line-height: 25px;
}
.number p.juanma{
    line-height: 35px;
    border-bottom:1px solid #cfcfcf;
    width: 100%;
}
.inp_yzm{
	line-height: 35px;
	border-bottom: 1px solid #cfcfcf;
	background-color: white;
	padding: 0 15px;
}
.inp_btn{
	float: right;
	width: 100px;
	height: 30px;
	line-height: 30px;
	font-size: 12px;
	border-radius: 5px;
	background-color: white;
	border: 1px solid #cfcfcf;
	outline:none
}
.inp_text,.inp_yzm_text{
	border: none;
	outline:none;
	font-size: 13px;
}
.tihuo p{
    padding-left: 15px;
    font-size: 12px;
    background-color: #fff;
    width: 100%;
    height: 30px;
    line-height: 28px;
    margin-top: 15px;
    border-top:1px solid #cfcfcf;
    border-bottom:1px solid #cfcfcf;
}
.tihuo p img{
    width: 20px;
    float: left;
    margin-top: 3px;
    margin-right: 12px;
}
.tihuo p span{
    float: right;
    margin-right: 15px;
}
.middle p{
    background-color: #fff;
    width: 100%;
    padding:0 17px;
    line-height: 40px;
    font-size: 13px;
}

.bottom{
    width: 100%;
    margin-top: 20px;
}
.bottom p{
	display: inline-block;
    width: 100%;
    margin:0 auto;
    text-align: center;
}
.bottom p span{
	display: inline-block;
    font-size: 12px;
    border-radius: 8px;
    width: 80%;
    background-color: #e84e40;
    height: 35px;
    line-height: 35px;
    color: #fff;
    text-align: center;
}
.bottom p span.right{
    margin-left: 20px;
    background-color: #78909c;
}
.alert_win{
	width: 80%;
	position: absolute;
	top: 40%;
	left: 10%;
	border: 1px solid #757576;
	border-radius: 10px;
	padding: 30px 0;
	background-color: white;
}
.alert_title{
	width: 100%;
	text-align: center;
	font-size: 20px;
	color: #5f7c95;
	line-height: 50px;
	margin-bottom: 20px;
}
.alert_sure_btn{
	width: 80%;
	height: 40px;
	color: white;
	background-color: #5f7c95;
	border: 1px solid #5f7c95;
	margin-left: 10%;
	border-radius: 5px;
	outline: none;
}
