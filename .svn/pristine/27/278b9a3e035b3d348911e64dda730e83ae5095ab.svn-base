	<body>
		<!--导航栏-->
		<div class="life_home_nav">
			<div class="life_nav_left">
				<a href="###"><img src="img/return_img.png"/></a>
			</div>
			<div class="life_home_nav_center">
				选择所在地
			</div>
		</div>
		<div class="life_select_address_wrap">
			<div class="life_select_content_inp">
				<div class="life_select_inp_div">
					<input type="text" name="" id="" value="" placeholder="搜索城市" />
