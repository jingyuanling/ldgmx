body{
	background-color: #f3f5f7;
}
.wine_withdraw_balance{
	width: 100%;
	overflow: hidden;
	padding: 39px 1.0em;
	box-sizing: border-box;
	line-height: 2.0em;
}
.wine_withdraw_balance span{
	font-size: 0.7em;
}
.wine_withdraw_balance_span1{
	float: left;
}
.wine_withdraw_balance_span2{
	float: right;
}
.wine_withdraw_balance_btn{
	font-size: 0.65em;
	text-align: center;
	width: 60%;
	margin: 0 auto;
	line-height: 2.5em;
	margin-top: 0.5em;
	margin-bottom: 0.3em;
	border: 1px solid black;
}
.wine_withdraw_balance_prompt{
	font-size: 0.5em;
	color: gray;
	text-align: center;
}
/*提现成功*/
.wine_withdraw_success_wrap{
	width: 100%;
	position: absolute;
}
.wine_withdraw_success_span1,.wine_withdraw_success_span2{
	display: block;
	width: 100%;
	text-align: center;
	margin-bottom: 0.5em;
}
.wine_withdraw_success_span1{
	font-size: 1.2em;
}
.wine_withdraw_success_span2{
	font-size: 1.0em;
	margin-bottom: 1.0em;
}