html,body{
	height: 100%;
	max-width: 750px;
	min-width: 320px;
	margin: 0 auto;
	font-family: "微软雅黑";
}
.container-fluid::-webkit-scrollbar{
	display: none;
}
.container-fluid{
	height: 100%;
	overflow-y: scroll;
	padding: 0.88rem 0.45rem;
	background-color: white;
}
button{
	outline: 0!important;
	border-color: transparent!important;
}
.material-nav{
	width: 100%;
	height: 0.88rem;
	position: fixed;
	min-width: 320px;
	max-width: 750px;
	color: #050001;
	padding: 0 0.45rem;
	background-color: white;
	border-bottom: 0.02rem solid #eeeeee;
	z-index: 9;
}
.material-nav .glyphicon{
	display: inline-block;
	position: absolute;
}
.material-nav .glyphicon,.material-nav .text-center{
	font-size: 0.3rem;
    line-height: 0.88rem;
}
.material-nav-inp{
	padding-left: 0.6rem;
	height: 0.6rem;
	margin-top: 0.09rem;
	background-color: #eeeeee;
}
.material-nav .form-group{
	position: relative;
}
.material-nav .search{
	display: inline-block;
	width: 0.31rem;
	height: 0.31rem;
	color: #000000;
	position: absolute;
	top: 0.15rem;
	left: 0.15rem;
}
.material-nav .search img{
	width: 100%;
	height: 100%;
}
/*底部*/
.material-btm{
	width: 100%;
	height: 0.88rem;
	background-color: white;
	border-top: 0.02rem solid #eeeeee;
	position: fixed;
	bottom: 0;
	max-width: 750px;
	min-width: 320px;
	font-size: 0;
}
.material-btm-ul{
	display: flex;
	justify-content: space-around;
}
.material-btm-ul li{
	padding-top: 0.05rem;
}
.material-btm-ul img{
	height: 0.52rem;
}
.material-btm-ul .text-center{
	line-height: 0.3rem;
	font-size: 0.16rem;
	color: #8a8a8a;
}
.material-btm-ul .text-red{
	color: #f03b42;
}
