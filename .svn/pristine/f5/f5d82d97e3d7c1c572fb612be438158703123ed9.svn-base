body{
    width: 100%;
    min-width: 320px;
    max-width: 640px;
    margin: 0 auto;
    font-family: "微软雅黑";
}
html{
     background-color: #f0f0f2;
}
img{
  border:0;
  display: block;
}
.clearfix:after {
visibility: hidden;
display: block;
font-size: 0;
content: " ";
clear: both;
height: 0;
}
.searchresult-box{
    width: 100%;
    background-color:#f0f0f2
}
.main-top{
    width: 100%;
    height: 39px;
    background-color: #548e11;
}
.main-top img{
    position: absolute;
    float: left;
    width: 10px;
    margin-top: 10px;
    margin-left: 13px;

}
.main-top img.serch{
    position: relative;
   /* position: absolute;*/
    float: right;
    width: 18px;
    margin-top: -28px;
    margin-right: 13px;

}
.main-top p{
    width: 100%;
    letter-spacing: 2px;
    font-size: 14px;
    color: #fff;
    text-align: center;
    line-height:39px;
}
.search-cont{
    width: 100%;
    padding: 8px 10px;
}
.search-cont li{
    width: 49%;
    float: left;
    background-color: #fff;
    height: 13em;
    list-style: none;
    margin-right: 2%;
    margin-bottom: 8px;
}
.search-cont li.right{
    margin-right: 0px;
}
.search-cont li img{
    width: 6em;
    margin:0 auto;
    margin-top: 1em;
}
.search-cont li .cont{
    height: 4.5em;
    margin-top: 0.7em;
    font-size: 0.8em;
    line-height: 1.5em;
    text-align: center;
    width: 100%;
}
