        var footerPlatforms4 = mchb.platformEnum(toPcSkipurl,sid).enum3_1;//标准版 触屏版 电脑版 客户端 4个
        var bottomArg = {bottomId : 'm_common_bottom', sid : sid, pin : pin ,footerPlatforms : footerPlatforms4};
        mchb.bottom(bottomArg);

