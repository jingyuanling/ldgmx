//        mchb.bottom(bottomArg);

        //通用提示下载京东客户端
        /*
            参数说明：
            名称						必填项(Y,N)			描述
            tipId				|		Y		|	显示divId
            sid					|		N		|	m版sid串  (可取'')
            isfloat 			| 		N		|	是否是悬浮窗 (true:悬浮,false:不悬浮)
            isAlwayShow			| 		N		|	是否显示,true:并不受到 24小时 点击 关闭X 的影响。 默认：false
            copyWrite			|		N		|	文案 富文本 支持，不建议太长可能会导致样式变形。eg:客户端首单返5元<br>更多特价商品等你抢
            location			|		N		|	停靠位置,(取值范围：'bottom:0px;':靠近底端  ，'top:0px;':靠近顶端)
            onClickTrynow		|		N		|	点击下载触发事件
            onClickTipX			|		N		|	点击关闭X触发事件
            downloadAppPlugIn	|		N		|	配置跳转app参数 (包含：parameters,otherApp参数)
                                                    具体详见 http://st.360buyimg.com/m/js/2014/module/plugIn/downloadAppPlugIn.src.js
        */
        var tipArg = {tipId : 'm_common_tip', sid : sid, isfloat: true, isAlwayShow : true,
            copyWrite:'自己自定义的文案<br>返卷100哦！',
            onClickTrynow: function(){},
            onClickTipX: function(){alert("自定义点击事件,并默认执行关闭app事件")}};
        mchb.jdTip(tipArg);

        var tipArg2 = {tipId : 'm_common_tip_no_float', sid : sid, isfloat: false, isAlwayShow : true,
            onClickTrynow: function(){},
            onClickTipX: function(){alert("自定义点击事件,并默认执行关闭app事件")}};

        mchb.jdTip(tipArg2);

