@charset "utf-8";
/* CSS Document */
.commoditylist{
	width:1200px;
	margin:30px auto;
}
.commodityindex:after{
	content:"";
	display:block;
	clear:both;
	height:0;
	visibility:hidden;
}
.crumbs-nav-main{
	float:left;
	margin-right:3px;
}
.crumbs-nav-main .crumbs-link{
	font-size:18px;
	font-weight:600;
	margin-right:5px;
	display:inline-block;
	line-height:24px;
}
.crumbs-nav-main .crumbs-arrow{
	font-family:SimSun-ExtB;
	font-style:normal;
	line-height:24px;
}
.crumbs-nav-main{
	font-size:12px;
}
.menu-drop{
	position:relative;
    display: inline-block;
	margin-right:5px;
}
.trigger{
	line-height:22px;
	padding:0 4px 0 8px;
	border:1px solid #ddd;
	cursor:pointer;
}
.trigger .menu-drop-arrow{
	display:inline-block;
	height:8px;
	width:15px;
	margin-left:2px;
	background:url(../img/tubiao.png) no-repeat -126px -87px;
	-webkit-transition: background-position .15s ease-in-out;
	-moz-transition: background-position .15s ease-in-out;
	transition: background-position .15s ease-in-out;
}
.z-menu-drop-open{
	position: relative;
    z-index: 5;
}
.menu-drop:hover .trigger{
	border:1px solid #ff4500;
	border-bottom:1px solid #fff;
	position:relative;
	z-index:6;
}
.menu-drop:hover .menu-drop-arrow{
	background-position:-126px -98px;
}
.menu-drop-main{
	display:none;
    width: 350px;
    padding: 10px 0 10px 8px;
    position: absolute;
    left: 0;
    top: 23px;
    border: 1px solid #ddd;
    background-color: #fff;
}
.menu-drop:hover .menu-drop-main{
	border-color: #ff4500;
	display:block;
}
.menu-drop-main li{
	float: left;
    width: 65px;
    height: 24px;
    overflow: hidden;
    line-height: 24px;
    margin-right: 5px;
    text-overflow: ellipsis;
    white-space: nowrap;
}
.commodityindex a:hover{
	color:#ff4500;
}
.w{
	margin-top:20px;
}
.selector{
	border-top: 1px solid #DDD;
    background: #FFF;
    margin-bottom: 10px;
}
.s-title{
	overflow:hidden;
	border-bottom: 1px solid #DDD;
    background: #F1F1F1;
    line-height: 34px;
    height: 34px;
}
.s-title h3{
	float:left;
	padding-left:10px;
	font-size:15px;
}
.s-title h3 b{
	color: #e4393c;
    margin-right: 5px;
}
.s-title h3 em{
	font-style:normal;
}
.s-title .st-ext{
	float:left;
	padding-left:20px;
	font-size:14px;
}
.J_selectorLine{
	font-size:14px;
}
.s-line a{
	display:inline-block;
}
.sl-wrap{
	position: relative;
    line-height: 34px;
    border-bottom: 1px solid #DDD;
	overflow:hidden;
}
.sl-key{
	float: left;
    width: 100px;
    padding-left: 10px;
}
.sl-value{
	margin-left: 110px;
    padding-right: 130px;
    padding-left: 10px;
    overflow: hidden;
	background:#fff;
}
.sl-b-letter{
	display:none;
	float: left;
    overflow: hidden;
    zoom: 1;
    padding-top: 6px;
    margin-bottom: 10px;
}
.sl-b-letter li{
	float:left;
	padding: 0 8px;
	height: 20px;
    line-height: 20px;
	color: #005aa0;
	border: 1px solid #FFF;
	cursor: default;
	text-align: center;
}
.sl-b-letter li.curr{
	border-color: #e4393c;
    color: #e4393c;
}
.clr{
	display: block;
    overflow: hidden;
    clear: both;
    height: 0;
    line-height: 0;
    font-size: 0;
}
.sl-v-list{
	overflow: hidden;
    zoom: 1;
    padding-top: 4px;
	margin-bottom:10px;
}
.v-fixed{
	height: 150px;
    overflow-x: hidden;
    overflow-y: auto;
}
.sl-v-list li{
    float: left;
    margin-bottom: 4px;
    height: 26px;
    line-height: 26px;
	font-size:12px;
	width: 130px;
    margin-right: 5px;
}
.sl-v-list li a{
	color:#005aa0;
}
.J_selectorLine .sl-v-list li a:hover{
	color:#ff4500;
}
.s-line .sl-v-list li a{
	width: 130px;
    display: inline-block;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
	color: #005aa0;
}
.sl-ext{
	position: absolute;
    top: 6px;
    right: 10px;
    width: 120px;
    height: auto;
    line-height: 22px;
    overflow: hidden;
	font-size:12px;
}
.sl-ext .sl-e-more{
    float: left;
    margin-right: 20px;
    position: relative;
    width: 50px;
    height: 22px;
    background: #fff;
    color: #333;
	line-height:22px;
}
.sl-ext .sl-e-more i{
    position: absolute;
    top: 0;
    right: 0;
    display: block;
    width: 20px;
    height: 20px;
    border: 1px solid #DDD;
	background:url(../img/tubiao.png) no-repeat -124px -109px;
	-webkit-transition: background-position .15s ease-in-out;
	-moz-transition: background-position .15s ease-in-out;
	transition: background-position .15s ease-in-out;
}
.sl-ext .sl-e-more:hover{
	color: #ff4500;
}
.sl-ext .sl-e-more:hover i{
	border:1px solid #ff4500;
	background-position:-124px -128px;
}
.sl-ext .sl-e-multiple{
    float: left;
    height: 20px;
    line-height: 20px;
    border: 1px solid #DDD;
    padding: 0 3px 0 21px;
    position: relative;
    background: #fff;
    color: #333;
}
.sl-ext .sl-e-multiple i{
    position: absolute;
    display: block;
    font-style: normal;
    left: 3px;
    top: 2px;
    width: 15px;
    height: 15px;
	background:url(../img/tubiao.png) no-repeat -126px -154px;
	-webkit-transition: background-position .15s ease-in-out;
	-moz-transition: background-position .15s ease-in-out;
	transition: background-position .15s ease-in-out;
}
.sl-ext .sl-e-multiple:hover i{
	background-position:-126px -180px;
}
.sl-ext .sl-e-multiple:hover{
	border:1px solid #ff4500;
	color:#ff4500;
}
.extend{
	margin-top: -1px;
    border: 2px solid #AAA;
    background: #f9f9f9;
}
.s-more{
    text-align: center;
    position: relative;
    top: -1px;
    border-top: 1px solid #ddd;
    -webkit-transition: border-color .15s ease-in-out;
    -moz-transition: border-color .15s ease-in-out;
    transition: border-color .15s ease-in-out;
	font-size:12px;
}
.s-more:hover{
	border-color:#ff4500;
}
.s-more:hover .sm-wrap{
	border-color: #FFF #ff4500 #ff4500;
	color:#ff4500;
}
.s-more .sm-wrap{
    position: relative;
    display: inline-block;
    margin-top: -1px;
    height: 28px;
    line-height: 28px;
    padding: 0 40px 0 20px;
    border-width: 1px;
    border-style: solid;
    border-color: #FFF #DDD #DDD;
    background: #fff;
    cursor: pointer;
    color: #333;
    -webkit-transition: border-color .15s ease-in-out,color .15s ease-in-out;
    -moz-transition: border-color .15s ease-in-out,color .15s ease-in-out;
    transition: border-color .15s ease-in-out,color .15s ease-in-out;
}
.s-more .sm-wrap i{
	display:inline-block;
	width:15px;
	height:8px;
	background:url(../img/tubiao.png) no-repeat -152px -90px;
}
.s-more:hover i{
	background-position:-152px -98px;
}
.g-main2{
	margin-bottom:10px;
	font-size:12px;
}
.filter{
    position: relative;
    z-index: 4;
    border-top: 1px solid #DDD;
    margin-bottom: 5px;
}
.filter .f-line.top {
    height: 25px;
    background: #F1F1F1;
}
.filter .f-line {
    padding: 6px 8px;
    border-bottom: 1px solid #E7E3E7;
    background: #F9F9F9;
    zoom: 1;
}
.clearfix:after {
    content: ".";
    display: block;
    height: 0;
    clear: both;
    visibility: hidden;
}
.filter .f-sort {
    float: left;
    margin-right: 24px;
}
.shanghupage .filter .f-sort a.curr {
    background: #e4393c;
    color: #FFF;
    border-color: #e4393c;
	-webkit-box-shadow:0 -9px 15px #cd2629 inset;
	-moz-box-shadow:0 -9px 15px #cd2629 inset;
	box-shadow:0 -9px 15px #cd2629 inset;
}
.filter .f-sort a {
    float: left;
    padding: 0 10px;
    height: 23px;
    border: 1px solid #CCC;
    line-height: 23px;
    margin-right: -1px;
    background: #FFF;
    color: #333;
}
.filter .f-search{
	float:left;
}
.filter .f-search .input-txt {
    width: 108px;
    color: #999;
	height: 17px;
    line-height: 17px;
    border: 1px solid #CCC;
    padding: 3px;
	float:left;
}
.btn-default {
	float:left;
    display: inline-block;
    line-height: 14px;
    height: 14px;
    background: #F7F7F7;
    text-align: center;
    text-decoration: none;
    cursor: pointer;
    padding: 4px 13px 5px;
    background-color: #f4f4f4;
}
.filter .f-search .btn {
    color: #333;
    border: 1px solid #CCC;
    border-radius: 0;
    margin-left: -1px;
}
.filter .f-pager .fp-text {
    line-height: 23px;
    margin-right: 10px;
    font-family: verdana;
}
.filter .f-pager .fp-text b {
    color: #e4393c;
    font-weight: 700;
}
.filter .f-pager .fp-text em,.filter .f-pager .fp-text i{
    font-weight: 400;
    font-style: normal;
}
.filter .f-pager .fp-next.disabled, .filter .f-pager .fp-prev.disabled {
    padding: 0;
    background: #F1F1F1;
    color: #CCC;
    cursor: default;
}
.filter .f-pager .fp-next,.filter .f-pager .fp-prev {
    width: 46px;
    height: 23px;
    border: 1px solid #DDD;
    background: #FFF;
    line-height: 23px;
    font-family: "\5b8b\4f53";
    text-align: center;
    font-size: 16px;
    color: #AAA;
}
.filter .f-pager .fp-next, .filter .f-pager .fp-prev, .filter .f-pager .fp-text {
    float: left;
}
.filter .f-pager {
    float: right;
}
.filter .f-store {
    float: left;
    padding-left: 5px;
}
filter .f-store .delivery-location {
    color: #999;
}
.filter .f-store .fs-cell {
    float: left;
    line-height: 25px;
    margin-right: 5px;
}
.filter .f-store #store-selector {
    float: left;
    height: 25px;
}
#store-selector {
    position: relative;
    z-index: 5;
    height: 26px;
    margin-right: 10px;
}
.filter .f-store #store-selector .text {
    color: #333;
}
#store-selector .text {
    float: left;
    position: relative;
    top: 0;
    overflow: hidden;
    height: 23px;
    padding: 0 24px 0 4px;
    border: 1px solid #ccc;
    background: #fff;
    line-height: 23px;
}
#store-selector .text b {
    display: block;
    position: absolute;
    right: 5px;
    top: 8px;
    overflow: hidden;
    width: 15px;
    height: 8px;
    background:url(../img/tubiao.png) no-repeat -152px -90px;
}
#store-selector .content {
	display:none;
    position: absolute;
    top: 24px;
    left: -45px;
    border: 1px solid #999;
    width: 390px;
    padding: 15px;
    background: #fff;
    box-shadow: 2px 2px 3px rgba(0,0,0,.12);
}
.JD-stock {
    position: relative;
}
.m, .sm {
    margin-bottom: 10px;
}
.m, .mb, .mc, .mt, .sm, .smb, .smc, .smt {
    overflow: hidden;
    zoom: 1;
}
.JD-stock .tab {
    width: 100%;
    height: 25px;
    float: left;
    border-bottom: 2px solid #e4393c;
    overflow: visible;
}
.JD-stock .tab li {
    float: left;
    clear: none;
    height: 23px;
    padding: 1px 1px 0;
    border: 1px solid #ddd;
    border-bottom: 0;
    margin-right: 3px;
    background-color: #fff;
    line-height: 22px;
    text-decoration: none;
}
.JD-stock .tab a {
    position: relative;
    float: left;
    height: 23px;
    padding: 0 30px 1px 10px;
    line-height: 23px;
    text-align: center;
    text-decoration: none;
    cursor: pointer;
    color: #005AA0;
    outline: 0;
}
.JD-stock .tab a i {
    position: absolute;
    right: 2px;
    top: 5px;
    display: block;
    width: 18px;
    height: 16px;
    overflow: hidden;
    opacity: .5;
    filter: alpha(opacity=50);
}
.JD-stock .tab a em{
	font-style:normal;
}
.JD-stock .tab .curr {
    height: 25px;
    padding: 0;
    border: 2px solid #e4393c;
    border-bottom: 0;
}
.JD-stock .mc {
    min-height: 239px;
    _height: 289px;
}
.area-list {
    padding-top: 5px;
}
.area-list li {
    float: left;
    width: 80px;
    padding: 2px 0 2px 15px;
    clear: none;
}
.area-list li a {
    float: left;
    padding: 2px 4px;
    color: #005aa0;
}
#store-selector .close {
    display: none;
    position: absolute;
    cursor: pointer;
    z-index: 2;
    top: 32px;
    left: 346px;
    width: 17px;
    height: 17px;
    background: url(//misc.360buyimg.com/product/list/1.0.4/css/i/search.ele.png) no-repeat 3px -124px;
}
.filter .f-store .fs-cell {
    float: left;
    line-height: 25px;
    margin-right: 5px;
}
.filter .f-feature ul li a i,.filter .f-store .instock a i {
    position: absolute;
    left: 0;
    top: 6px;
    background: url(//misc.360buyimg.com/product/list/1.0.4/css/i/search.ele.png) no-repeat 9999px 9999px;
    display: inline-block;
    height: 12px;
    width: 12px;
    background-color: #fff;
    border: 1px solid #ccc;
    font-size: 0;
}
.filter .f-feature {
    float: left;
    position: relative;
    width: auto;
    height: 25px;
    overflow: hidden;
}
.filter .f-feature ul li {
    float: left;
    display: inline;
    height: 25px;
    line-height: 25px;
}
.filter .f-feature ul li a, .filter .f-store .instock a {
    position: relative;
    display: inline-block;
    padding-left: 18px;
    padding-right: 10px;
    color: #333;
}
.filter .f-feature ul li a {
    padding-right: 5px;
}
