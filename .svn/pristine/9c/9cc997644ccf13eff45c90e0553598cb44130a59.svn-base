@charset "utf-8";
/* CSS Document */

.activitybg{
	margin-top:1px;
	background:url(../img/yun1.png) #d71936 no-repeat center bottom;
	padding-bottom:100px;
}
.yunbg{
	width:1200px;
	margin:auto;
	background:url(../img/yun.png) no-repeat;
}
.activitybox{
	width:1000px;
	margin:auto;
	overflow:hidden;
}
.activityleft{
	float:left;
	width:344px;
	margin-top:33px;
}
.activityleft .huojiangbang{
	width:285px;
	height:135px;
	background:#d20f25;
	margin:5px auto 0;
	padding:5px 10px;
}
.activityleft .huojiangbang h3{
	font-size:16px;
	color:#fff;
}
.activityleft .huojiangbang .zhongjianbox{
	width:250px;
	height:95px;
	margin:7px auto 0;
	background:#fff;
	overflow:hidden;
	padding:5px 8px;
}
#demo2,#demo3{
	font-size:12px;
}
.zhongjianbox ul{
	font-size:12px;
}
.zhongjianbox ul span{
	display:inline-block;
	line-height:20px;
	text-overflow:ellipsis; 
	overflow:hidden; 
	white-space:nowrap; 
}
.zhongjianbox ul li>span:nth-of-type(1){
	width:40px;
	text-overflow:ellipsis; 
	overflow:hidden; 
	white-space:nowrap; 
}
.zhongjianbox ul li>span:nth-of-type(2){
	width:140px;
	text-overflow:ellipsis; 
	overflow:hidden; 
	white-space:nowrap; 
}
.zhongjianbox ul li>span:nth-of-type(3){
	width:70px;
}
.activityright{
	float:left;
	margin-top:45px;
	background:url(../img/bbbggg.png) no-repeat center bottom;
	padding-bottom:20px;
}
.turntable{
	overflow:hidden;
}
.turntable .prize{
	float:left;
	width:127px;
	height:106px;
	margin-left:1px;
	margin-top:1px;
}
.turntable .prize.Draw-a-button{
	width:382px;
	height:106px;
}
.turntable .prize.Draw-a-button input{
	background:url(../img/choujiangbutton.png) no-repeat;
	width:382px;
	height:106px;
	cursor:pointer;
}
.activitymain{
	background:#f4f0ee;
	padding:30px 0;
}
.Activities-that{
	width:1148px;
	-webkit-border-radius:10px;
	-moz-border-radius:10px;
	border-radius:10px;
	margin:auto;
	background:url(../img/line.png) #fdd100 no-repeat center center;
	padding:26px;
}
.Activities-that h4{
	font-size:18px;
	color:#d61935;
}
.Activities-that .textmianbox{
	border:1px solid #d61935;
	width:1106px;
	margin-top:10px;
	padding:5px 20px;
	font-size:14px;
	line-height:30px;
}