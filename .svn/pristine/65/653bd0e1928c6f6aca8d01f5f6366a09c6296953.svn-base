.rebate_wrap_m{
	width: 100%;
}
.rebate_banner_m,.rebate_banner_m img{
	width: 100%;
}
.rebate_content1_wrap_m{
	width:100%;
	background-color: #f2f6e7;
	padding-bottom: 1.2em;
}
.rebate_title_m{
	width: 90%;
	margin-left: 5%;
	padding: 1em 0;
}
.rebate_title_m img{
	width: 96%;
	margin-left: 2%;
}
.rebate_content1_left_img_m,.rebate_promptly_m{
	width: 70%;
	margin-top: 0.8em;
	margin-left: 15%;
}
.rebate_content1_left_m{
	width: 90%;
	margin-left: 5%;
	padding: 0.8em;
	background-color: #FFFFFF;
	color: #556115;
}
.rebate_content1_left_title_m{
	font-size: 1.1em;
}
.rebate_content1_left_m p{
	font-size: 1em;
	line-height: 1.5em;
}
.rebate_content1_right_m{
	width: 80%;
	margin-left: 10%;
	background-color: #eef2df;
	-moz-box-shadow: 0em 0.5em 1em #c2c3bd; /* 老的 Firefox */
	box-shadow: 0em 0.5em 1em #c2c3bd;
	margin-top: 0.8em;
}
.rebate_content1_right_title_m{
	background-color: #e2e7ca;
	padding: 0.5em 0;
	width: 104%;
	margin-left: -2%;
	-moz-box-shadow: 0em 0.5em 1em #c2c3bd; /* 老的 Firefox */
	box-shadow: 0em 0.5em 1em #c2c3bd;
}
.rebate_content1_right_title_m img{
	width: 44%;
	margin-left: 28%;
}
.gonggao_ul_div_m{
	margin-top: 1em;
	overflow: hidden;
	padding-bottom: 0.5em;
	color: #af6316;
}
.gonggao_ul_div_m span{
	display: inline-block;
	float: left;
	width: 33.33%;
	text-align: center;
}
.gonggao_ul_m,.gonggao_ul_m li{
	width: 100%;
	overflow: hidden;
}
.gonggao_ul_m{
	height: 6.5em;
}
.gonggao_ul_m li span{
	float: left;
	display: inline-block;
	width: 33.33%;
	white-space:nowrap;
	overflow: hidden;
	font-size: 1em;
	line-height: 1.6em;
}
.rebate_content1_text_m{
	width: 90%;
	margin-left: 5%;
	padding: 0.8em;
	background-color: #f1f5e6;
	border: 1px solid #556115;
	color: #556115;
	margin-top: 0.8em;
}
.rebate_content1_text_title_m{
	font-size: 1.1em;
}
.rebate_content1_text_m p{
	text-indent: 2em;
	font-size: 1em;
	line-height: 1.4em;
	margin-top: 0.3em;
}
.rebate_content2_m{
	background-color: #e6ead2;
}
.rebate_content2_center_m{
	width: 90%;
	margin-left: 5%;
	padding-bottom: 0.8em;
}
.rebate_content2_text1_m,.rebate_content2_text2_m{
	background-color: #FFFFFF;
    padding: 0.8em;
    color: #556115;
    margin-top: 0.5em;
}
.rebate_indent_m{
	text-indent: 2em;
}
.rebate_content2_center_m p{
	margin-top: 0.4em;
	font-size: 1em;
	line-height: 1.5em;
}
.rebate_content3_m{
	background-color: #f2f6e7;
}
.rebate_content2_text_title_m{
	font-size: 1.1em;
}
