.rebate_wrap{
	width: 100%;
	min-width: 1160px;
}
.rebate_banner,.rebate_banner img{
	width: 100%;
	min-width: 1160px;
}
.rebate_title{
	width: 100%;
	padding: 40px 0;
}
.rebate_title img{
	width: 1160px;	
	margin: 0 auto;
}
.rebate_content1_wrap{
	width: 100%;
	background-color: #f2f6e7;
	position: relative;
}
.rebate_content1{
	width: 1200px;
	margin: 0 auto;
	overflow: hidden;
	padding-top: 10px;
	position: relative;
	padding-bottom: 70px;
}
.rebate_content1_left{
	float: left;
	width: 625px;
	height: 390px;
	background-color: #ffffff;
	padding: 30px;
}
.rebate_content1_left_title{
	color: #556115;
	font-size: 25px;
	margin-bottom: 15px;
	font-weight: normal;
}
.rebate_content1_left p{
	font-size: 20px;
	line-height: 35px;
	color: #556115;
}
.rebate_content1_right{
	float: right;
	width: 420px;
	height: 450px;
	margin-right: 15px;
	background-color: #eef2df;
	-moz-box-shadow: -2px 8px 30px #a1a858; /* 老的 Firefox */
	box-shadow: -2px 8px 30px #a1a858;
}
.rebate_content1_right_title{
	height: 60px;
	width: 425px;
	margin-left: -2.5px;
	background-color: #f0f5e3;
	-moz-box-shadow: -1px 5px 10px #a1a858; /* 老的 Firefox */
	box-shadow: -1px 5px 10px #a1a858;
	padding-top: 11px;
}
.rebate_content1_right_title img{
	width: 235px;
	margin: 0 auto;
}
.gonggao_ul_div{
	width: 100%;
	height: 100%;
	padding-top: 45px;
}
.gonggao_ul_div_title{
	width: 100%;
	overflow: hidden;
	margin-bottom: 15px;
}
.gonggao_ul_div_title span{
	float: left;
	display: inline-block;
	width: 140px;
	color: #af6316;
	font-size: 20px;
	text-align: center;
}
.gonggao_ul_div  .gonggao_ul{
	height: 293px;
}
.gonggao_ul_div .gonggao_ul,.gonggao_ul_div .gonggao_ul li{
	width: 100%;
	overflow: hidden;
}
.gonggao_ul_div .gonggao_ul li span{
	width: 140px;
	display: inline-block;
	float: left;
	width: 140px;
	color: #af6316;
	font-size: 16px;
	text-align: center;
	line-height: 30px;
	white-space:nowrap;
	overflow: hidden;
}
.rebate_content1_left_img{
	width: 570px;
	margin-left: 26px;
	position: absolute;
    top: 420px;
}
.rebate_content1_text{
	width: 100%;
	color: #556115;
	border: 1px solid #556115;
	padding: 30px;
	margin-top: 500px;
}
.rebate_content1_text_title{
	color: #556115;
    font-size: 25px;
    margin-bottom: 10px;
    font-weight: normal;
}
.rebate_content1_text p{
	text-indent: 2em;
	font-size: 20px;
    line-height: 35px;
    color: #556115;
}
.rebate_content2{
	width: 100%;
	background-color: #e6ead2;
	position: relative;
}
.rebate_content2_text1,.rebate_content2_text2{
	background-color: #FFFFFF;
	margin-bottom: 10px;
	padding: 25px 30px;
}
.rebate_content2_center{
	width: 1200px;
	margin: 0 auto;
	overflow: hidden;
	padding-top: 10px;
	padding-bottom: 50px;
	color: #556115;
}
.rebate_content2_text_title{
    font-size: 25px;
    margin-bottom: 10px;
    font-weight: normal;
}
.rebate_indent{
	text-indent: 2em;
	line-height: 30px;
	margin-bottom: 5px;
	font-size: 20px;
}
.rebate_content2_text2 p{
	line-height: 30px;
	margin-bottom: 5px;
	font-size: 20px;
}
.rebate_promptly {
    width: 570px;
    margin: 0 auto;
    margin-top: 30px;
}
.rebate_content3{
	width: 100%;
	background-color: #f2f6e7;
	position: relative;
}
.rebate_content3_text{
	background-color: #f2f6e7;
	border: 1px solid #556115;
}
.rebate_bottom{
	width: 100%;
	height: 80px;
	background-color: #e2e9c0;
}
.rebate_flower2{
	width: 150px;
	position: absolute;
	right: 0;
	top: 180px;
}
.rebate_flower1{
	width: 150px;
	position: absolute;
	left: 0;
	top: 0;
}
.rebate_flower3{
	width: 150px;
	position: absolute;
	right: 0;
	bottom: 30px;
}
.rebate_flower4{
	width: 150px;
	position: absolute;
	left: 0;
	bottom: 0;
}
