.wine-wrap{
	height: 100%;
	padding-top: 1.05rem;
	overflow-y: scroll;
	position: relative;
}

.add-bank-card{
	width: 100%;
	padding: 0 0.37rem;
	position: absolute;
    bottom: 0;
}
.add-bank-card .text-center{
	width: 100%;
    height: 0.95rem;
    border: 0.02rem solid #d10000;
    color: #d10000;
    line-height: 0.95rem;
    font-size: 0.3124rem;
    border-radius: 0.08rem;
}
.add-bank-card .text-center img{
	width: 0.38rem;
	height: 0.37rem;
	position: relative;
	top: -0.03rem;
}
.card-manage-content{
	padding: 0.24rem 0.37rem;
}
.card-manage-content li{
	width: 100%;
	height: 2.3rem;
	margin-bottom: 0.13rem;
	background-image: linear-gradient(#db4f47,#d93c34);
	border-radius: 0.1rem;
	padding: 0.58rem 0 0 0.46rem;
}
.card-manage-content .card-logo{
	width: 0.84rem;
	height: 0.84rem;
	border-radius: 50%;
	background-color: #FFFFFF;
	float: left;	
	padding: 0.12rem;
	margin-right: 0.3rem;
}
.card-manage-content .card-logo img{
	width: 100%;
	height: 100%;
}
.card-manage-content .card-name{
	color: #FFFFFF;
	font-size: 0.2916rem;
	margin-bottom: 0.15rem;
}
.card-manage-content .card-num{
	font-size: 0.4166rem;
	color: #FFFFFF;
	height: 0.38rem;
	line-height: 0.38rem;
	padding-right: 0.35rem;
}
.card-manage-content .card-delete-i{
	display: inline-block;
	width: 0.38rem;
	height: 0.38rem;
	float: right;
}
.confirm-bounced-gray{
    width: 100%;
    height: 100%;
    position: fixed;
    max-width: 750px;
    min-width: 320px;
    top: 0;
    background-color: rgba(0,0,0,0.5);
    z-index: 9;
    display: none;
}
.confirm-bounced-alert{
    width: 5.8rem;
    height: 2.9rem;
    background-color: #FFFFFF;
    position: absolute;
    top: 50%;
    left: 50%;
    margin-top: -1.45rem;
    margin-left: -2.9rem;
    border-radius: 0.08rem;
}
.confirm-bounced-gray .confirm-text{
    height: 2rem;
    font-size: 0.3333rem;
    color: #666666;
    padding: 0;
 	line-height: 2rem;
    text-align: center;
    border-bottom: 0.01rem solid #eaeaea;
}
.confirm-bounced-alert .two-btn{
    width: 100%;
    height: 0.9rem;
    overflow: hidden;
}
.confirm-bounced-alert .give-up,.confirm-bounced-alert .think-again{
    width: 50%;
    height: 100%;
    float: left;
    font-size: 0.3333rem;
    color: #666666;
    line-height: 0.9rem;
    text-align: center;
}
.confirm-bounced-alert .think-again{
    color: #d10000;
    border-left: 0.01rem solid #eaeaea;
}