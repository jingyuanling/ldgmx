body{
    width: 100%;
    min-width: 320px;
    max-width: 640px;
    margin: 0 auto;
    font-family: "微软雅黑";
}
html{
    background-color:#ebebeb
}
img{
  border:0;
  display: block;
}
.clearfix:after {
visibility: hidden;
display: block;
font-size: 0;
content: " ";
clear: both;
height: 0;
}
.xiaodianhuiyuan-box{
    width: 100%;
    height: 100%;
    background-color: #eaeaea;
}
.main-top{
    width: 100%;
    height: 39px;
    background-color: #548e11;
    font-family:"微软雅黑";
}
.main-top img{
    position: absolute;
    float: left;
    width: 10px;
    margin-top: 10px;
    margin-left: 13px;

}
.main-top img.serch{
    position: relative;
    float: right;
    width: 15px;
    margin-top: -30px;
    margin-right: 13px;

}
.main-top p{
    width: 100%;
    letter-spacing: 2px;
    font-size: 14px;
    color: #fff;
    text-align: center;
    line-height:39px;
}
.dian-tit{
    width: 100%;
    padding: 8px 5px;
    height: 90px;
    border-bottom: 1px solid #dadad9;
}
.dian-tit span{
    float: left;
    display: block;
    width: 65px;
    height: 65px;
    border-radius: 50%;
    overflow: hidden;
    border: 1px solid #8b8b8b;
}
.dian-tit img{
    width: 69px;
}
.dian-tit .cont{
    padding: 5px;
    float: left;
    margin-left: 10px;
}
.dian-tit .cont p{
   color: #000;
   font-size: 12px;
   line-height: 17px;
}
.dian-tit .cont p.p2{
   color: #8b8b8b;
   font-size: 12px;
   line-height: 20px;
}
.dian-tit .cont p.juli{
    color:#8b8b8b;
    letter-spacing:0px;
    font-size: 12px;
    border-radius: 4px;
    margin-top: 3px;
}

