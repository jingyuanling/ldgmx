.wine-wrap{
	padding: 1.05rem 0 0;
	height: 100%;
	overflow-y: scroll;
}
.financial-upper{
	width: 100%;
	height: 3.34rem;
	padding-top: 0.6rem;
	background: url(../imgs/financial-upper-bg.jpg) 0 0 no-repeat;
	background-size: 100% 100%;
}
.financial-upper p{
	text-align: center;
	color: #FFFFFF;
}
.financial-small-tit{
	font-size: 0.25rem;
}
.financial-upper .num{
	font-size: 0.9375rem;
}
.financial-upper .withdrawal{
	width: 2.08rem;
	height: 0.58rem;
	line-height: 0.58rem;
	color: #d10000;
	font-size: 0.2917rem;
	border-radius: 0.06rem;
	display: block;
	margin: 0 auto;
	background-color: #FFFFFF;
}


.person-two-amount{
	margin-bottom: 0.17rem;
	height: 1.67rem;
	padding: 0.25rem 0.37rem;
	background-color: #FFFFFF;
	border-bottom: 0.01rem solid #dfdfdf;
}
.person-two-amount .left{
	border-right: 0.01rem solid #dfdfdf;
}
.person-two-amount .left,.person-two-amount .right{
	display: inline-block;
	width: 50%;
}
.person-two-amount .num,.person-two-amount .text{
	width: 100%;
	text-align: center;
}
.person-two-amount .num{
    font-size: 0.5417rem;
    color: #666666;
}
.person-two-amount .text{
	font-size: 0.25rem;
	color: #999999;
}
.person-my {
    background-color: #FFFFFF;
    padding: 0 0.37rem;
    margin-bottom: 0.16rem;
}
.person-my .agent,.person-my .team{
	height: 1.05rem;
	line-height: 1.05rem;
}
.person-my .agent{
	border-bottom: 0.01rem solid #e7e7e7;
}
.person-my .left{
	font-size: 0.2917rem;
	color: #333333;
}
.person-my .right{
	display: inline-block;
	float: right;
	font-size: 0.25rem;
	color: #999999;
}
.person-my .right img{
	width: 0.14rem;
    height: 0.29rem;
    margin-left: 0.28rem;
    position: relative;
    top: -0.03rem;
}
.confirm-bounced-gray{
	width: 100%;
	height: 100%;
	position: fixed;
	max-width: 750px;
	min-width: 320px;
	top: 0;
	background-color: rgba(0,0,0,0.5);
	z-index: 9;
	display: block;
}
.confirm-bounced-alert{
	width: 5.8rem;
	height: 3.3rem;
	background-color: #FFFFFF;
	position: absolute;
	top: 50%;
	left: 50%;
	margin-top: -1.65rem;
	margin-left: -2.9rem;
	border-radius: 0.08rem;
}
.confirm-bounced-gray .confirm-text{
	height: 2.4rem;
	font-size: 0.3333rem;
	color: #666666;
	padding: 0.5rem 0.6rem 0;
	text-align: center;
	border-bottom: 0.01rem solid #eaeaea;
}
.confirm-bounced-alert .two-btn{
	width: 100%;
	height: 0.9rem;
	overflow: hidden;
}
.confirm-bounced-alert .give-up,.confirm-bounced-alert .think-again{
	width: 50%;
	height: 100%;
	float: left;
	font-size: 0.3333rem;
	color: #666666;
	line-height: 0.9rem;
	text-align: center;
}
.confirm-bounced-alert .think-again{
	color: #d10000;
	border-left: 0.01rem solid #eaeaea;
}