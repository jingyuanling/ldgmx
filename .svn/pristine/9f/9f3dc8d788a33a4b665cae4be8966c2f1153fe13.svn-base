.system-home-wrap{
	width: 100%;
	height: 100%;
	padding-top: 0.88rem;
	background-color: #f1f3f5;
	overflow-y: scroll;
}
.home-goods-num{
	width: 100%;
	height: 3rem;
	border: 0.03rem solid #e5e5e5;
	border-bottom: 0.01rem solid #e5e5e5;
	background-color: white;
	padding: 0 0.27rem;
}
.home-goods-num ul{
	font-size: 0;
	overflow: hidden;
}
.home-goods-num li{
	width: 50%;
	height: 1.48rem;
	float: left;
	align-items:center; 
	display: -webkit-flex;
}
.home-goods-num li p{
	width: 100%;
	font-size: 0.44rem;
	display: inline-block;
	text-align: center;
}
.home-goods-num li span{
	font-size: 0.45rem;
	font-weight: lighter;
	display: inline-block;
	width: 100%;
	text-align: center;
	color: #666666;
}
.home-cont{
	margin-top: 0.22rem;
	border: 0.01rem solid #e5e5e5;
	padding-bottom: 2.58rem;
	overflow: hidden;
	background-color: white;
}
.home-mant-module li{
	width: 3.35rem;
	height: 2.93rem;
	float: left;
	border-radius: 0.15rem;
	margin: 0.27rem 0 0 0.27rem;
}
.home-mant-module li:nth-child(even){
	float: right;
	margin-left: 0;
	margin: 0.27rem 0.27rem 0 0;
}
.home-mant-module{
	font-size: 0;
}
.home-mant-module p{
	display: inline-block;
	width: 100%;
	height: 1.7rem;
}
.home-mant-module p img{
	margin: 0 auto;
	margin-top: 0.48rem;
}
.home-mant-module span{
	display: inline-block;
	width: 100%;
	font-size: 0.54rem;
	line-height: 1.2rem;
	text-align: center;
	color: white;
}
