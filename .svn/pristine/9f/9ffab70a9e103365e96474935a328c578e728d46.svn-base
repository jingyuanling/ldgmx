html{
     background-color: #eaeaea;
}
body{
    width: 100%;
    min-width: 320px;
    max-width: 640px;
    margin: 0 auto;
    font-family: "微软雅黑";
}
.find-box{
    width: 100%;
    height: 100%;
     background-color: #eaeaea;
}
img{
  border:0;
  display: block;
}
.clearfix:after {
visibility: hidden;
display: block;
font-size: 0;
content: " ";
clear: both;
height: 0;
}
.header{
    width: 100%;
    max-width: 640px;
    min-width: 320px;
    height: 37px;
    background: #548e12;
    z-index: 100;
}
.logo img{
    width:39px;
    margin-left: 15px;
    margin-top: 7px;
    float: left;
}
.ipt{
    margin-left: 60px;
    margin-right: 51px;
    height: 37px;
}
.ipt input{
    width: 100%;
    border:none;
    height: 24px;
    float: left;
    margin-top: 6px;
    border-radius: 5px;
}
.ipt img{
    width: 12px;
    float: right;
    margin-top: -19px;
    margin-right: 7px;
}
.shopcar img{
    float: right;
    width: 23px;
    margin-right: 15px;
    margin-top: -28px;
}
.mingxi-list{
    width: 100%;
    margin-top: 10px;
   
}
.list11{
    margin-top: 15px;
}
.mingxi-list .list1{
    width: 100%;
    height: 35px;
    background-color:#fff;
    border-top: 1px solid #bfbfbf;
}
.mingxi-list .list1 img{
    float: left;
    width: 21px;
    margin-left: 13px;
    margin-top: 5px;
}
.mingxi-list .list1 p{
    font-size: 12px;
    float: left;
    line-height: 33px;
    margin-left: 15px;
}
.mingxi-list .list1 p.right {
    float: right;
    margin-right: 25px;
}
.mingxi-list .list2{
    border-bottom: 1px solid #bfbfbf;
}
.guanggao{
    width: 100%;
    height: 100px;
    background-color: #888888;
    margin-top: 50px;
}
.bottom{
    position: absolute;
    max-width: 640px;
    min-width: 320px;
    width: 100%;
    height:47px;
    background-color: #fff;
    bottom: 0px;
}
.bottom li{
    width: 33.3%;
    float: left;
    list-style: none;
}
.bottom li img{
    margin:0 auto;
    width: 27px;
    margin-top: 6px;
}