body{
    width: 100%;
    min-width: 320px;
    max-width: 640px;
    margin: 0 auto;
    font-family: "微软雅黑";
}
html{
     background-color: #f0f0f2;
}
img{
  border:0;
  display: block;
}
.clearfix:after {
visibility: hidden;
display: block;
font-size: 0;
content: " ";
clear: both;
height: 0;
}
.zhifuok-box{
    width: 100%;
    background-color:#f0f0f2
}
.main-top{
    width: 100%;
    height: 39px;
    background-color: #548e11;
}
.main-top img{
    position: absolute;
    float: left;
    width: 12px;
    margin-top: 10px;
    margin-left: 15px;

}

.main-top p.ok{
    width: 36px;
    float: right;
    margin-top: -40px;
    margin-right: 13px;
    color: #fff;

}

.main-top p{
    width: 100%;
    letter-spacing: 2px;
    font-size: 14px;
    color: #fff;
    text-align: center;
    line-height:39px;
}
.zhanghao{
    width: 100%;
    /*height: 40px;*/
    background-color: #fff;
    
    /*margin-top: 15px;*/
}
.zhanghao p.tit{
     height: 35px;
     line-height: 35px;
     padding-left: 13px;
     font-size: 12px;
     color: #000;
     background-color: #f0f0f2;
}
.zhanghao p{
    height: 40px;
    line-height: 40px;
     border-top: 1px solid #e8e8e8;
}
.zhanghao p.last{
    border-bottom: 1px solid #e8e8e8;
}
.zhanghao span{
    font-size: 12px;
    color: #000;
    line-height: 38px;
    display: block;
    float: left;
    margin-left: 13px;
}
.zhanghao input{
    position: absolute;
    height: 37px;
    border: none;
    font-size: 12px;
    margin-left: 10px;
    outline: none;
    width: 180px;
}
.zhaopian > p{
    width: 100%;
    height: 40px;
    line-height: 40px;
    font-size: 12px;
    color:#000;
   padding-left: 13px;
}
.zhaopian .tupian{
    padding: 10px 13px;
    background-color: #fff;
}
.zhaopian .tupian p{
    width: 64px;
    height: 64px;
    float: left;
    margin-right: 5px;
   /* background-color: red;*/

}
