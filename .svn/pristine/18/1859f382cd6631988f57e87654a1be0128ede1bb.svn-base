.wine-wrap-four{
	height: 100%;
	padding-bottom: 2.2rem;
}
.cart-list{
	height: 100%;
	background-color: #FFFFFF;
	overflow-y: scroll;
}
.cart-list-li{
	height: 2.42rem;
	padding: 0.27rem 0.37rem 0;
	overflow: hidden;
}
.left-goodImg-div{
	height: 1.88rem;
	width: 2.47rem;
	float: left;
	margin-right: 0.2rem;	
}
.cart-list-li .left-goodImg-div .sel-one{
	margin-right: 0.25rem;
	margin-top: 0.77rem;
}
.cart-goodsImg,.cart-goodsImg img{
	width: 1.88rem;
	height: 1.88rem;
	border-radius: 0.06rem;
}
.cart-list-li .good-desc{
	font-size: 0.2708rem;
	line-height: 0.36rem;
	height: 0.72rem;
	display: -webkit-box;
	-webkit-box-orient: vertical;
	-webkit-line-clamp: 2;
	overflow: hidden;
	color: #333333;
}
.cart-list-li .good-size{
	margin-top: 0.15rem;
}
.cart-list-li .good-size span{
	height: 0.4rem;
	line-height: 0.4rem;
	padding: 0 0.13rem;
	font-size: 0.2499rem;
	color: #999999;
	background-color: #eeeeee;
	border-radius: 0.03rem;
}
.choose-num{
	display: block;
	margin-top: 0.13rem;
	overflow: hidden;
}
.choose-num .num-tit{
	font-size: 0.3333rem;
	color: #d10000;
}
.choose-num-btn{
	float: right;
	background-color: #FFFFFF;
	font-size: 0.2708rem;
	line-height: 0.47rem;
	border: 0.02rem solid #eaeaea;
	overflow: hidden;
	border-radius: 0.03rem;
}
.choose-num-btn p,.choose-num-btn span{
	font-size: 0.2708rem;
}
.choose-num-btn p{
	height: 0.47rem;
	width: 0.49rem;
	text-align: center;
	display: inline-block;
	color: #666666;
	float: left;
}
.choose-num-btn .less_than_one{
	color: #d2d2d2;
}
.choose-num-btn span{
	width: 0.88rem;
	height: 0.47rem;
	text-align: center;
	float: left;
	border-left: 0.02rem solid #eaeaea;
	border-right: 0.01rem solid #eaeaea;
}

/*结算栏*/
.btm-settlement{
	width: 100%;
	height: 1.03rem;
	border-top: 0.01rem solid #dddddd;
	padding-left: 0.37rem;
	position: absolute;
	bottom: 1.05rem;
	left: 0;
	background-color: #FFFFFF;
}
.btm-settlement .sel-all-div{
	width: 1.2rem;
	font-size: 0.2708rem;
	line-height: 1.03rem;
	position: absolute;
	left: 0.37rem;
}
.btm-settlement .sel-all,.cart-list .sel-one{
	width: 0.34rem;
	height: 0.34rem;
	float: left;
	margin-right: 0.13rem;
	border: 0.04rem solid #7d7d7d;
	border-radius: 50%;
	margin-top: 0.345rem;
	position: relative;
}
.btm-settlement .sel-all img,.cart-list .sel-one img{
	width: 0.24rem;
	height: 0.16rem;
	position: absolute;
	top: 0.09rem;
	left: 0;
}
/*select选中状态*/
.btm-settlement .sel_active{
	border: none;
	background-color: #d10000;
}
.btm-settlement .sel_active img{
	left: 0.05rem;
}
.btm-settlement .account-btn{
	width: 2.3rem;
	height: 100%;
	font-size: 0.3333rem;
	background-color: #d10000;
	line-height: 1.03rem;
	color: #FFFFFF;
	text-align: center;
	position: absolute;
	top: 0;
	right: 0;
}
.btm-settlement .price{
	padding-left: 1.3rem;
	line-height: 1.03rem;
	padding-right: 2.53rem;
	text-align: right;
}
.btm-settlement .price .span1{
	font-size: 0.2083rem;
	color: #999999;
	margin-right: 0.12rem;
}
.btm-settlement .price .span2{
	font-size: 0.2499rem;
	color: #333333;
}
.btm-settlement .price .span3{
	font-size: 0.3333rem;
	color: #d10000;
}
