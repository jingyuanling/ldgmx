/*head*/
.life_home_nav{
	width: 100%;
	background-color: #d9333f;
	height: 50px;
	position: fixed;
	max-width: 640px;
	min-width: 320px;
	z-index: 100;
}
.life_home_nav>div{
	position: absolute;
}
.life_home_nav_inp{
	width: 66%;
	left: 17%;
	height: 30px;
	top: 10px;
}
.life_home_nav_inp input{
	width: 100%;
	height: 100%;
	border-radius: 5px;
	border: 0;
	padding-left: 22px;
	box-sizing: border-box;
	background-color: #c3161e;
	color: #FFFFFF;
	font-size: 12px;
}
.life_home_nav_inp img{
	position: absolute;
	width: 15px;
	height: 15px;
	top: 7.5px;
	left: 3.5px;
}
.life_nav_left{
	height: 100%;
	left: 0;
	top: 0;
	line-height: 50px;
	padding: 0 3%;
	font-size: 16px;
	color: white;
}
.life_home_nav_right{
	right: 0;
	height: 36px;
	top: 7px;
	padding: 0 3%;
}
.life_home_nav_right a{
	font-size: 22px;
    color: white;
}
.life_home_nav_right img{
	height: 100%;
}
.life_home_nav_inp2{
	position: relative;
}
.life_home_nav_inp2 input{
	padding-right: 30px;
	background-color: #e6e6e6;
	color: #666666;
	border: none;
}
.life_home_nav_inp_right{
	position: absolute;
	width: 30px;
	height: 24px;
	right: 0;
	top: 4px;
	border-left: 1px solid #d5d3d3;
	color: #D5D3D3;
	font-size: 18px;
	text-align: center;
	line-height: 24px;
}
.life_home_nav2 .life_nav_left{
	color: #666666;
	background-color: #FFFFFF;
}
.life_nav_left{
	position: relative;
}
.life_nav_left .life_home_triangle_small{
	width: 10px;
	height: 10px;
	position: absolute;
	right: 0;
	bottom: 10px;
}
/*foot*/
.life_bottom{
	width: 100%;
	height: 50px;
	overflow: hidden;
	margin-top: 0.8em;
	position: fixed;
	bottom: 0;
	max-width: 640px;
	min-width: 320px;
	z-index: 100;
	background-color: #ffffff;
	border-top: 1px solid #e9e9e9;
}
.life_bottom_columns{
	float: left;
	width: 33.33%;
	text-align: center;
	position: relative;
}
.life_bottom_columns img{
	margin: 0 auto;
	height: 25px;
	margin-top: 3px;
}
.life_bottom_columns p{
	display: inline-block;
	width: 100%;
	height: 22px;
	line-height: 22px;
	color: #9d9d9d;
	font-size: 12px;
}
.life_border_right{
	position: absolute;
	height: 30px;
	width: 1px;
	background-color: #9d9d9d;
	top: 10px;
	right: 0;
}
.life_nav_left img{
	height: 26px;
	margin-top: 12px;
}
.life_home_nav_center{
    width: 150px;
	height: 100%;
	font-size: 25px;
	text-align: center;
	left: 50%;
	margin-left: -75px;
	line-height: 50px;
	color: white;
	letter-spacing: 4px;
}
.life_bottom_columns .life_bottom_deep{
	color: #222222;
}
