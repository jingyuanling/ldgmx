.collect-words-wrap{
	width: 100%;
	min-height: 100%;
	background-color: #fa353f;
	font-size: 0;
	font-family: "黑体";
	position: relative;
	min-width: 1150px;
	overflow-x: hidden;
}
.words-banner{
	width: 100%;
}
.words-banner img{
	width: 100%;
}
.words-content{
	width: 1100px;
	margin: 20px auto 0;
	position: relative;
	z-index: 9;
	min-height: 950px;
}
.words-import{
	width: 100%;
	height: 528px;
	background: url(../img/center-bg.jpg) 0 0 no-repeat;
	background-size: 100% 100%;
	padding: 18px 5px;
}
.words-five-card{
	width: 100%;
}
.words-five-card ul{
	overflow: hidden;
	height: 399px;
}
.words-five-card li{
	width: 210px;
	float: left;
	margin-right: 10px;
}
.words-five-card .words-card-center{
 	width: 200px;
	height: 385px;
	background-color: #acacac;
	position: relative;
	overflow: hidden;
	margin: 5px auto;
 }
.words-five-card li:last-child{
	margin-right: 0;
}
.words-five-card li.have .words-card-center{
	background-color: #cd1727;
}
.words-card-upper{
	width: 100%;
	height: 142px;
	background-color: white;
	position: relative;
	padding-top: 15px;
	overflow: hidden;
}
.words-card-descri{
	font-size: 12px;
	color: #fa353f;
	position: relative;
	z-index: 9;
}
.right-top-num{
	display: block;
	position: absolute;
	top: -32px;
	right: -32px;
	width: 64px;
	height: 64px;
	border-radius: 50%;
	background-color: #cd1727;
	font-size: 20px;
	color: white;
	padding: 32px 32px 0 1px;
	text-align: center;
	line-height: 25px;
}
.words-guang-img{
	width: 100%;
	height: 135px;
	position: absolute;
	bottom: 0;
}
.words-card-down{
	width: 100%;
	padding: 28px 0 0 18px;
	position: relative;
}
.words-five-card li  .right-top-descri{
	width: 47px;
	height: 79px;
	padding-top: 30px;
	position: absolute;
	top: 9px;
	right: 8px;
}
.words-five-card .none  .right-top-descri{
	background: url(../img/lion-gray.png) 0 0 no-repeat;
	background-size: 100% 100%;
}
.words-five-card .have  .right-top-descri{
	background: url(../img/lion.png) 0 0 no-repeat;
	background-size: 100% 100%;
}
.right-top-descri span{
	width: 25px;
	display: inline-block;
	margin-left: 11px;
	font-size: 15px;
	text-align: center;
	line-height: 20px;
	color: white;
}
.words-five-card li .words-card-text{
	width: 143px;
	height: 188px;
	font-size: 125px;
	line-height: 210px;
	text-align: center;
	font-family:'jdfmaokai117370c6dd1b4cb';
}
.words-five-card .none .words-card-text{
	background: url(../img/words-card-bg-gray.png) 0 0 no-repeat;
	background-size: 100% 100%;
	color: #3b3b3b;
}
.words-five-card .have .words-card-text{
	background: url(../img/words-card-bg-red.png) 0 0 no-repeat;
	background-size: 100% 100%;
	color: #222222;
}
.card-btm-give,.card-btm-ask{
	height: 35px;
	width: 100%;
	font-size: 21px;
	line-height: 35px;
	position: absolute;
	bottom: -35px;
	left: 0;
	background-color: #fa353f;
	color: white;
	text-align: center;
}
.words-content .at-once-merge{
	width: 1080px;
	background-color: #acacac;
	text-align: center;
	height: 70px;
	line-height: 70px;
	font-size: 42px;
	color: white;
	margin: 20px auto 23px;
}
.words-content .at-once-merge-active{
	background-color: #fa353f;
}
.words-ruls-tit{
	overflow: hidden;
	height: 50px;
}
.words-ruls-tit span{
	font-size: 48px;
	color: white;
}
.words-ruls-tit a{
	color: white;
	font-size: 25px;
	height: 30px;
	margin-top: 12px;
	border-bottom: 2px solid white;
	float: right;
}
.words-ruls-matter{
	font-size: 23px;
	padding-top: 35px;
	color: white;
	padding-bottom: 160px;
}
.words-ruls-matter li{
	line-height: 40px;
}
.words-ruls-matter span{
	float: left;
}
.words-ruls-matter p{
	padding-left: 25px;
}
.words-btm-point{
	width: 100%;
	position: absolute;
	bottom: 0;
	z-index: 10;
}
.words-btm-point img{
	width: 100%;
}
.words-btm-left,.words-btm-right{
	position: absolute;
	bottom: 0;
	width: 407px;
}
.words-btm-left{
	left: 0;
}
.words-btm-right{
	right: 0;
}
.words-record{
	height: 100%;
	min-height: 950px;
	padding: 16px;
}
.words-record-tit{
	overflow: hidden;
}
.words-record-tit span{
	display: inline-block;
	width: 527px;
	height: 70px;
	font-size: 42px;
	color: white;
	line-height: 70px;
	text-align: center;
}
.words-record-tit .give{
	float: left;
}
.words-record-tit .ask{
	float: right;
}
.words-record-tit .record-tit-gray{
	background-color: #bababa;
}
.words-record-tit .record-tit-red{
	background-color: #fa353f;
}
.words-give-record{
	padding-top: 30px;
}
.words-give-record p{
	font-size: 35px;
	color: #fa353f;
	line-height: 50px;
}
.record-give-btn{
	width: 97px;
	height: 34px;
	background-color: #fa353f;
	display: inline-block;
	text-align: center;
	line-height: 34px;
	font-size: 20px;
	color: white;
	position: relative;
	top: -5px;
	left: 5px;
}
/*弹框*/
.words-ask-gray,.words-give-gray,.words-active-gray,.ask-for-me-gray{
	width: 100%;
	height: 100%;
	position: fixed;
	top: 0;
	left: 0;
	z-index: 10;
	background-color: rgba(0,0,0,0.5);
	font-family: "黑体";
	display: none;
}
.ask-for-me-gray{
	display: block;
}
.words-center-prize{
	width: 0px;
	height: 0px;
	position: absolute;
	top: 50%;
	left: 50%;
	background-color: rgba(255,255,255,0.4);
	border-radius: 50%;
	box-shadow: 0 0 200px white;
}
.words-center-prize img{
	width: 0;
	height:0;
	margin-left: 115px;
}
.words-alert-content{
	width: 500px;
	height: 340px;
	background-color: white;
	position: absolute;
	left: 50%;
	top: 50%;
	margin-top: -170px;
	margin-left: -250px;
	position: relative;
	overflow: hidden;
}
.words-alert-content-small{
	height: 260px;
}
.words-alert-content .alert-content-tit{
	font-size: 25px;
	color: #bbbbbb;
	background-color: #f2f2f2;
	height: 44px;
	line-height: 44px;
	text-align: center;
}
.words-alert-content .alert-tit-cancle{
	width: 70px;
	height: 70px;
	border-radius: 50%;
	background-color: #dedede;
	position: absolute;
	top: -33px;
	right: -35px;
	padding-top: 40px;
	padding-left: 12px;
}
.words-alert-content .alert-tit-cancle img{
	width: 18px;
	height: 18px;
}
.alert-content-matter{
	width: 365px;
	margin: 34px 0 0 70px;
}
.words-alert-content .alert-content-inp{
	font-size: 30px;
	padding: 0 20px;
	height: 55px;
	width: 100%;
	line-height: 55px;
	border: none;
	outline: none;
	color: white;
	background-color: #dedede;
}
.words-alert-content .alert-warm-prompt{
	text-align: center;
	font-size: 18px;
	margin-top: 26px;
	color: #999999;
	line-height: 25px;
}
.words-alert-content .alert-sure-btn{
	text-align: center;
	color: white;
	height: 50px;
	font-size: 33px;
	line-height: 50px;
	background-color: #ff0707;
	margin-top: 50px;
}
.words-active-gray .five-active-words{
	position: absolute;
	width: 198px;
	height: 387px;
}
.words-active-gray .words-active-guang{
	top: 50%;
	left: 50%;
	width: 0;
	height: 0;
}
.words-active-gray .words-active-ming{
	top: -387px;
	left: 50%;
	margin-left: -99px;
}
.words-active-gray .words-active-xing{
	top: 50%;
	left: -387px;
	margin-top: -193px;
}
.words-active-gray .words-active-shang{
	bottom: -387px;
	left: 50%;
	margin-left: -99px;
}
.words-active-gray .words-active-cheng{
	top: 50%;
	right: -387px;
	margin-top: -193px;
}
.collect-words-wrap .words-ruls{
	padding-top: 30px;
}
@media only screen and (max-width: 1914px) {
	.words-btm-left{
		left: -100px;
	}
	.words-btm-right{
		right: -100px;
	}
}
@media only screen and (max-width: 1514px) {
	.words-btm-left{
		left: -300px;
	}
	.words-btm-right{
		right: -200px;
	}
}
@media only screen and (max-width: 1100px) {
	.words-btm-left{
		left: -407px;
	}
	.words-btm-right{
		right: -407px;
	}
	.collect-words-wrap{
		overflow-y: scroll;
	}
}
/*移入事件*/
.card-btm-give,.card-btm-ask{
	cursor: pointer;
}



