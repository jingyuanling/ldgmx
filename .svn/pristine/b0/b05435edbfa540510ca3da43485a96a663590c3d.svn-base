body{
    width: 100%;
    min-width: 320px;
    max-width: 640px;
    margin: 0 auto;
    font-family: "微软雅黑";
}
html{
    background-color: #eaeaea;
}
img{
  border:0;
  display: block;
}
.clearfix:after {
visibility: hidden;
display: block;
font-size: 0;
content: " ";
clear: both;
height: 0;
}
.gerendetails-box{
    width: 100%;
    height: 100%;
    background-color: #eaeaea;
}
.main-top{
    width: 100%;
    height: 39px;
    background-color: #548e11;
}
.main-top img{
    position: absolute;
    float: left;
    width: 10px;
    margin-top: 10px;
    margin-left: 13px;

}
.main-top p{
    letter-spacing: 2px;
    font-size: 14px;
    color: #fff;
    text-align: center;
    line-height:39px;
}
.main-top p.xinzeng{
    float: right;
    position: relative;
    margin-top: -39px;
    margin-right: 13px;
}
.xinxi{
    width: 100%;
    margin-top: 15px;
    border-bottom:1px solid #bbbbbb;

}
.xinxi p{
   /* max-width: 640px;*/
    padding: 0 13px;
    line-height: 40px;
    color: #000;
    font-size: 12px;
    border-top:1px solid #bbbbbb;
    background-color: #fff;
   /* padding-left: 50px;*/
}

.xinxi p input{
    outline: none;
    border:none;
    height: 39px;
    width: 80%;
    position: relative;
    line-height: 40px;
   /* padding-left: 50px;
    margin-top: -40px;*/

}
.xinxi p input.check{
    width: 20px;
    float: right;
}
