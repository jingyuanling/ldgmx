body{
	background-color: #f3f5f7;
}
.wine_index_box .header{
    width: 100%;
    max-width: 640px;
    min-width: 320px;
    height: 37px;
    background: #d9333f;
    position: fixed;
    z-index: 100;
    margin-top: -37px;
}
.wine_ipt{
	width: 20em;
    margin-left: 20px;
    height: 37px;
    float: left;
}
.wine_ipt .wine_ipt_inp{
    width: 100%;
    border:none;
    height: 24px;
    float: left;
    margin-top: 6px;
    border-radius: 5px;
    padding-left: 18px;
    font-size: 0.7em;
}
.wine_ipt img{
    width: 12px;
    float: left;
    margin-left: 3px;
    margin-top: -19px;
    margin-right: 7px;
}
.wine_shopcar{
	float: right;
	height: 27px;
	margin-top: 6px;
}
.wine_shopcar img{
    height: 100%;
    margin-right: 15px;
}
.wine_banner{
    width: 100%;
    margin-top: 37px;
}
.wine_banner .swiper-container img{
    width: 100%;
}
.wine_hot{
    width: 98%;
    margin:0 auto;
    margin-top: 1%;
}
.wine_hot ul{
    width: 100%;
    height: 8.7em;
    margin-bottom: 1%;
}
.wine_hot ul li{
    list-style: none;
    float: left;
    margin-right: 1%;
    width: 49.5%;
    font-weight: 900;
    background-color: #fff;
    border: 1px solid #d7d7d7;
}
.wine_hot ul li.right{
    margin-right: 0px;
}
.wine_hot ul li img{
    width: 4.5em;
    float: right;
    margin-right: 0.8em;
}
.wine_hot ul li .cont{
    width: 5.6em;
    padding-left: 0.2em;
    float: left;
    text-align: center;
}
.wine_hot ul li span{
    display: block;
    margin-top: 2em;
    color: #e40000;
   
}
.wine_hot_text3{
	background-color: #e13d48;
	color: white;
	margin-top: 0.4em;
	font-size: 0.6em;
	line-height: 1.8em;
}
.wine_hot ul a li p{
    color: #000;
    font-size: 0.8em;
    margin-top: 0.4em;
}
.wine_guanggaowei img{
    width: 98%;
    margin-left: 1%;
}
.wine_index-more{
    margin-top: 2%;
    padding-bottom: 50px;
    overflow: hidden;
}
.wine_tit{
	width: 100%;
	font-size: 2em;
	height: 2.75em;
	line-height: 2.75em;
	text-align: center;
	border: 1px solid #d7d7d7;
	margin-top: 0.7em;
	margin-bottom: 0.7em;
	color: #d9333f;
}
.wine_index-more .wine_shangpin{
    width: 93%;
    margin-left: 3.5%;
    margin-top: 1%;
}
.wine_shangpin ul{
   margin-bottom: 3%;
}
.wine_shangpin ul li{
    background-color: #fff;
    width: 47.8%;
    height: 16.5em;
    list-style: none;
    float: left;
    margin-right: 3%;
    border-radius: 3px;
    border:1px solid #d1d1d1;
}
.wine_shangpin ul li.wine_right{
    margin-right: 0px;
}
.wine_tupian{
    width: 100%;
    height: 11.8em;
}
.wine_tupian img{
    width: 100%;
    height: 11.8em;
}
.wine_shangpin .wine_name{
    width: 100%;
    height: 4.5em;
    padding-top: 0.2em;
    padding-left: 10%;
    padding-right: 10%;
    box-sizing: border-box;
}
.wine_shangpin .wine_name p.wine_money{
    font-size: 0.9em;
    color: #c30000;
    overflow: hidden;
}
.wine_name_P_span{
	font-size: 0.9em;
	color: #acacac;
	float: right;
}
/*首页分类按钮*/
.wine_goods_class_ul{
	display: inline-block;
	width: 100%;
	display: flex;
	justify-content: space-around;
	padding: 0.8em 0.5em;
	box-sizing: border-box;
	background-color: white;
}
.wine_goods_class_ul li{
	width: 4em;
	height: 4em;
	border-radius: 50%;
}
.wine_goods_class_ul li img{
	width: 100%;
	height: 100%;
}
.wine_goods_class_ul a span{
	font-size: 0.8em;
	display: inline-block;
	width: 100%;
	text-align: center;
}
