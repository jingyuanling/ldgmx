body{
    width: 100%;
    min-width: 320px;
    max-width: 640px;
    margin: 0 auto;
    font-family: "微软雅黑";
}
html{
    background-color:#ebebeb
}
img{
  border:0;
  display: block;
}
.clearfix:after {
visibility: hidden;
display: block;
font-size: 0;
content: " ";
clear: both;
height: 0;
}
.yuemingxi-box{
    width: 100%;
    height: 100%;
    background-color: #eaeaea;
}
.main-top{
    width: 100%;
    height: 39px;
    background-color: #548e11;
    font-family:"微软雅黑";
}
.main-top img{
    position: absolute;
    float: left;
    width: 10px;
    margin-top: 10px;
    margin-left: 13px;
}
.main-top p{
    width: 100%;
    letter-spacing: 2px;
    font-size: 14px;
    color: #fff;
    text-align: center;
    line-height:39px;
}
.list-cont .list{
    padding: 5px 10px;
    width: 100%;
    border-bottom:1px solid #c7c8c6;
}
.list-cont .list p{
    color: #000;
    font-size: 12px;
    line-height: 23px;
}
.list-cont .list p span{
    color: red;
    float: right;
    margin-top:10px;
}
.list-cont .list p.time{
    color: #8b8b8b;
}
