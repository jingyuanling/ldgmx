.wine-wrap{
	padding: 1.05rem 0 0;
}
.person-gmx-logo{
	width: 6.94rem;
	height: 1.22rem;
	top: 0.24rem;
}
.person-upper{
	width: 100%;
	height: 5.41rem;
}
.agent-account-infor{
	width: 100%;
	height: 100%;
	top: 0;
	left: 0;
	padding-top: 0.82rem;
	color: #FFFFFF;
	text-align: center;
}
.agent-user-photo{
	width: 1.61rem;
	height: 1.61rem;
	margin: 0 auto;
	display: block;
	background-color: #FFFFFF;
	border-radius: 50%;
}
.agent-user-photo img{
	width: 100%;
	height: 100%;
}
.agent-account-infor .userName{
	font-size: 0.3151rem;
	margin-top: 0.1rem;
	line-height: 0.75rem;
}
.agent-account-infor .level{
	display: inline-block;
}
.agent-commission span{
	font-size: 0.25rem;
	color: #999999;
}
.agent-commission{
	text-align: center;
	margin-top: 0.45rem;
}
.agent-commission .span1{
	margin-right: 0.3rem;
}
.agent-timenav-tab{
	width: 100%;
}
.agent-timenav-tab table{
	width: 100%;
}
.agent-timenav-tab td{
	font-size: 0.2912rem;
	color: #333333;
	height: 1.05rem;
	line-height: 1.05rem;
	border-bottom: 0.01rem solid #e4e4e4;
	position: relative;
	text-align: center;
}


.agent_ta_active span{
	width: 0.68rem;
	height: 0.05rem;
	background-color: #D10000;
	position: absolute;
	bottom: 0;
	left: 50%;
	margin-left: -0.34rem;
	border-radius: 0.03rem;
}
.agent-timenav-tab .agent_ta_active{
	color: #D10000;
}
