.dingdan-box{
    width: 100%;
    background-color: #eaeaea;
    padding-top: 39px;
}
.dingdan{
    width: 100%;
    margin-top: 10px;
    background-color: #fff;
    padding: 5px 15px;
}
.dingdan p{
    font-size: 14px;
    color: #000;
    line-height: 25px;
    overflow: hidden;
}
.dingdan p span.right{
    float: right;
    font-size: 14px;
}
.dingdan p.time{
    font-size: 12px;
    color: #a29fa2;
}
.shangpin{
    padding: 10px;
    border-top:1px solid #cccccd;
}
.shangpin p{
    width: 100%;
    line-height: 35px;
}
.shangpin img{
    float: left;
    width: 25px;
    margin-right: 10px;
    margin-top: 3px;
}
.shangpin p span.shuliang{
    color:#a29fa2;
    float: right;
}
.shangpin p.zongmoney{
    border-top:1px solid #cccccd;
    width: 100%;
    margin-top: 10px;
}
.shangpin p.zongmoney span{
    float: right;
}