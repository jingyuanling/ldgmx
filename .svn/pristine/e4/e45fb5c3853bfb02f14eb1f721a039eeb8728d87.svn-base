.life_pay_wrap{
	width: 100%;
	height: 100%;
	background-color: #efefef;
	padding-top: 50px;
}
.life_pay_wrap2{
	position: relative;
    top: -5em;
}
.life_consumer_claims{
	padding: 0 3%;
	box-sizing: border-box;
	line-height: 3em;
	border-top: 1px solid #d3d3d3;
	margin-top: 0.5em;
	font-size: 1.1em;
}
.life_consumer_claims_num{
	width: 94%;
	margin-left: 3%;
	margin-top: 0.5em;
	position: relative;
}
.life_consumer_inp{
	width: 100%;
	outline: none;
	padding: 0 3% 0 15%;
	box-sizing: border-box;
	line-height: 2.0em;
	font-size: 1.8em;
	border: 1px solid #d3d3d3;
	border-radius: 5px;
}
.life_consumer_claims_num span{
	display: inline-block;
	width: 15%;
	position: absolute;
	left: 0;
	top: 0;
	text-align: center;
	line-height: 2.0;
	font-size: 1.8em;
}
.life_sure_btn{
	margin-top: 2em;
}
.life_sure_btn2{
	width: 70%;
	color: #ff5858;
	line-height: 2.5em;
	text-align: center;
	font-size: 1.2em;
	margin-left: 15%;
	margin-top: 4em;
	border-radius: 0.3em;
	border: 1px solid #ff5858;
}
.life_billing_message{
	margin-top: 2em;
	border-top: 1px solid #d3d3d3;
}
.life_look_details{
	font-size: 1em;
	text-align: center;
	color: #9d9d9d;
	margin-top: 0.6em;
}
.life_keyboard{
	width: 100%;
	position: absolute;
	bottom: 0;	
	background-color: #f5f5f5;
	overflow: hidden;
	min-width: 320px;
	max-width: 640px;
}
.life_keyboard_H{
	height: 0;
}
.life_switch{
	width: 100%;
	height: 2em;
	padding-top: 0.5em;
	background-color: #f5f5f5;
}
.life_switch img{
	height: 1.0em;
	margin: 0 auto;
}
.life_different_delete{
	position: relative;
}
.life_keyboard_ul .life_different_delete img{
	width: 30%;
	position: absolute;
	top: 30%;
	left: 35%;
}
.life_keyboard_ul{
	width: 100%;
	overflow: hidden;
	border-top: 1px solid #d2d2d2;
	border-left: 1px solid #d2d2d2;
	border-right: 1px solid #d2d2d2;
}
.life_border{
	border-right: 1px solid #d2d2d2;
	border-left: 1px solid #d2d2d2;
}
.life_keyboard_ul li{
	width: 33.33%;
	float: left;
	height: 2.5em;
	line-height: 2.5em;
	text-align: center;
	font-size: 1.5em;
	border-bottom: 1px solid #d2d2d2;
}
.life_different_point,.life_different_delete{
	background-color: #e0e0e0;
}
.life_pay_alert_wrap{
	position: absolute;
	width: 100%;
	height: 100%;
	min-width: 320px;
	max-width: 640px;
	background-color: rgba(0,0,0,0.5);
	top: 0;
	padding-top: 50px;
	display: none;
}
.enter_pass_alert,.wrong_alert{
	width: 80%;
	margin-left: 10%;
	background-color: #FFFFFF;
	position: absolute;
	display: none;
	top: 50%;
    margin-top: -50%;
}
.enter_pass_alert_title{
	display: inline-block;
	width: 100%;
	height: 2em;
	font-size: 1.2em;
	line-height: 2em;
	overflow: hidden;
	padding-left: 3%;
	color: #222222;
	border-bottom: 1px solid #d5d3d3;
}
.wrong_prompting{
	font-size: 1.3em;
	line-height: 5em;
	text-align: left;
	padding-left: 5%;
}
.enter_pass_alert_title span,.wrong_alert_delete span{
	float: right;
	color: #d5d3d3;
	margin-right: 5%;	
	font-size: 1.5em;
}
.transfer_amount_ident{
	text-align: center;
	margin-top: 1.2em;
	font-size: 1.2em;
}
.transfer_amount{
	text-align: center;
	font-size: 1.8em;
	line-height: 2em;
}
.life_alert_sure_btn{
	width: 100%;
	font-size: 1.6em;
	line-height: 2em;
	text-align: center;
	background-color: #d9333f;
	color: white;
}
/*模拟密码输入框*/
.alert_box{
    width: 94%;
    margin-left: 3%;
    background: #fff;
    text-align: center;
}
.alert_box input{
	margin: 0;
	padding: 0;
	width: 1px;
	opacity: 0;
	height: 1px;
	border: none;
}
.alert_box label{
    display: block;
}
.alert_box ul{
    border: 1px solid #c8c8c8;
    font-size: 0;
    display: inline-block;
    position: relative;
    font-size: 0;
}
.alert_box ul li{
    display: inline-block;
    width: 35px;
    height: 35px;
    font-size: 24px;
    font-weight: 700;
    text-align: center;
    line-height: 40px;
    border-left: 1px solid #e6e6e6;
    vertical-align: middle;
    overflow: hidden;
}
.alert_box ul li:first-child {
    border-left: none;
}
.wrong_two_btn{
	overflow: hidden;
	margin-bottom: 1em;
}
.wrong_two_btn span{
	float: right;
	margin-right: 0.7em;
	font-size: 1.1em;
}
.wrong_again{
	color: #d9333f;
}
.wrong_alert{
	top: 50%;
	margin-top: -50%;
}


.alert_ipt{
	border: 1px solid red;
}
