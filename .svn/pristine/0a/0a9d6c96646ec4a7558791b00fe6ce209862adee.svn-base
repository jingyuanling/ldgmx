body{
	background-color: #f3f5f7;
}
.wine_personal_content{
	padding-top: 39px;
}
.wine_three_div{
	overflow: hidden;
	padding: 0 0.8em;
	font-size: 0.8em;
	line-height: 3.2em;
	border-bottom: 1px solid #d7d7d7;
}
.wine_three_div .wine_gt,.wine_percent{
	float: right;
}
.wine_percent{
	margin-right: 0.2em;
}
.wine_three_div .wine_gt{
	font-weight: 700;
}
.wine_three_div span img{
	display: inline-block;
	margin-right: 0.5em;
	height: 1.2em;
}
.wine_three_div{
	background-color: #f8f8f8;
	overflow: hidden;
}
.wine_three_div span:nth-child(2){
	font-size: 0.8em;
	color: #999999;
}
.wine_three_div span:nth-child(3){
	font-size: 0.75em;
	color: #999999;
	float: right;
}
.wine_personal_inp{
	background-color: #f8f8f8;
	outline:medium;
	border: 1px solid #d7d7d7;
	border-radius: 0.5em;
	width: 80%;
	font-size: 0.75em;
	color: #999999;
	padding: 0.2em 0.2em;
	box-sizing: border-box;
}
.wine_personalR{
	float: right;
	display: none;
} 
/*生日页面*/
.wine_birthday_wrap{
	width: 100%;
	height: 100%;
	padding-top: 39px;
}
.wine_birthday_content{
	overflow: hidden;
	border: 1px solid #d7d7d7;
	background-color: #f8f8f8;
	padding: 1em 0.5em;
	display: flex;
	justify-content: space-around;
}
.wine_birthday_content>div{
	font-size: 1em;
	float: left;
	text-align: center;
}
.wine_birthday_content select{
	font-size: 1em;
	text-align: center;
	border-radius: 0.2em;
	border: 1px solid #d7d7d7;
}
.wine_birthday_text{
	width: 20%;
}
