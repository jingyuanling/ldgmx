/*弹框*/
.generate_alert{
	font-size: 0;
}
.generate_alert .modal-dialog{
	width: 4.5rem;
	height: 4.5rem;
	position: absolute;
	top: 50%;
	left: 50%;
	margin-top: -2.25rem;
	margin-left: -2.25rem;
}
.generate_alert .modal-content{
	width: 100%;
	height: 100%;
	border-radius: 0;
	background-color: white;
	padding: 0.48rem 0.27rem;
}
.generate_alert .right-icon{
	margin-top: 0.6rem;
	width: 1.38rem;
	height: 1.38rem;
	margin-bottom: 0.45rem;
}
.generate_alert .sure_btn{
    width: 0.5rem;
    height: 0.5rem;
    background-color: #eeeeee;
    font-size: 0.4rem;
    padding: 0;
    position: absolute;
    left: 100%;
    top: 0;
    border-radius: 0;
}
.generate_alert .sure_btn img{
	width: 0.3rem;
	height: 0.29rem;
	margin-top: 0.11rem;
}
#upload-code-btn img{
	width: 100%;
}
.generate_alert .infor{
	font-size: 0.24rem;
	color: #1c1c1c;
}
.material-sort .btn-group .btn{
	text-align: center;
}
.data-list-li{
	display: flex;
	justify-content: space-between;
	overflow: hidden;
	height: 0.6rem;
	line-height: 0.6rem;
}
.data-list-li span{
	font-size: 0.24rem;
	width: 100%;
	float: left;
	text-align: center;
}
.data-list-li .details-use-btm{
	color: #f03b42;
	background-color: transparent;
}
