.wine_header{
    width: 100%;
    max-width: 640px;
    min-width: 320px;
    height: 39px;
    background-color: #d9333f;
    font-family:"微软雅黑";
    position: fixed;
    z-index: 999999;
    overflow: hidden;
}
.wine_header img{
    position: absolute;
    float: left;
    width: 10px;
    margin-top: 10px;
    margin-left: 13px;
}
.wine_header p{
    letter-spacing: 2px;
    font-size: 1.5em;
    color: #fff;
    text-align: center;
    line-height:39px;
    box-sizing: border-box;
}
.wine_header_right{
	font-size: 1.5em;
    color: #fff;
    position: absolute;
    right: 0;
    line-height: 39px;
    top: 0;
    margin-right: 10px;
}
