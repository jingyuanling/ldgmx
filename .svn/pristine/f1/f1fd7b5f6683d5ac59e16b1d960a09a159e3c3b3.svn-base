body{
	font-family: "微软雅黑";
}
.zone-head{
	width: 100%;
	border-bottom: 3px solid #00c65d;
	background-color: white;
}
.zone-head-center{
	width: 1200px;
	height: 110px;
	padding-top: 40px;
	margin: 0 auto;
}
.zone-head-center .ld-logo{
    width: 177px;
    height: 31px;
	float: left;
}
.zone-head-center .ld-logo img{
    width: 100%;
    height: 100%;
}
.zone-head-center .head-search-inp{
	width: 615px;
	height: 70px;
	float: left;
	margin-left: 229px;
	font-size: 15px;
	position: relative;
}
.zone-head-center .head-search-inp input{
	width: 100%;
	height: 38px;
	position: absolute;
	left: 0;
	padding: 0 80px 0 5px;
	border: 3px solid #00c65d;
}
.zone-head-center .search-btn{
	width: 80px;
	height: 32px;
	position: absolute;
	top: 3px;
	right: 3px;
	color: white;
	background-color: #00c65d;
	text-align: center;
	line-height: 32px;
	font-size: 16px;
}
.zone-head-center .search-btn:hover{
	cursor: pointer;
}
.zone-head-center .top-search{
	display: inline-block;
	width: 365px;
	height: 30px;
	position: absolute;
	top: 38px;
	left: 0;
	white-space: nowrap;
	overflow: hidden;
}
.zone-head-center .top-search span{
	font-size: 15px;
	color: #999999;
	margin-right: 8px;
	line-height: 30px;
	float: left;
}
.zone-cart-btn{
	display: block;
	width: 140px;
	height: 38px;
	float: right;
	font-size: 12px;
	line-height: 34px;
	border: 2px solid #e7e7e7;
	position: relative;
	text-align: center;
	background-color: #f9f9f9;
	padding-left: 8px;
}
.zone-cart-btn .cart-img{
	width: 19px;
	height: 20px;
	top: 7px;
	left: 9px;
	display: inline-block;
	position: absolute;
}
.zone-cart-btn .cart-goods-num{
	min-width: 18px;
	height: 18px;
	display: inline-block;
	background-color: #da1026;
	border-radius: 9px;
	text-align: center;
	position: absolute;
	top: -7px;
	right: -1px;
	line-height: 18px;
	color: white;
	padding: 0 2px;
}
.cart-zero-hover{
	position: absolute;
	top: 36px;
	right: 0;
	height: 71px;
	width: 298px;
	padding-right: 20px;
	line-height: 71px;
	text-align: right;
	font-size: 12px;
	color: #999999;
	border: 1px solid #e9e9e9;
	border-bottom: 2px solid #e9e9e9;
	display: none;
}
.zone-cart-btn .sleep-cart-img{
	width: 51px;
	height: 44px;
	position: absolute;
	top: 13.5px;
	left: 40px;
}
.zone-cart-btn:hover .cart-zero-hover{
	display: block;
}
.zone-head .zone-menu{
	width: 1200px;
	padding-left: 223px;
	height: 31px;
	margin: 0 auto;
	line-height: 31px;
	overflow: hidden;
	position: relative;
	color: #333333;
}
.zone-head .zone-menu a{
	font-size: 15px;
	padding: 0 27px;
}
.zone-head .zone-menu a:after {
    content: "";
    position: absolute;
    top: 50%;
    margin-top: -7px;
    width: 2px;
    height: 14px;
    background: #222;
    margin-left: 22px;
}
.zone-head .zone-menu .last-a:after{
	height: 0;
}
.zone-menu .menu-slogan{
	width: 200px;
	height: 40px;
	font-size: 13px;
	color: #00c457;
	float: right;
	line-height: 40px;
	text-align: center;
	display: inline-block;
}
.zone-menu .menu-slogan img {
	width: 16px;
	height: 15px;
	margin-right: 5px;
	display: inline-block;
}
.zone-menu .fine-goods-nav-tit{
	width: 190px;
	height: 100%;
	font-size: 16px;
	line-height: 31px;
	color: white;
	background-color: #00c65d;
	position: absolute;
	left: 0;
	text-align: center;
}
.zone-head-center .top-search span:hover,
.zone-menu a:hover,.fine-goods-nav a:hover,
.features-buy-content .goods_item:hover .goods_item_info .p2{
	color: #00c65d;
	cursor: pointer;
}



/*隐藏分类列表样式*/
.fine-goods-nav{
	width: 190px;
	position: absolute;
	background-color: white;
	background: hsla(0,0%,100%,.95);
	box-shadow: 0 1px 10px rgba(0,0,0,.14);
	float: left;
	background-color: #FFFFFF;
	z-index: 99;
	top: -45px;
}
.fine-goods-nav-tit{
	width: 100%;
	height: 35px;
	font-size: 15px;
	color: white;
	line-height: 35px;
	text-align: center;
	background-color: #00c65d;
}
.fine-goods-nav-tab{
	height: 400px;
	position: relative;
	display: none;
}
.fine-goods-nav-tab .tab-head{
	width: 190px;
	height: 100%;
	padding: 10px 0 20px;
}

.fine-goods-nav-tab .tab_head_item{
	width: 100%;
	height: 74px;
	padding: 0 15px 0;
	border-bottom: 1px dotted #ddd;
}
.fine-goods-nav-tab .tab_head_item_last{
	border: none;
}
.tab_head_item img{
	position: absolute;
	display: inline-block;
	top: 2px;
    left: -3px;
}
.fine-goods-nav-tab .item_title{
	font-size: 15px;
	height: 25px;
	line-height: 25px;
	display: inline-block;
	width: 100%;
	position: relative;
	margin-top: 15px;
	padding-left: 35px;
}
.tab_head_item .item_title span{
	display:inline-block;
	white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
    width: 122px;
}
.fine-goods-nav-tab .item_title:after{
	content: ">";
    position: absolute;
    right: 0;
    top: 5px;
    height: 19px;
    line-height: 19px;
    font-family: simsun;
}
.tab_head_item .item_children{
	white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
    padding-left: 30px;
    position: relative;
}
.tab_head_item .item_children a{
	font-size: 15px;
	color: #999999;
}
.tab_head_item .item_children a{
	padding: 0 5px;
}
.tab_head_item .item_children a:after{
	content: "";
    position: absolute;
    top: 50%;
    margin-top: -7px;
    width: 1px;
    height: 14px;
    background: #999999;
    margin-left: 4.5px;
}
.tab_head_item .item_children a:last-child:after{
	height: 0;
}
/*二级分类*/
.fine-goods-nav .tab_body{
	position: absolute;
    left: 190px;
    top: 0;
    z-index: 10;
    display:none;
}
.fine-goods-nav-tab .tab_head_item:hover .tab_body{
	display:block!important;
}
.fine-goods-nav .tab_body_item{
	padding: 10px 20px;
    width: 810px;
    min-height: 400px;
    background: #fff;
    overflow: hidden;
    border: 1px solid #ddd;
}
.fresh_fs_nav_cate{
	float: left;
    width: 100%;
}
.fresh_fs_nav_cate .cate_item{
	position: relative;
    padding-left: 80px;
}
.fresh_fs_nav_cate .cate_item .cate_tit{
	overflow: hidden;
    position: absolute;
    left: 0;
    top: 9px;
    width: 70px;
    text-align: right;
    font-weight: 700;
    white-space: nowrap;
    text-overflow: ellipsis;	
}
.fresh_fs_nav_cate .cate_tit_arrow{
    margin-left: 5px;
    font-size: 13px;
    font-family: simsun;
    font-style: normal;
    color: #666;
}
.cate_tit_lk{
	font-size: 13px;
	color: #666;
}
.tab_body_item .cate_con{
	overflow: hidden;
    padding: 6px 10px 6px 0;
    border-bottom: 1px solid #eee;
}
.tab_body_item  .cate_con_lk{
	font-size: 13px;
	float: left;
    margin: 4px 0;
    padding: 0 10px;
    height: 16px;
    border-left: 1px solid #e0e0e0;
    line-height: 16px;
    white-space: nowrap;
    color: #666;
}
.fine-goods-nav:hover{
	color: #00c65d;
	cursor: pointer;
}
.fine-goods-nav-tit:hover .fine-goods-nav-tab{
	display: block;
}
