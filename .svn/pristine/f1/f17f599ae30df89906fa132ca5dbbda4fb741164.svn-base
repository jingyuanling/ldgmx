body{
	background-color: #f3f5f7;
}
.wine_goodsList_head{
    width: 100%;
    max-width: 640px;
    min-width: 320px;
    height: 37px;
    background: #d9333f;
    position: fixed;
    z-index: 100;
    height: 39px;
}
.wine_goodsList_head_return img{
    width: 10px;
    margin-left: 15px;
    margin-top: 7px;
    float: left;
}
.wine_ipt{
	width: 20em;
    margin-left: 20px;
    height: 37px;
    float: left;
}
.wine_ipt .wine_ipt_inp{
    width: 100%;
    border:none;
    height: 24px;
    float: left;
    margin-top: 6px;
    border-radius: 5px;
    padding-left: 18px;
    font-size: 0.7em;
}
.wine_ipt img{
    width: 12px;
    float: left;
    margin-left: 3px;
    margin-top: -19px;
    margin-right: 7px;
}
.wine_home{
	float: right;
	height: 100%;
	line-height: 39px;
	margin-right: 20px;
}
.wine_home span{
	color: white;
	font-size: 1.2em;
}
.search_content{
	width: 100%;
	margin-top: 39px;
	padding: 20px 10px 20px 30px;
	box-sizing: border-box;
}
.wine_hot_search_titile{
	font-size: 1.5em;
	margin-bottom: 10px;
	margin-left: -20px;
}
.search_content_ul{
	list-style: none;
}
.search_content_ul>li{
	font-size: 1.0rem;
	display: inline-block;
	background-color: #dce1e9;
	padding: 10px 20px;
	margin-bottom: 10px;
	margin-right: 10px;
}

