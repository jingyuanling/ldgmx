.header{
    width: 100%;
    height: 423px;
    background: url(../img/head.jpg) no-repeat center;
}
.click{
    display: block;
    width: 100%;
    height: 550px;
    background: url(../img/a.jpg) no-repeat center;
}
.content{
    display: block;
    width: 100%;
    height: 670px;
    background: url(../img/content.jpg) no-repeat center;
}
.saoma{
    text-align: center;
    position: relative;
}
.saoma .picture{
    width: 260px;
    height: 260px;
    padding: 25px;
}
.saoma .left_icon,
.saoma .right_icon{
    width: 176px;
    height: 263px;
    position: absolute;
}
.saoma .left_icon{
    left: 0;
    top:20px;
}
.saoma .right_icon{
    right: 0;
    top:20px;
}
.introduce{
    width: 1017px;
    margin: 0 auto;
}
.introduce p{
    font-size: 20px;
    line-height: 35px;
    text-indent:2em;
}
.intro{
    width: 1017px;
    background-color: #ecefef;
    margin: 0 auto;
}
.intro p{
    font-size: 20px;
    line-height: 35px;
    text-indent:2em;
}
.rongyu{
    background-color: #ecefef;
    margin: 0 auto;
    width: 1017px;
    height: 330px;
}
.rongyu ul{
    width: 100%;
    height: auto;
    overflow: hidden;
    box-sizing: border-box;
}
.rongyu ul li {
    width: 315px;
    height: 216px;
    float: left;
    padding: 10px;
}
.rongyu ul li img{
    width: 326px;
    height: 216px;
}
.hezuo{
    width: 1097px;
    height: 450px;
    margin: 0 auto;
}
.hezuo ul{
    width: 100%;
    height: auto;
    overflow: hidden;
    box-sizing: border-box;
}
.hezuo ul li {
    width: 254px;
    height: 130px;
    float: left;
    padding: 10px;
}
.hezuo ul li img{
    width: 260px;
    height: 130px;
    border: 1px solid #1a1315;
}