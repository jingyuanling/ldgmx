body{
    width: 100%;
    min-width: 320px;
    max-width: 640px;
    margin: 0 auto;
    font-family: "微软雅黑";
}
html{
    background-color: #eaeaea;
    min-width: 320px;
    max-width: 640px;
    width: 100%;
    margin: 0 auto;
}
img{
  border:0;
  display: block;
}
.clearfix:after {
visibility: hidden;
display: block;
font-size: 0;
content: " ";
clear: both;
height: 0;
}
.geren-box{
    width: 100%;
    height: 100%;
    margin:0 auto;
    background-color: #eaeaea;
}
.main-top{
    width: 100%;
    height: 39px;
    background-color: #548e11;
}
.main-top img{
    position: absolute;
    float: left;
    width: 10px;
    margin-top: 10px;
    margin-left: 13px;

}
.main-top p{
    letter-spacing: 2px;
    font-size: 14px;
    color: #fff;
    text-align: center;
    line-height:39px;
}
.shezhi-cont{
    width: 100%;
    margin-top: 40px;
}
.shezhi-cont input{
    border:none;
    padding-left: 15px;
    font-size: 12px;
    line-height: 38px;
    color:#000;
}
.shezhi-cont .list1{
    color:#000;
    width: 100%;
    height: 40px;
    background-color:#fff;
    border-top: 1px solid #bfbfbf;
}


.shezhi-cont .list2{
    border-bottom: 1px solid #bfbfbf;
}
.zhanghao p{
    width: 100%;
    height: 40px;
    font-size: 12px;
    float: left;
    line-height: 38px;
    padding-left: 15px;
}
p.xiugai{
    width: 200px;
    margin:0 auto;
    font-size: 12px;
    background-color: #fff;
    line-height: 33px;
    height: 35px;
    border: 1px solid #d1d0d0;
    border-radius: 8px;
    margin-top: 20px;
    text-align: center;
}
p.ps{
    font-size: 12px;
    color: #000;
    padding-left: 15px;line-height: 38px
}