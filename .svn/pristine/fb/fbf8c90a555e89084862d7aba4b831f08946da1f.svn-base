.gerendetails-box{
    width: 100%;
    height: 100%;
    background-color: #f3f5f7;
    padding-top: 0.8rem;
}
.main-top{
    width: 100%;
    height: 39px;
    background-color: #548e11;
}
.main-top img{
    position: absolute;
    float: left;
    width: 10px;
    margin-top: 10px;
    margin-left: 13px;

}
.main-top p{
    letter-spacing: 2px;
    font-size: 14px;
    color: #fff;
    text-align: center;
    line-height:39px;
}
.main-top p.xinzeng{
    float: right;
    position: relative;
    margin-top: -39px;
    margin-right: 13px;
}
.adress{
    background-color: #fff;
    width: 100%;
    line-height: 0.5rem;
    padding: 0.2rem;
    color: #222222;
    border-bottom: 0.01rem solid #bbbbbb;
}
.adress p{
    width: 100%;
    color: #000;
    margin-bottom: 0.2rem;
}
.adress p span.name{
	font-size: 0.3rem;
    margin-right: 15px;
    font-family: script;
}
.adress p .phone_num{
	color: #999999;
}
.adress p span.moren{
    display: block;
    width: 40px;
    height: 20px;
    text-align: center;
    line-height: 20px;
    background-color: #e84e40;
    color: #fff;
    margin-right: 10px;
}
.adress p span{
	font-size: 0.26rem;
    float: left;
}
.adress p span.dizhi{
    float: left;
    width: 100%;
    padding-right: 0.2rem;
    line-height: 0.3rem;
}
.adress p span.right{
    float: right;
    margin-top:-0.3rem;
}
.adress .shemoren{
    position: relative;
    font-size: 12px;
    margin-top: 10px;
    margin-bottom: 0;
}
.adress .shemoren p{
    margin-bottom: 0px;
    font-size: 0.26rem;
}
.adress .shemoren input{
    width: 0.26rem;
    height: 100%;
	position: absolute;
	left: 0;
}
.adress .shemoren p.left{
    float: left;
    padding-left: 0.4rem;
}
.adress .shemoren p.right{
	display: inline-block;
	position: absolute;
    right: 0;
    width: 1rem;
}
.adress .shemoren p.right img{
    float: left;
    width: 18px;
    vertical-align: top;
    margin-top: 7px;
}
.adress .shemoren p.right span{
    display: block;
}
