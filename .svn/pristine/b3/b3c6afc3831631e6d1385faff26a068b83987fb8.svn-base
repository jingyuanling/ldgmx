body{
	background-color: #f3f5f7;
}
.wine_withdraw_balance{
	width: 100%;
	overflow: hidden;
	padding: 39px 1.0em;
	box-sizing: border-box;
	line-height: 2.0em;
}
.wine_withdraw_balance span{
	font-size: 0.3em;
}
.wine_withdraw_balance_span1{
	float: left;
}
.wine_withdraw_balance_span2{
	float: right;
	color: #d9333f;
}
.wine_withdraw_balance_btn{
    font-size: 0.3rem;
    text-align: center;
    width: 60%;
    margin: 0 auto;
    margin-top: 0.3rem;
    border: 1px solid #222222;
    line-height: 0.6rem;
    color: #222222;
}
.wine_withdraw_balance_prompt{
    font-size: 0.2rem;
    color: #999999;
    text-align: center;
    margin-top: 0.2rem;
}
/*提现成功*/
.wine_withdraw_success_wrap{
	width: 100%;
	padding-top: 1.8rem;
	/*position: absolute;*/
}
.wine_withdraw_success_span1,.wine_withdraw_success_span2{
	display: block;
	width: 100%;
	text-align: center;
	margin-bottom: 0.5em;
}
.wine_withdraw_success_span1{
	font-size: 0.3rem;
}
.wine_withdraw_success_span2{
	font-size: 0.3rem;
}
.wine_withdraw_success_span2 a{
	color: #D9333F;
	font-size: 0.4rem;
	margin-left: 0.1rem;
}
