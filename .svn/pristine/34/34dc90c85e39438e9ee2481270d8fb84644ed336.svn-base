ul.order-manage-ul{
	padding-top: 0;
}
.order-details-pay-btn{
	width: 100%;
	height: 1.25rem;
	overflow: hidden;
	padding-top: 0.225rem;
	border-top: 0.01rem solid #eaeaea;
}
.order-details-pay-btn a{
	width: 50%;
	height: 0.7rem;
	font-size: 0.3124rem;
	display: inline-block;
	line-height: 0.7rem;
	text-align: center;
	color: #333333;
	border-radius: 0.05rem;
}
.order-details-pay-btn img{
	width: 0.38rem;
	height: 0.38rem;
	margin-right: 0.15rem;
	position: relative;
	top: -0.02rem;
}
.choose-pay-way-tit{
	font-size: 0.2917rem;
	color: #333333;
	height: 0.73rem;
	line-height: 0.73rem;
	padding-left: 0.37rem;
}
.order-details-banner{
	height: 1.68rem;
	width: 100%;
	overflow: hidden;
	position: relative;
	color: #FFFFFF;
	background: -webkit-linear-gradient(90deg, #d10000, #ee0e0f); /* Safari 5.1 - 6.0 */
    background: -o-linear-gradient(90deg, #d10000, #ee0e0f); /* Opera 11.1 - 12.0 */
    background: -moz-linear-gradient(90deg, #d10000, #ee0e0f); /* Firefox 3.6 - 15 */
    background: linear-gradient(90deg, #d10000, #ee0e0f); /* 标准的语法（必须放在最后） */
}
.order-details-banner img{
	width: 1.95rem;
	height: 1.41rem;
	position: absolute;
	bottom: 0;
	right: 0.77rem;
}
.order-details-banner{
	padding-top: 0.4rem;
}
.order-details-banner .order-pay-state{
	font-size: 0.3334rem;
}
.order-details-banner .order-pay-remind{
	font-size: 0.25rem;
}
.order-details-banner .order-pay-state,.order-details-banner .order-pay-remind{
	height: 0.3611rem;
	line-height: 0.3611rem;
	padding-left: 0.58rem;
}
.order-details-address{
	background-color: #FFFFFF;
	height: 2.08rem;
	width: 100%;
	padding: 0.34rem 0.37rem 0 0.55rem;	
}
.order-details-address .user-message{
	font-size: 0.2917rem;
	color: #333333;
	margin-right: 0.28rem;
}
.order-details-user,.order-details-address .address-p{
	line-height: 0.3611rem;
}
.order-details-address img{
	width: 0.33rem;
	height: 0.32rem;
	float: right;
}
.order-details-address .address-p{
	color: #999999;
	font-size: 0.25rem;
	margin-top: 0.1rem;
	display: -webkit-box;
	-webkit-box-orient: vertical;
	-webkit-line-clamp: 2;
	overflow: hidden;
}
.order-details-btm{
	padding: 0.4rem 0 0.3rem;
	background-color: #FFFFFF;
	position: relative;
}
.order-details-btm-state{
	padding: 0 0.37rem 1.05rem;
}
.order-details-btm-state p{
	height: 0.51rem;
	line-height: 0.51rem;
	overflow: hidden;
}
.order-num-copy{
	display: inline-block;
	float: right;
	color: #d10000;
	width: 1.26rem;
	height: 100%;
	font-size: 0.25rem;
	line-height: 0.46rem;
	text-align: center;
	border: 0.02rem solid #d10000;
	border-radius: 0.05rem;
}
.order-details-btm-state p span{
	font-size: 0.25rem;
	color: #666666;
}
.order-buy-details{
	width: 7.5rem;
	overflow: hidden;
	position: absolute;
	bottom: 0;
	left: 0;
	border-top: 0.01rem solid #f5f5f5;
}
.order-buy-details .left,.order-buy-details .buy-btn{
	float: left;
	height: 1.05rem;
	text-align: center;
	line-height: 1.04rem;
}
.order-buy-details .left{
	width: 4.38rem;
}
.order-buy-details .buy-btn{
	width: 3.12rem;
	background-color: #D10000;
}
.order-buy-details .buy-btn a{
	font-size: 0.3334rem;
	color: #FFFFFF;
	display: inline-block;
	width: 100%;
	height: 100%;
}
.order-buy-details .left .span1{
	font-size: 0.2917rem;
	color: #333333;
}
.order-buy-details .left .span2{
	font-size: 0.375rem;
	color: #d10000;
}
.order-buy-details .left .span3{
	font-size: 0.25rem;
	color: #999999;
	margin-left: 0.1rem;
}
.order-details-apply{
	height: 1.25rem;
	padding-top: 0.28rem;
	border-top: 0.01rem solid #f0f0f0;
}
.order-details-apply .apply-btn{
	font-size: 0.3125rem;
	color: #e20000;
	height: 0.69rem;
	width: 2.13rem;
	border: 0.01rem solid #e20000;
	border-radius: 0.05rem;
	float: right;
	text-align: center;
	line-height: 0.67rem;
}
/*猜你喜欢*/
.person-guess-like{
	margin-top: 0.17rem;
}
.person-order-tit {
    display: inline-block;
    width: 100%;
    padding: 0 0.37rem;
    background-color: #FFFFFF;
    position: relative;
    height: 1.05rem;
    line-height: 1.05rem;
    border-bottom: 0.01rem solid #dbdbdb;
}
.person-order-tit .vertical-line {
    width: 0.08rem;
    height: 0.45rem;
    background-color: #d10000;
    position: absolute;
    left: 0;
    top: 0.3rem;
    border-radius: 0.04rem;
}
.person-order-tit .left {
    font-size: 0.2917rem;
    color: #333333;
}
.confirm-receipt-btn{
	width: 100%;
	display: block;
	position: fixed;
	height: 1.05rem;
	max-width: 750px;
	min-width: 320px;
	bottom: 0;
	font-size: 0.3333rem;
	color: #FFFFFF;
	background-color: #d10000;
	text-align: center;
	line-height: 1.05rem;
}
.order-details-banner .order-pay-over-state{
	height: 100%;
	line-height: 0.88rem;
}
.order-details-height-banner{
	height: 2.88rem;
}
.banner-return-address{
	margin-top: 0.39rem;
}
.banner-return-address p{
	padding: 0 0.37rem 0 0.58rem;
	line-height: 0.3611rem;
}
.banner-return-address .return-message,.banner-return-address .return-message span{
	font-size: 0.2916rem;
	color: #FFFFFF;
}
.banner-return-address .return-message{
	display: -webkit-box;
	-webkit-box-orient: vertical;
	-webkit-line-clamp: 2;
	overflow: hidden;
}
.banner-return-address .order-pay-remind{
	margin-bottom: 0.1rem;
}
.order-details-height-banner2{
	height: 2.67rem;
}
