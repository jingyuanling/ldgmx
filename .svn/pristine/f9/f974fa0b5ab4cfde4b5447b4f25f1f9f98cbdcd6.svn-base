body{
	font-family: "微软雅黑";
}
.draw-wrap{
	width: 100%;
	height: 100%;
	overflow-y: scroll;
	overflow-x: hidden;
	min-width: 1200px;
	background-color: #fee3c1;
	padding-bottom: 50px;
}
.draw_content_upper{
	background: url(../img/wrap-bg.png) 0 0 no-repeat;
	background-size: 100% 100%;
}
.draw-content,.draw_content_upper{
	width: 100%;
	height: 740px;
}
.draw-content,.draw_content_down{
	width: 1110px;
	margin: 0 auto;
}
.draw_content_tit{
	width: 100%;
}
.draw_content_tit img{
	width: 507px;
	height: 202px;
}
.draw-center{
	width: 100%;
	margin-top: -20px;
	position: relative;
}
.left_winners,.left_winners_tit{
	border: 3px dashed #8e0404;
	background-color: #f94b00;
}
.left_winners{
	width: 342px;
	height: 407px;
	margin-top: 42px;
	border-radius: 8px;
}
.left_winners_tit{
	width: 165px;
	height: 45px;
	margin: -45px auto 0;
	border-bottom: none;
	border-radius: 8px 8px 0 0;
	font-size: 31.61px;
	text-align: center;
	color: white;
}
.left_winners_con{
	width: 100%;
	height: 100%;
	padding-top: 20px;
}
.swiper-container{
	width: 100%;
	height: 100%;
}
.left_winners_con .winners_line{
	width: 100%;
	font-size: 14px;
	color: white;
}
.left_winners_con .winners_line span{
	display: inline-block;
	text-align: center;
}
.left_winners_con .winners_line span:nth-child(1){
	width: 65px;
}
.left_winners_con .winners_line span:nth-child(2){
	width: 142px;
}

.left_winners_con .winners_line span:nth-child(3){
	width: 120px;
}
.right_even_draw{
	width: 715px;
	height: 448px;
	background: url(../img/slot-machines-bg.png) 0 0 no-repeat;
	background-size: 100% 100%;
	position: absolute;
	right: 0;
	top: -42px;
	padding: 35px;
}
.right_even_draw_content{
	width: 100%;
	height: 100%;
}
.draw_content_ul li{
	/*border: 2px solid #ff0000;*/
	border: 2px solid transparent;
}
.draw_content_ul .draw_img img{
	width: 118px;
	height: 113px;
}
.draw_content_ul li{
	margin-left: 5px;
	margin-top: 8px;
	display: inline-block;
}
.draw_content_ul li:nth-child(odd) img{
	border: 2px solid rgba(255,255,255,0.8);
}
.draw_content_ul li:nth-child(even) img{
	border: 2px solid rgba(0,0,0,0.3);
}
.draw_content_ul .draw_two_btn{
	width: 378px;
	height: 92px;
	border-color: rgba(255,255,255,0.8);
    top: -13px;
    position: relative;
    border-radius: 5px;
}
.draw_content_ul .draw_two_btn .draw_img{
	width: 100%;
	height: 100%;
	overflow: hidden;
	position: relative;
}
.draw_content_ul .draw_two_btn .draw_img img{
	float: left;
	width: 176px;
	height: 64px;
	border: none;
	margin-left: 7.5px;
    margin-top: 12px;
}
#mapCanvas{
	display: block;
	position: absolute;
	top: 0;
	left: 0;
	border-radius: 5px;
}
.right_bcbbc{
	width: 335px;
	height: 337px;
	position: absolute;
	left: 105%;
	top: 100px;
}
.right_bcbbc img,.right_girl img,.left_boy img{
	width: 100%;
	height: 100%;
}
.right_girl{
	width: 159px;
	height: 265px;
	position: absolute;
	left: -150px;
	bottom: 0;
}
.draw_content_down{
	padding-top: 30px;
}
.draw_ruls{
	position: relative;
	padding-left: 230px;
}
.draw_ruls_tit img{
	width: 253px;
	height: 65px;
}
.draw_ruls_text{
	margin-top: 15px;
	padding: 15px 30px;
	border-radius: 10px;
	border: 4px dashed #f94b00;
}
.draw_ruls_text div{
	font-size: 15.81px;
	line-height: 25px;
	font-family: "黑体";
}
.draw_ruls_text div span{
	color: #f94b00;
	float: left;
}
.draw_ruls_text div p{
	padding-left: 30px;
}
.left_boy{
	width: 178px;
	height: 257px;
	position: absolute;
	left: 50px;
	top: 35px;
}
/*弹框*/
.prize-alert-gray{
	width: 100%;
	height: 100%;
	position: fixed;
	top: 0;
	background-color: rgba(0,0,0,0.5);
	display: none;
}
.prize-alert-content{
	width: 630px;
	height: 480px;
	background-color: #de3336;
	position: absolute;
	top: 50%;
	left: 50%;
	margin-top: -240px;
	margin-left: -315px;
}
.prize-alert-content .content_bg{
	width: 100%;
	height: 233px;
	position: absolute;
	bottom: 0;
	left: 0;
}
.alert-prize-zone{
	width: 100%;
	height: 377px;
	border: 4px solid #de3336;
	border-bottom-color: white;
	background-color: #f7c507;
	position: relative;
}
.award-btn{
	width: 212px;
	height: 76px;
	margin: 0 auto;
	margin-top: 14px;
	background: url(../img/award-bg.png) 0 0 no-repeat;
	position: relative;
	font-size: 36.89px;
	color: #a31e31;
	text-align: center;
	line-height: 70px;
}
.alert-prize-zone-ul{
	text-align: center;
	width: 510px;
	margin: 65px auto 0;
}
.alert-prize-zone-ul li{
	display: inline-block;
}
.alert-prize-zone-ul li:nth-child(n+6){
	margin-top: 30px;
}
.alert-prize-zone-ul li .get-prize-img{
	width: 101px;
	height: 88px;
	margin-bottom: 8px;
}
.alert-prize-zone-ul .get-prize-img img{
	margin: 0 auto;
	height: 100%;
}
.alert-prize-zone .prize_name{
	font-size: 12px;
	color: #f63d45;
	text-align: center;
	font-weight: bold;
}
.delete-alert-btn{
	width: 46px;
	height: 46px;
	position: absolute;
	top: 0;
	right: 0;
}
.alert-prize-zone-ul .one-prize .get-prize-img{
	width: 241px;
	height: 210px;
}
.alert-prize-zone .one-prize .prize_name{
	font-size: 20.77px;
}
