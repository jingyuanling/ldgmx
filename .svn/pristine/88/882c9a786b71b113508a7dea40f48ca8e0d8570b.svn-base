@charset "utf-8";
/* CSS Document */
.Goods-details{
	margin:80px auto 40px;
	width:1200px;
}
.Gdetatop{
	overflow:hidden;
}
.Gdleftpic{
	float:left;
}
#mallpic,#mallpic img{
	width:418px;
	height:418px;
	
}
#mallpic{
	border:1px solid #f2f2f2;
}
.productExhibitionbox{
	margin-top:22px;
	overflow:hidden;
}
.productExhibitionbox div{
	float:left;
}
.passcenter{
	width:364px;
}
.passcenter ul{
	height:60px;
	width:364px;
}
.passcenter ul li,.passcenter ul li img{
	width:60px;
	height:60px;
}
.passcenter ul li{
	margin-right:16px;
	float:left;
}
.passcenter ul>li:last-of-type{
	margin:0;
}
.passbutton{
	width:18px;
	height:30px;
	background:rgba(51,51,51,0.4);
	margin:15px 5px 0;
}
.passbutton a{
	display:block;
	height:19px;
	width:11px;
	margin:5px auto;
}
.passleft{
	background:url(../img/tubiao.png) no-repeat -128px -54px;
}
.passright{
	background:url(../img/tubiao.png) no-repeat -145px -54px;
}
.Gdcentermain{
	float:left;
	margin-left:20px;
}
.howmoney{
	width:496px;
	height:40px;
	padding:0 8px;
	margin-top:30px;
	background:yellow;
}
.howmoney span{
	color:#ff4500;
	font-size:22px;
	line-height:40px;
	display:inline-block;
	margin-left:42px;
}
.havemoney{
	margin-top:15px;
}
.havemoney span{
	display:block;
	font-size:12px;
	line-height:30px;
}
.xiaoliang{
	border-bottom:1px dashed #999;
	border-top:1px dashed #999;
	overflow:hidden;
	margin-top:5px;
}
.xiaoliang p{
	float:left;
	border-right:1px dashed #999;
	font-size:12px;
	margin:10px 0;
	width:165px;
	text-align:center;
}
.xiaoliang>p:last-of-type{
	border:none;
}
.xuanxiang{
	margin:10px 0 0 10px;
	font-size:12px;
}
.xuanxiang dl{
	overflow:hidden;
	margin:5px 0;
}
.xuanxiang dt,.xuanxiang dd{
	float:left;
	line-height:30px;
	overflow:hidden;
}
.xuanxiang dt{
	width:70px;
	line-height:30px;
}
.xuanxiang dd p{
	width:68px;
	height:28px;
	border:2px solid #999;
	text-align:center;
	line-height:28px;
	float:left;
	margin-right:10px;
}
.xuanxiang dd p.thisap{
	border-color:#ff4500;
	color:#ff4500;
}
.xuanxiang dd input{
	height:29px;
	width:44px;
	border:1px solid #999;
	text-align:center;
	margin:0 5px;
	line-height:29px;
}
.xuanxiang dd a{
	display:inline-block;
	width:10px;
	height:10px;
	border:1px solid #999;
	text-align:center;
	line-height:10px;
	font-size:6px;
}
.xuanxiang dd span{
	display:inline-block;
	margin-left:20px;
}
.xuanxiang dd div{
	color:#3355aa;
	overflow:hidden;
	line-height:30px;
}
.xuanxiang dd div i{
	display:block;
	width:26px;
	height:26px;
	float:left;
	margin-right:5px;
	margin-top:2px;
	background:red;
}
.twobuttonbuy{
	overflow:hidden;
	width:370px;
	margin:35px auto 0;
}
.twobuttonbuy a{
	display:block;
	width:178px;
	height:38px;
	border:1px solid #c40000;
	background:#ffeded;
	line-height:38px;
	text-align:center;
	color:#c40000;
	float:left;
}
.twobuttonbuy a.ingouwucar{
	background:#c40000;
	color:#fff;
	margin-left:10px;
}
.Gdrightpic{
	width:227px;
	margin-left:20px;
	float:left;
	border-left:1px solid #ccc;
}
.Gdrighttitle{
	border:1px dashed #437ce5;
	width:140px;
	position:relative;
	margin:20px auto;
}
.Gdrighttitle h5{
	position:absolute;
	width:75px;
	font-size:12px;
	height:22px;
	background:#fff;
	text-align:center;
	line-height:22px;
	top:-10px;
	left:32px;
	color:#437ce5;
}
.Gdrighttuijianbox{
	width:130px;
	margin:auto;
	height:460px;
}
.Gdrighttuijianmain{
	width:130px;
	height:370px;
	background:black;
	font-size:12px;
	overflow:hidden;
}
.Gdrighttuijianmain dl,.Gdrighttuijianmain li{
	height:183px;
}
.Gdrighttuijianmain dt img{
	width:130px;
	height:130px;
}
.Gdrighttuijianmain dd{
	height:50px;
}
.Gdrighttuijianmain dd i{
	color:#f00;
	font-style:normal;
}
.Gdrighttuijianmain li{
	margin-bottom:7px;
}
.fangxiang{
	overflow:hidden;
	margin:10px 0 0 5px;
}
.fangxiang div{
	width:50px;
	height:20px;
	float:left;
	background:#999;
	margin:0 5px;
}
.fangxiang div:hover{
	background:#333;
}
.myadvantage{
	border-bottom:1px solid #e5e5e5;
	border-top:1px solid #e5e5e5;
	margin-top:20px;
}
.advantagetitle{
	overflow:hidden;
	background:#f2f2f2;
	height:40px;
	line-height:40px;
	font-size:14px;
}
.advantagetitle h3{
	font-size:14px;
	float:left;
	color:#919191;
	margin-left:20px;
}
.advantagetitle span{
	display:block;
	float:right;
	color:#f25419;
	margin-right:10px;
}
.advanmain{
	overflow:hidden;
	width:840px;
	margin:30px auto;
}
.advanmain ul{
	float:left;
	width:210px;
}
.advanmain ul div{
	width:58px;
	height:58px;
	margin:auto;
	background:black;
}
.advanmain ul dt{
	font-size:16px;
	text-align:center;
	margin-top:16px;
}
.advanmain ul dd{
	font-size:10px;
	text-align:center;
	margin-top:10px;
}
.goodsdebottom{
	overflow:hidden;
	margin-top:10px;
}
.G-dleft{
	float:left;
	width:190px;
}
.tjdianpu{
	width:188px;
	border:1px solid #e5e5e5;
	font-size:12px;
}
.tjdianpu h5{
	font-size:12px;
	background:#fafafa;
	border-bottom:1px solid #e5e5e5;
	line-height:48px;
	height:48px;
	padding-left:16px;
	overflow:hidden;
}
.tjdianpu h5 span{
	display:block;
	max-width:120px;
	text-overflow: ellipsis;
	overflow: hidden;
	white-space: nowrap;
	float:left;
}
.tjdianpu h5 a{
	width:18px;
	height:16px;
	background:url(../img/xiaotubiao.png) no-repeat -75px -177px; 
	display:block;
	float:left;
	margin:16px 0 0 10px;
}
.tjdianpu span{
	font-weight:600;
	display:block;
	text-align:center;
	margin-top:10px;
	line-height:34px;
}
.tjdianpu li{
	overflow:hidden;
}
.tjdianpu .hitscent{
	width:126px;
	margin:25px auto 16px;
}
.tjdianpu .hitscent dl{
	float:left;
	width:42px;
	text-align:center;
}
.tjdianpu li a{
	display:block;
	width:70px;
	height:24px;
	line-height:26px;
	text-align:center;
	float:left;
	color:#666;
	border:1px solid #ccc;
	background:#f5f5f5;
	margin-bottom:14px;
}
.tjdianpu li a.onclone{
	width:72px;
	height:26px;
	background:#333;
	color:#fff;
	margin-left:20px;
	margin-right:6px;
}
.mysearch{
	width:190px;
	margin-top:10px;
}
.mysearch h5{
	width:180px;
	height:24px;
	line-height:24px;
	background:#000;
	color:#fff;
	font-size:12px;
	font-weight:600;
	padding-left:10px;
	border-radius:2px 2px 0 0;
}
.mysearchbox{
	width:188px;
	border:1px solid #ccc;
	font-size:12px;
}
.mysearchbox dl{
	overflow:hidden;
	margin-top:10px;
}
.mysearchbox dl dt{
	float:left;
	width:50px;
	text-align:right;
	line-height:22px;
}
.mysearchbox dl dd{
	float:left;
	width:136px;
	margin-left:2px;
}
.mysearchbox dl dd input{
	border:1px solid #a7a6aa;
	border-radius:2px;
	height:20px;
}
.mysearchbox dl dd .msea{
	width:114px;
}
.mysearchbox dl dd .jgb{
	width:48px;
	margin-right:2px;
}
.mysearchbox dl .bdsstj{
	width:57px;
	height:22px;
	display:block;
	margin:0 auto 10px;
	background:#000;
	color:#fff;
	border-radius:3px;
}
.cbggwbox{
	margin-top:10px;
}
.cbggwbox,.cbggwbox img{
	width:190px;
	height:350px;
}
.G-dcenter{
	float:left;
	margin-left:10px;
}
.complextoptab{
	margin-top:4px;
}
.complextoptab ul{
	border:1px solid #dedfde;
	border-top:2px solid #999;
	width:798px;
	font-size:12px;
	background:#f2f2f2;
}
.complextoptab ul:after{
	display:block;
	height:0;
	clear:both;
	visibility:hidden;
	content:"";
}
.complextoptab ul li{
	float:left;
	height:30px;
	line-height:30px;
}
.complextoptab ul li a{
	position:relative;
	display:block;
	padding:0 10px;
}
.complextoptab ul li a.curr{
	height:34px;
	border:1px solid #dedfde;
	border-bottom:1px solid #fff;
	border-top:2px solid #e4393c;
	background:#fff;
	line-height:34px;
	top:-6px;
	margin-left:-1px;
}
.complexbottomtab .complexmain1{
	border:1px solid #dedfde;
	border-top:none;
	padding:20px;
	font-size:12px;
	width:758px;
	display:block;
}
.complexmain1 h5{
	font-weight:500;
	overflow:hidden;
	line-height:22px;
}
.complexmain1 h5 span{
	color:#3355aa;
}
.complexmain1 h5 a{
	display:inline-block;
	width:74px;
	height:22px;
	border-radius:15px;
	background:#cc0000;
	color:#fff;
	line-height:22px;
	text-align:center;
	margin-left:10px;
}
.complexmain1 dt{
	color:#a59999;
	margin-top:24px;
}
.complexmain1 dd{
	overflow:hidden;
	margin-top:10px;
}
.complexmain1 dd p{
	width:232px;
	float:left;
	text-overflow:ellipsis; 
	overflow:hidden; 
	white-space:nowrap; 
	margin-right:20px;
	line-height:26px;
}
.complexmain2,.complexmain3,.complexmain4{
	display:none;
}
.complexmain2 table{
	background:#ccc;
	font-size:12px;
	margin-top:20px;
}
.complexmain2 th.tdTitle{
	text-align:center;
}
.complexmain2 td{
	background:#fff;
	padding:2px 5px;
}
.complexmain2 th{
	padding:5px;
}
.complexmain2 .tdTitle{
	background:#F5FAFE;
	width:110px;
	text-align:right;
}
.complexmain3{
	font-size:12px;
}
.complexmain3 .com-table-header{
	height: 30px;
    line-height: 30px;
    border: 1px solid #ddd;
    overflow: hidden;
    background-color: #f7f7f7;
	padding: 0 10px;
	margin-top:20px;
}
.complexmain3 .com-table-header span{
	display:block;
	float:left;
	font-size:12px;
	text-align:center;
	font-weight:600;
	color:#666;
}
.complexmain3 .column1{
	width:400px;
}
.complexmain3 .column2{
	width:80px;
}
.complexmain3 .column3{
	width:120px;
}
.complexmain3 .column5{
	width:158px;
}
.comments-item{
	padding:20px 10px;
	border:1px solid #ddd;
	margin-top:-1px;
}
.p-comment .time{
	color:#999;
	font-weight:500;
}
.type-item{
	line-height:22px;
	text-align:center;
}
.user-item{
	overflow:hidden;
	margin-top:5px;
}
.user-item .user-item{
	width: 16px;
    height: 16px;
    float: left;
    margin-right: 5px;
    margin-top: 3px;
	display:block;
}
.user-item .user-name{
	display:block;
	float:left;
}
.complexmain3 .complextoptab{
	margin-top:20px;
}
.G-dright{
	float:left;
	margin-left:10px;
	width:190px;
}
#comment {
	border: 1px solid #ddd;
	border-top: 2px solid #999;
	margin:10px 0;
}
#comment h2 {
	padding: 0 12px;
	font-size: 14px;
	line-height: 30px;
}
#comment .mc {
	padding: 10px 0;
	overflow: hidden;
	zoom: 1;
	clear:both;
}
#comment .rate {
	width: 190px;
	float: left;
	padding: 20px 0 0;
	text-align: center;
}
#comment .rate strong {
	font: 400 46px/30px arial;
	color: #e4393c;
	text-align:center;
}
#comment .rate strong span {
	font-size: 24px;
	color: #e4393c;
	font-family: arial;
}
#comment .percent {
	float: left;
	width: 186px;
	height: 74px;
	padding: 8px 0;
}
#comment .percent dl {
	padding: 2px 0;
	overflow: hidden;
	zoom: 1;
}
#comment .percent dt {
	float: left;
	width: 70px;
}
#comment .percent span {
	color: #9C9A9C;
}
#comment .percent dd {
	float: left;
	width: 100px;
	height: 10px;
	margin-top: 6px;
	overflow: hidden;
	background: #efefef;
}
#comment .percent dd div {
	overflow: hidden;
	height: 10px;
	background-color: #ED0000;
}