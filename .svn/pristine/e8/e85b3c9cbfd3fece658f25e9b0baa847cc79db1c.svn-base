@charset "utf-8";

.attention ul{
	background:#f2f2f2;
	overflow:hidden;
	font-size:14px;
}
.attention ul li{
	font-size:14px;
	float:left;
	height:46px;
	line-height:46px;
	width:118px;
	text-align:center;
	cursor:pointer;
	color:ff4500;
}
.attention ul li .attention_title_bg{
	float:left;
	background:url(../img/qiehuan.png);
	display:block;
	width:88px;
	height:32px;
	margin:7px 15px;
	line-height:32px;
	color:#ff4500;
}
.attention ul a:hover{
	display:block;
	width:88px;
	height:32px;
	margin:7px 15px;
	line-height:32px;
	color:#ff4500;
}
.attentin_main{
	width:1058px;
	border:1px solid #e6e6e6;
	margin-top:20px;
}
.attention_choice{
	width:580px;
	line-height:60px;
	font-size:12px;
	margin-left:20px;
}
.attention_choice input{
	vertical-align:middle;
	margin-top:-2px;
	margin-right:3px;
}
.attention_choice span{
	color:#0088c0;
	cursor:pointer;
}
.attentin_main_lie{	
	overflow:hidden;
	width:1030px;
	margin:auto;
}

.attention_main_lie_1{
	float:left;
	width:230px;
	border:1px solid #e6e6e6;
	margin:0 12px;
	margin-bottom:10px;
	overflow:hidden;
}

.attention_main_lie_1 .attention_main_lie_a img{
	display:block;
	width:178px;
	height:178px;
	margin:10px auto 0;
	background:#ff4500;
}

.attention_main_lie_1 .attention_main_lie_name{
	text-overflow: ellipsis;
	overflow: hidden;
	white-space: nowrap;
	width:200px;
	margin:auto;
	line-height:30px;
	font-size:12px;
	font-family:"宋体";
	color:#0088c0;
}
.attention_main_lie_1 .attention_main_lie_money{
	font-size:12px;
	line-height:30px;
	font-family:"Arial Black";
	color:#ff4500;
	margin-left:15px;
}
.attention_main_lie_1 .attention_main_lie_jion{
	width:200px;
	margin:0 auto 10px;
	overflow:hidden;
	font-size:12px;
	line-height:20px;
}
.attention_main_lie_jion a{
	margin-right:6px;
	float:right;
	font-size:12px;
	text-align:center;
	line-height:20px;
	width:76px;
	height:20px;
	border:1px solid #e6e6e6;
	cursor:pointer;
}
.attention_main_lie_jion a:hover{
	border:1px solid #ff4500;
	color:#ff4500;
}
.attention_main_lie_1 .attention_main_lie_jion input{
	display:block;
	float:left;
	margin-top:4px;
	margin-right:3px;
}
.attention_choice_2{
	overflow:hidden;
	width:1030px;
	line-height:60px;
	font-size:12px;
	margin-left:20px;
}
.attention_choice_2 span{
	color:#0088c0;
	cursor:pointer;
}
.attention_pag{
	float:right;
	margin-right:20px;
}

.attention_pag input{
	border:1px solid #999;
	width:40px;
}
.attention_pag a{
	margin-left:10px;
}
