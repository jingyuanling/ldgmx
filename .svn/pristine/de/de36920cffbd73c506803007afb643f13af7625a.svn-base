body{
	background-color: #f3f5f7;
}
.wine_agency_wrap{
	width: 100%;
	padding-bottom: 2rem;
}
.wine_agency_balance{
	padding-bottom: 0.25rem;
	width: 100%;
	padding-top: 0.8rem;
	background-color: #cd222f;
}
.wine_agency_balance p{
	text-align: center;
	color: white;
}
.wine_agency_balance .wine_agency_balance_p1{
	font-size: 0.28rem;
	margin-top: 0.3rem;
}
.wine_agency_balance .wine_agency_balance_p2{
	font-size: 0.7rem;
	margin-top: 0.2rem;
}
.wine_agency_money_details{
	width: 100%;
	overflow: hidden;
}
.wine_agency_money_details div{
	width: 50%;
	float: left;
	padding: 0.15rem 0;
	background-color: #cd222f;
	box-sizing: border-box;
}
.wine_agency_money_details .left{
	border-right: 1px solid white;
}
.wine_agency_money_details div p{
	text-align: center;
	color: white;
}
.wine_agency_money_details div p:nth-child(1){
	font-size: 0.4rem;
}
.wine_agency_money_details div p:nth-child(2){
	font-size: 0.26rem;
}
.wine_two_btn{
	width: 100%;
	display: flex;
	justify-content: space-around;
	padding: 0.2rem 0;
	margin-bottom: 0.2rem;
	background-color: #f8f8f8;
    border-bottom: 1px solid #d7d7d7;
}
.wine_two_btn>div{
	width: 2.7rem;
	height: 0.8rem;
	text-align: center;
	font-size: 0.3rem;
	line-height: 0.8rem;
	border-radius: 0.4em;
	color: white;
}
.wine_two_btn .wine_withdrawal_btn{
	background-color: #e3335b;
}
.wine_two_btn .wine_commission_btn{
	background-color: #ffa21d;
}
.wine_three_div{
	overflow: hidden;
	padding: 0.2rem 0.2rem;
	font-size: 0.3rem;
	background-color: #f8f8f8;
	color: #0c0c0c;
	border-top: 1px solid #d7d7d7;
}
.wine_agency_wrap .wine_members{
	border-bottom: 1px solid #d7d7d7;
}
.wine_three_div .wine_gt,.wine_percent{
	float: right;
}
.wine_percent{
	margin-right: 0.2em;
}
.wine_three_div .wine_gt{
	font-weight: 700;
}
.wine_three_div span img{
	display: inline-block;
	margin-right: 0.5em;
	height: 1.2em;
}
