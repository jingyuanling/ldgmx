.wine-wrap{
	height: 100%;
	padding-top: 1.05rem;
	overflow-y: scroll;
	position: relative;
}

.add-bank-card{
	width: 100%;
	padding: 0 0.37rem;
	position: absolute;
    bottom: 0;
}
.add-bank-card .text-center{
	width: 100%;
    height: 0.95rem;
    border: 0.02rem solid #d10000;
    color: #d10000;
    line-height: 0.95rem;
    font-size: 0.3124rem;
    border-radius: 0.08rem;
}
.add-bank-card .text-center img{
	position: relative;
	top: -0.03rem;
}
