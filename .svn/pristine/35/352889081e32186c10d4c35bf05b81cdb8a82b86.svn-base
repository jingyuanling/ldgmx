body{
	background-color: #f3f5f7;
}
#wine_cart_notEmptyCart{
	padding-top: 39px;
}
.shop-group {
	margin: 0;
	padding: 0;
	background: #f0f2f5;
	height: 100%;
}
.shop-group .shop-group-item {
	margin-bottom: 10px;
	background: white;
}
.shop-group-item .shop-title {
	height: 35px;
	line-height: 35px;
	border-width: 1px 0 1px 0;
}
.shop-title .item {
	margin-left: 40px;
	height: 35px;
	line-height: 35px;
}
.customize-shtit.shop-title .item {
	position: relative;
}
.cart-checkbox {
	display: block;
	width: 20px;
	height: 20px;
	margin: 0 auto;
	/*background: url(../img/shp-cart-icon-sprites.png?a=3) no-repeat 0 0;*/
	background-size: 50px 100px;
}
.check-wrapper-unit {
	margin-top: -10px;
}
.check-wrapper .cart-checkbox {
	width: 20px;
	height: 20px;
}
.shop-title-content {
	width: 100%;
	height: 35px;
	line-height: 35px;
}
.shop-title-name {
	font-size: 13px;
	color: #252525;
	margin-top: 1px;
}
.shop-title-detail {
	color: #f15353;
	font-size: 12px;
}
.shp-cart-list li, .shp-cart-promotion-list li {
	padding-bottom: 10px;
	border-width: 0 0 1px 0;
}
.shp-cart-list li {
	min-height: 70px;
}
.shp-cart-conditions-link, .shp-cart-conditions-span {
	display: block;
	color: #999;
	font-size: 12px;
	min-height: 1.4em;
	margin-right: 15px;
	margin-top: 12px;
	padding-bottom: 2px;
}
.shp-cart-list .items, .shp-cart-conditions-span, .shp-cart-item-abstract, .shp-cart-conditions-link, .shp-cart-list .diver-hr-dashed, .shp-cart-list .diver-hr-solid {
	margin-left: 40px;
}
.shp-cart-conditions-link {
	background: url('../img/icon-shp-cart-arrow-right.png') no-repeat;
	background-size: 6px 10px;
	background-position: right 2px;
}
.icon-condition, .icon-exchange {
	background: #f24b48;
	color: #fff;
	padding: 0;
	width: 40px;
	height: 14px;
	overflow: hidden;
	text-align: center;
	display: inline-block;
	font-size: 12px;
	line-height: 1.2em;
	vertical-align: top;
}
.shp-cart-conditions-link .icon-condition, .shp-cart-conditions-span .icon-condition {
	position: absolute;
}
.condition-description {
	width: auto;
	padding-left: 50px;
	line-height: 1.2em;
	margin-right: 30px;
	padding-top: 0;
	padding-right: 12px;
	display: table-cell;
	vertical-align: top;
}
.diver-hr-dashed {
	height: 1px;
	overflow: hidden;
	clear: both;
	border-width: 0 0 1px 0;
}
.clear {
	clear: both;
}
.clear:after {
	content: ".";
	display: block;
	height: 0;
	clear: both;
	visibility: hidden;
}
.items {
	text-align: left;
	padding-top: 10px;
}
.check-wrapper {
	height: 20px;
	width: 20px;
	margin-left: -29px;
	margin-top: 19px;
	display: block;
	position: absolute;
}
.cart-checkbox.checked {
	border: 1px solid #d9333f;
}
.shp-cart-item-core {
	min-height: 60px;
}
.shop-cart-display.shp-cart-item-core {
	overflow: hidden;
	position: relative;
}
.cart-product-cell-1, .cart-product-cell-additional {
	width: 70px;
	height: 70px;
	display: block;
	float: left;
}
.shop-cart-display .cart-product-cell-1 {
	width: auto;
	height: auto;
	position: absolute;
	top: 0;
	left: 0;
}
.cart-photo-thumb {
	width: 59px;
	height: 59px;
}
.shop-cart-display .cart-product-cell-1 .cart-photo-thumb {
	width: 79px;
	height: 79px;
}
.cart-product-cell-2 {
	min-width: 118px;
	width: 40%;
	float: left;
}
.shop-cart-display .cart-product-cell-2 {
	padding-left: 92px;
	padding-right: 16px;
	width: 100%;
	box-sizing: border-box;
	margin-bottom: 2px;
}
.cart-product-name {
	font-size: 13px;
	line-height: 1.2em;
	color: #666;
	margin-bottom: 5px;
	vertical-align: top;
	height: 2.4em;
}
.shop-cart-display .cart-product-name {
	margin-bottom: 5px;
	height: 30px;
}
.cart-product-name a {
	color: #555;
}
.cart-product-name a span {
	height: 2.3em;
	overflow: hidden;
	text-overflow: ellipsis;
	line-height: 1.2em;
	display: -webkit-box;
	-webkit-line-clamp: 2;
	-webkit-box-orient: vertical;
}
.shop-cart-display .cart-product-name a span {
	height: 31px;
	max-height: 31px;
	font-size: 0.8em;
	line-height: 15px;
	vertical-align: middle;
	display: -webkit-box;
	-webkit-box-pack: center;
	-webkit-box-align: start;
}
.cart-product-cell-3 {
	width: 80px;
	position: absolute;
	right: 0;
	white-space: nowrap;
	margin-right: 10px;
	text-align: right;
	margin-top: -2px;
}
.shop-cart-display .cart-product-cell-3 {
	width: auto;
	position: static;
	text-align: left;
	margin: 0 0 9px 0;
	height: 13px;
	font-size: 13px;
	line-height: 1;
	color: #9e9d9e;
	line-height: 13px;
}
.shp-cart-item-price {
	font-size: 0.6em;
    margin-right: 1.0em;
	margin-top: 4px;
	color: #d9333f;
}
.shop-cart-display .shp-cart-item-price {
	display: inline-block;
	height: 100%;
	vertical-align: text-top;
}
.shop-cart-display .shp-cart-opt {
	height: 25px;
	margin-top: 10px;
}
.quantity-wrapper {
	display: inline-block;
}
.wastebin-container {
	display: inline-block;
	cursor: pointer;
	/*width: 23px;
	height: 23px;*/
	position: absolute;
	right: 0;
	margin-right: 15px;
}
.quantity-decrease, .quantity, .quantity-increase {
	float: left;
	font-size: 14px;
	text-align: center;
}
.quantity-decrease, .quantity-increase {
	background: #fff;
	border: 1px solid #ccc;
	color: #666;
	display: block;
	line-height: 24px;
	width: 32px;
	overflow: hidden;
	text-indent: -200px;
	/*background: url(../img/shp-cart-icon-sprites.png?a=3) no-repeat -17px -21px;*/
	background-size: 50px 100px;
	}
.quantity-decrease {
	-webkit-border-radius: 2px 0 0 2px;
	background-position: 10px -20px;
}
.quantity-decrease.disabled, .quantity-increase.disabled {
	background-color: #e8e8e8;
	color: #999;
	background-position: 8px -46px;
}
.customize-qua .quantity-decrease, .customize-qua .quantity-increase, .customize-qua .quantity {
	line-height: 22px;
	height: 22px;
	width: 30px;
}
.wastebin {
display: inline-block;
cursor: pointer;
}
.wastebin-down {
	font-size: 0.5em;
	display: block;
	margin-top: -3px;
}
.shop-footer {
	height: 30px;
	line-height: 30px;
	margin-top: 10px;
}
.shop-footer .shop-footer-total {
	margin-left: 36px;
	color: #f15353;
	font-size: 15px;
	margin-top: 3px;
	display: inline-block;
}
.shop-footer-total i {
	text-decoration: none;
	font-style: normal;
}
.payment-total-bar {
	width: 100%;
	height: 50px;
	overflow: hidden;
	position: fixed;
	background: #eaedf1;
	bottom: 0;
}
.payment-total-bar-new {
	background-color: RGBA(255,255,255,1)!important;
	height: 57px!important;
	border-top:1px solid #bcbcbf;
	border-bottom:1px solid #bcbcbf;
	min-width: 320px;
	max-width: 640px;
}
.shp-chk {
	width: 38px;
	float: left;
	text-align: center;
	overflow: hidden;
	padding-top: 15px;
}
.shp-chk-new {
	width: 50px!important;
	margin-left: 10px;
	margin-top: 5px;
}
.payment-total-bar span {
	color: red!important;
	font-size: 14px;
	font-weight: 900;
}
.payment-total-bar .cart-checkbox {
	margin-left: 11px;
}
.cart-checkbox-text {
	color: #fff!important;
	opacity: .8;
	display: table-cell;
	vertical-align: middle;
	padding-left: 5px;
	font-size: 12px!important;
}
.shp-chk-new .cart-checkbox {
display: table-cell!important;
}
.shp-cart-info {
	display: block;
	float: left;
	min-width: 200px;
	margin-top: 3px;
}
.shp-cart-info-new {
	margin-left: 11px;
}
.shp-cart-total {
	margin-top: 6px;
	display: block;
}
.payment-total-bar strong, .shp-cart-total span {
	color: #e4393c;
	font-size: 14px;
	font-weight: normal;
}
.shp-cart-info-new .shp-cart-total, .shp-cart-total span {
	color: #9c9a9c!important;
	font-size: 12px;
	margin-top: 7px!important;
}
.sale-off-new {
	padding-top: 5px;
	display: inline-block;
	color: #fff!important;
	opacity: .8;
}
.sale-off-new span {
	color: #fff!important;
	opacity: .8;
}
.btn-right-block {
	display: block;
	width: 70px;
	padding: 0 8px;
	line-height: 50px;
	text-align: center;
	background: #e4393c;
	color: #fff;
	font-size: 14px;
	float: right;
	font-weight: normal;
	position: absolute;
	right: 0;
}
.btn-right-block-new {
	width: 100px!important;
	line-height: 60px!important;
	background-color: #f15353!important;
	font-size: 18px!important;
}
.btn-right-block span {
	color: #fff;
	font-size: 14px;
}
.btn-right-block-new span {
	font-size: 13px;
}
