*{
	margin: 0;
	padding: 0;
}
.PC_box_alert{
	height: 100%;
	width: 100%;
	position: fixed;
	background-color:rgba(0,0,0,0.5);
	top: 0;
	left: 0;
}
.PC_box_content{
	width: 732px;
	height: 550px;
	position: absolute;
	top: 50%;
	left: 50%;	
	margin-left: -366px;
	margin-top: -275px;
}
.PC_box_content>img{
	width: 100%;
	height: 100%;
}
.PC_box_delete{
	width: 40px;
	height: 40px;
	position: absolute;
	right: 0;
	top: 50px;
	z-index: 10;
}
.PC_box_delete>img{
	width: 100%;
	height: 100%;
}
.get_box_hand{
	width: 30px;
	position: absolute;
	bottom: 35px;
	left: 50%;
	margin-left: -15px;
	animation: bounce 0.8s infinite;
}
.get_box_hand>img{
	width: 100%;
}
