.double-debt-wrap::-webkit-scrollbar,.pwd-alert-gray::-webkit-scrollbar{
	display: none;
}
.double-debt-wrap{
	width: 100%;
	height: 100%;
	overflow-y: scroll;
	position: relative;
	background-color: #ff1f53;
	font-size: 0;
}
.double-debt-PC{
	width: 100%;
	background: url(../img/get-debt-big-bg.png) 0 0 no-repeat;
	background-size: 100% auto;
}
.double-debt-wrap .wrap-btm-img{
	width: 100%;
}
.double-debt-wrap .wrap-btm-img img{
	width: 100%;
}
.animate_enve{
	width: 0;
	height: 0;
	position: absolute;
	top: 50%;
	left: 50%;
	z-index: 9;
}
.animate_enve_active{
	animation:mymove 5s;
	-webkit-animation:mymove 5s; /*Safari and Chrome*/
}
.double-debt-banner{
	width: 100%;
}
.double-debt-banner img{
	width: 100%;
}
.double-debt-content{
    width: 1100px;
    margin: 0 auto;
}
.amount_tit{
	text-align: center;
	font-size: 47.42px;
	color: white;
	line-height: 75px;
	font-family:'jingzhizongyiti10fb7d7fd01b4cb';
}
.double-cont-ul{
	overflow: hidden;
	padding-top: 20px;
}
.double-cont-ul li{
	width: 370px;
    height: 130px;
	margin-bottom: 35px;
    padding-top: 20px;
    background: url(../img/debet-bg1.png) 0 0 no-repeat;
	background-size: 100% 100%;
}
.double-cont-ul li p span{
	font-family: "微软雅黑";
	font-weight: bold;
}
.double-cont-ul li:nth-child(2n+1){
	float: left;
	margin-left: 100px;
}
.double-cont-ul li:nth-child(2n){
	float: right;
	margin-right: 100px;
}
.double-cont-ul .red{
	background: url(../img/debet-bg2.png) 0 0 no-repeat;
	background-size: 100% 100%;
}
.double-cont-ul li:hover{
	cursor: pointer;
	background: url(../img/debet-bg2.png) 0 0 no-repeat;
	background-size: 100% 100%;
	
}
.double-cont-ul li p{
	font-size: 0.32rem;
	text-align: center;
	color: white;
	font-family:'jingzhizongyiti10fb7d7fd01b4cb';
}
.ruls-wrap{
	margin-top: 100px;
	width: 100%;
	font-family: "微软雅黑";
	background: url(../img/ruls-bg.png) 0 0 no-repeat;
	background-size: 100% 100%;
	padding-top: 120px;
	padding-bottom: 60px;
}
.ruls-wrap h1{
	font-size: 52.16px;
	color: #333333;
	text-align: center;
}
.ruls-wrap p{
	font-size: 23.83px;
	color: #666666;
	text-align: center;
	line-height: 45px;
}
/*弹出框*/
.pwd-alert-gray{
	width: 100%;
	height: 100%;
	background-color: rgba(0,0,0,0.5);
	position: fixed;
	top: 0;
	left: 0;
	overflow: scroll;
	font-size: 0;
	display: none;
}
.pwd-alert{
	width: 671px;
	height: 464px;
	background: url(../img/inp-pwd-bg.png) 0 0 no-repeat;
	margin: 0 auto;
	position: absolute;
	top: 50%;
	left: 50%;
	margin-left: -335px;
	margin-top: -232px;
}
.delete_pwd_alert{
    width: 32px;
    height: 32px;
    position: absolute;
    top: -3px;
    right: 43px;
}
.pwd-alert-tit{
	text-align: center;
	color: #ef2f51;
	padding-top: 30px;
}
.pwd-alert-tit p{
	font-size: 36.39px;
    line-height: 47px;
    font-family:'jingzhizongyiti10fb7d7fd01b4cb';
}
.pwd-alert-tit p span{
	font-family: "微软雅黑";
	font-weight: bold;
}
#inp_pwd{
	display: none;
}
.pwd-div-content{
    width: 540px;
    height: 300px;
    margin-left: 75px;
    padding-top: 12px;
}
.pwd_inp_ul{
	width: 380px;
	margin: 0 auto;
	overflow: hidden;
	border: 2px solid #a46309;	
	border-left: none;
}
.pwd_inp_ul li{
	width: 63px;
	height: 60px;
	color: #333333;
	float: left;
	font-size: 60px;
	text-align: center;
	line-height: 85px;
	border-left: 2px solid #a46309;
}
.under_inp_text{
	display: block;
    width: 380px;
    margin: 0 auto;
    overflow: hidden;
    line-height: 40px;
}
.under_inp_text span{
	font-size: 18px;
	font-weight: lighter;
    font-family: "微软雅黑";
}
.under_inp_text .pwd_forget{
	float: right;
	color: #1974b5;
}
.under_inp_text .pwd_error{
	color: #FF1F53;
	display: none;
}
.pwd-div-content .use-time{
	font-size: 20.62px;
	text-align: center;
	color: #a46309;
	font-weight: lighter;
	font-family: "微软雅黑";
	line-height: 45px;
}
.sure-pay{
    width: 226px;
    height: 55px;
    background-color: #fed865;
    border: 2px solid #ebd133;
    margin: 72px auto 0;
    border-radius: 10px;
    font-size: 32.17px;
    color: #a46309;
    text-align: center;
    line-height: 55px;
    font-family: "微软雅黑";
}
.fly-red-box{
	width: 1200px;
	/*height: 100%;*/
	height: 0;
	position: fixed;
	top: 0;
	left: 50%;
	margin-left: -600px;
	z-index: 99;
}
.fly-red-box img{
	width: 0;
	height: 0;
	position: absolute;
	top: 50%;
	left: 50%;
	margin-left: -249px;
	margin-top: -182.5px;
}
.fly_red_box_active{
	animation:boxmove 5s;
	-webkit-animation:boxmove 5s;
}
.fly_red_img_active{
	animation:birdmove 5s;
	-webkit-animation:birdmove 5s; /*Safari and Chrome*/
}
@keyframes boxmove{
	0%{
		height: 100%;
	}
	90%{
		height: 100%;
	}
	100%{
		height: 0;
	}
}
/*Safari and Chrome*/
@-webkit-keyframes boxmove {
	0%{
		height: 100%;
	}
	90%{
		height: 100%;
	}
	100%{
		height: 0;
	}
}
@keyframes birdmove
{
	0%{
		width: 0;
		height: 0;
	}
	40%{
		width: 498px;
		height: 365px;
		left: 50%;
		top: 50%;
		margin-left: -249px;
		margin-top: -182.5px;
		-webkit-transform: rotatez(0);
		-moz-transform: rotatez(0);
		-ms-transform: rotatez(0);
		-o-transform: rotatez(0);
		transform: rotatez(0);
	}
	100%{
		width: 0;
		height: 0;
		left: 850px;
		top: 20px;
		-webkit-transform: rotatez(80deg);
		-moz-transform: rotatez(80deg);
		-ms-transform: rotatez(80deg);
		-o-transform: rotatez(80deg);
		transform: rotatez(80deg);
	}
}
/*Safari and Chrome*/
@-webkit-keyframes birdmove 
{
	0%{
		width: 0;
		height: 0;
	}
	40%{
		width: 5.42rem;
		height: 3.98rem;
		left: 50%;
		top: 50%;
		margin-left: -2.71rem;
		margin-top: -1.99rem;
		-webkit-transform: rotatez(0);
		-moz-transform: rotatez(0);
		-ms-transform: rotatez(0);
		-o-transform: rotatez(0);
		transform: rotatez(0);
	}
	100%{
		width: 0;
		height: 0;
		left: 7.095rem;
		top: 0.44rem;
		-webkit-transform: rotatez(80deg);
		-moz-transform: rotatez(80deg);
		-ms-transform: rotatez(80deg);
		-o-transform: rotatez(80deg);
		transform: rotatez(80deg);
	}
}
