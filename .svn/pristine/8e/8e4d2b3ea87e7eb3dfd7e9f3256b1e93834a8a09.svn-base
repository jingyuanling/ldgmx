.zhifu-box{
    width: 100%;
    background-color:#f0f0f2;
    padding-top: 39px;
}
.main-top{
    width: 100%;
    height: 39px;
    background-color: #548e11;
}
.main-top img{
    position: absolute;
    float: left;
    width: 10px;
    margin-top: 10px;
    margin-left: 13px;
}
.main-top img.serch{
    position: relative;
    float: right;
    width: 18px;
    margin-top: -28px;
    margin-right: 13px;

}
.main-top p{
    width: 100%;
    letter-spacing: 2px;
    font-size: 14px;
    color: #fff;
    text-align: center;
    line-height:39px;
}
.yonghu{
    margin-top:7px;
    width: 100%;
    padding: 10px 20px;
    background-color: #fff;
    line-height: 30px;
    font-size: 14px;
    color: #000;
}
.yonghu p.dizhi{
    color: #827c82;
    line-height: 20px;
}
.yonghu p.right{
    float: right;
     margin-top: -20px;
     color: #000;
     font-size: 25px;
     margin-left: 10px;
}
.select{
    padding: 10px 20px;
    width: 100%;
    margin-top: 10px;
    height: 110px;
    background-color: #fff;
    line-height: 33px;
    font-size: 14px;
}
.select p{
    overflow: hidden;
}
.select p span{
    float: left;
}
.select p img{
    width: 22px;
    vertical-align: top;
    margin-top: 5px;
    margin-right: 20px;
}
.select p span.money{
    color: #827c82;
    margin-left: 20px;
}
.select p span.xuan{
    width: 16px;
    height: 16px;
    border-radius: 50%;
    border:1px solid #827c82;
    float: right;
    /*选中状态*/
    /*background-color: #d9333f;*/
}
.active{
	background-color: #d9333f;
}
.youhui{
    height: 40px;
    line-height: 20px;
}
.youhui p span img{
    margin-top: 1px;
}
.youhui p span.money{
    height: 20px;
    padding:8px;
    background-color: #e63c2c;
    color: #fff;
    line-height: 6px;
}
.jiesuan{
    padding: 10px 20px;
    width: 100%;
    margin-top: 10px;
    height: 120px;
    background-color: #fff;
    line-height: 33px;
    font-size: 14px;
}
.jiesuan p{
    width: 100%;
    height: 33px;
}
.jiesuan p.top{
    margin-bottom: 10px;
}
.jiesuan p span{
    float: left;
}
.jiesuan p img{
    width: 22px;
    vertical-align: top;
    margin-top: 5px;
    margin-right: 20px;
}
.jiesuan p span.geshu{
    color: #827c82;
   float: right;
   margin-right: 20px;
}
.jiesuan p span.qian{
    float: right;
}
.fukuan{
    position: absolute;
    bottom: 0;
    max-width: 640px;
    min-width: 320px;
    width: 100%;
    height: 40px;
    background-color: #fff;
    border-top:1px solid #d7d7d7;
    border-bottom: 1px solid #d7d7d7;
}
.fukuan p{
    font-size: 16px;
    width: 50%;
    float: left;
    line-height: 38px;
    text-align: center;
    color: #e84e40;
}
.fukuan p.anniu{
    background-color: #e84e40;
    color: #fff;

}
.wine_HB_num{
	font-size: 12px;
	background-color: #d9333f;
	padding: 2px 3px;
	color: white;
	border-radius: 3px;
	margin-left: 5px;
}
/*支付成功样式*/
.wine_pay_success_wrap{
	width: 100%;
	height: 100%;
	padding-top: 39px;
	background-color: #f3f5f7;
}
.wine_pay_success_logo{
	width: 100%;
	border-bottom: 1px solid #d7d7d7;
	padding: 2em 0;
}
.wine_pay_success_logo img{
	border-radius: 50%;
	width: 4em;
	margin: 0 auto;
	margin-bottom: 0.5em;
}
.wine_pay_success_logo p{
	text-align: center;
	font-size: 1.5em;
}
.wine_pay_success_details{
	width: 100%;
	padding: 2em 0;
}
.wine_pay_success_details p{
	text-align: center;
	font-size: 1.5em;
}
.wine_pay_success_details .wine_see_btn{
	background-color: #ffa21d;
	color: white;
	padding: 0.3em 0.8em;
	border-radius: 0.3em;
}
.wine_pay_success_details .wine_return_index_btn{
	background-color: #d9333f;
	color: white;
	padding: 0.3em 0.8em;
	border-radius: 0.3em;
}
.wine_pay_success_text_p1{
	margin-bottom: 0.5em;
}
.wine_pay_success_text_p3{
	margin-top: 1.5em;
}
/*优惠券弹出框*/
.ZF_coupon_box_wrap{
	width: 100%;
	height: 100%;
	max-width: 640px;
	min-width: 320px;
	background-color: rgba(0,0,0,0.5);
	position: absolute;
	top: 0;
	display: none;
}
.ZF_coupon_box{
	width: 100%;
	position: absolute;
	bottom: -100%;
	background-color: #f8f8f8;
	padding: 0 0.5em;
	box-sizing: border-box;
}
.ZF_coupon_box_logo{
	text-align: center;
	padding: 0.5em 0;
	border-bottom: 1px solid #d7d7d7;
}
.ZF_coupon_box_logo img{
	display:inline-block;
	margin-right: 0.5em;
	height: 1em;
}
.ZF_coupon_box_logo span{
	color: #d9333f;
	font-size: 1.0em;
}
.ZF_coupon_box_title{
	width: 100%;
	border-bottom: 1px solid #d7d7d7;
	overflow: hidden;
}
.ZF_coupon_box_titleL,.ZF_coupon_box_titleR{
	float: left;
	width: 50%;
	text-align: center;
	font-size: 1.0em;
	padding: 0.5em 0;
}
.ZF_voucher_details li,.ZF_voucher_details2 li{
	margin-top: 1em;
}
.ZF_voucher_details li span{
	display: inline-block;
	width: 1em;
	height: 1em;
	border: 1px solid #d9333f;
	border-radius: 0.2em;
	margin: 0 1em;
}
.ZF_voucher_details li p,.ZF_voucher_details2 li p{
	padding-left: 3em;
	box-sizing: border-box;
	margin-top: 0.4em;
	color: #c5c5c5;
}
.ZF_voucher_details,.ZF_voucher_details2{
	height: 7em;
	overflow-y: scroll;
}
.ZF_voucher_details2{
	display: none;
}
.ZF_voucher_details2{
	color: #c5c5c5;
}
.ZF_voucher_details2 li span{
	display: inline-block;
	width: 1em;
	height: 1em;
	border: 1px solid #c5c5c5;
	border-radius: 0.2em;
	margin: 0 1em;
}
