body{
	background-color: #f3f5f7;
}
.wine_agent_order_wrap{
	padding-top: 0.8rem;
}
.wine_commission_wrap{
	width: 100%;
	height: 100%;
	padding-top: 1.58rem;
	background-color: #f3f5f7;
}
.wine_commission_title{
	width: 100%;
	max-width: 640px;
	min-width: 320px;
	overflow: hidden;
	padding: 0.2rem;
	box-sizing: border-box;
	border-bottom: 0.01rem solid #d7d7d7;
	background-color: #f8f8f8;
	position: fixed;
	top: 0.8rem;
	z-index: 9;
}
.wine_commission_title select{
	float: left;
	font-size: 0.28rem;
	border: 0;
	outline: none;
	background-color: #f3f5f7;
}
.wine_commission_title_span{
	font-size: 0.28rem;
	display: inline-block;
	float: right;
}
.wine_commission_title_span img{
	display: inline-block;
	margin-right: 0.2em;
}
.wine_commission_details{
	margin-top: 0.19rem;
	border-top: 1px solid #d7d7d7;
	border-bottom: 1px solid #d7d7d7;
	overflow: hidden;
	background-color: #f8f8f8;
}
.wine_commission_details p{
	overflow: hidden;
	padding-top: 0.1rem;
	position: relative;
}
.wine_commission_details p span{
	font-size: 0.3rem;
	float: left;
}
.commission_details_name{
	display: inline-block;
	background-color: #d9333f;
	padding: 0.08rem 0.4rem;
	border-radius: 0 0.15rem 0.15rem 0;
	color: white;
	margin-right: 0.5em;
}
.commission_details_num{
	font-size: 0.24rem!important;
	color: #bfbebe;
	position: absolute;
    bottom: 0;
}
.wine_commission_details p .commission_details_time{
	font-size: 0.24rem!important;
	color: #bfbebe;
	margin-top: 0.2em;
	display: inline-block;
	padding-right: 0.5em;
	position: absolute;
	bottom: 0;
	right: 0.2rem;
}
.wine_commission_money_wrap{
	display: flex;
	justify-content: space-around;
}
.wine_commission_money{
	float: left;
	width: 2.1rem;
	background-color: #f8f8f8;
}
.wine_commission_money p{
	text-align: center;
	font-size: 0.5rem;
	color: #d9333f;
	padding: 0.25rem 0;
}
.wine_commission_money span{
	display: block;
	width: 100%;
	color: #bfbebe;
	text-align: center;
	font-size: 0.28rem;
	padding-bottom: 0.27rem;
}
/*代理人详情*/
.agency_man_details_wrap{
	padding-top: 0.8rem;
	border-bottom: 1px solid #d7d7d7;
	padding-bottom: 1rem;
}
.agency_man_details_top{
	padding: 0.4rem 0.2rem 0.5rem;
	background-color: #cd222f;
	overflow: hidden;
}
.agency_man_photo{
	width: 1rem;
	height: 1rem;
}
.agency_man_photo img{
	width: 100%;
	height: 100%;
}
.agency_man_photo,.agency_man_photo img{
	border-radius: 50%;
}
.agency_man_details_topL{
	width: 3.65rem;
	float: left;
	overflow: hidden;
	border-right: 1px solid white;
	box-sizing: border-box;
}
.agency_man_details_topR{
	width: 2.35rem;
	float: right;
}
.agency_man_photo,.agency_man_photo_text{
	float: left;
	color: white;
}
.agency_man_photo_text{
	width: 2.6rem;
	padding-left: 0.3rem;
}
.agency_man_photo_text_name,.agency_man_details_topR_p1{
	font-size: 0.34rem;
	margin-bottom: 0.15rem;
}
.agency_man_photo_text_p2,.agency_man_details_topR_p2{
	font-size: 0.26rem;
}
.agency_man_details_topR p{
	text-align: center;
	color: white;
}
.order_entrance{
	display: flex;
	overflow: hidden;
	border-bottom: 1px solid #d7d7d7;
}
.wine_last_order .wine_last_orderP1{
	overflow: hidden;
	background-color: #f8f8f8;
	border-bottom: 0.01rem solid #d7d7d7;
	border-top: 0.01rem solid #d7d7d7;
	padding: 0.2rem 0.2rem;
	box-sizing: border-box;
	margin-top: 0.2rem;
}
.wine_last_order .wine_last_orderP1 span:nth-child(1){
	float: left;
	font-size: 0.3rem;
}
.wine_last_order .wine_last_orderP1 span:nth-child(2){
	float: right;
	font-size: 0.28rem;
	color: #bfbfbf;
}
.wine_last_order .wine_last_orderP1 .right_into{
	display: inline-block;
	height: 0.28rem;
    margin-left: 0.15rem;
    position: relative;
    top: -0.02rem;
}
.wine_order_details_entrance{
	padding: 0 0.2rem;
	box-sizing: border-box;
	background-color: #f8f8f8;
	border-bottom: 1px solid #d7d7d7;
	border-top: 1px solid #d7d7d7;
	margin-bottom: 0.2rem;
}
.wine_order_details_entrance .wine_order_details_entrance1{
	padding: 0.25rem 0;
	overflow: hidden;
	font-size: 0.3rem;
}
.wine_order_details_entrance .wine_order_details_entrance1 span:nth-child(1){
	float: left;
	color: black;
}
.wine_order_details_entrance .wine_order_details_entrance1 span:nth-child(2){
	float: right;
	color: #bfbfbf;
}
.wine_order_mes .good_photo{
	float: left;
	margin-right: 0.15rem;
	width: 1.05rem;
	height: 1.05rem;
}
.wine_order_mes .good_describe{
	width: 100%;
	font-size: 0.26rem;
}
.wine_order_mes .good_describe span{
	float: right;
	margin-top: 2em;
	color: #999999;
}
.wine_order_mes .good_describe span a{
	color: #df5a64;
}
.good_photo img{
	width: 100%;
	border: 0.01rem solid #d7d7d7;
}
.wine_order_mes{
	overflow: hidden;
	border-bottom: 1px solid #d7d7d7;
	background-color: #f8f8f8;
	padding-bottom: 0.2rem;
}
.agency_man_commission_reward{
	text-align: center;
	box-sizing: border-box;
	font-size: 0.3rem;
	overflow: hidden;
	padding: 0.2rem;
	
}
.agency_man_commission_reward span:nth-child(1){
	float: left;
}
.agency_man_commission_reward span:nth-child(2){
	color: #df5a64;
}

/*弹出框*/
.wine_time_alert_wrap{
	position: fixed;
	top: 0;
	width: 100%;
	min-width: 320px;
	max-width: 640px;
	height: 100%;
	background-color: rgba(0,0,0,0.5);
	display: none;
}
.wine_time_alert{
	width: 90%;
	position: absolute;
	margin-left: 5%;
	border: 1px solid red;
}
.wine_time_alert .wine_time_alertS,.wine_time_alert .wine_time_alertX{
	width: 100%;
}
.wine_time_alertX{
	width: 100%;
}
.wine_time_alertX .wine_time_alertX_L,.wine_time_alertX .wine_time_alertX_R{
	width: 50%;
	float: left;
	text-align: center;
	line-height: 3em;
	font-size: 1.2em;
	border-top: 1px solid #d7d7d7;
}
.wine_time_alertX .wine_time_alertX_L{
	background-color: #FFFFFF;
	color: #000000;
	border-radius: 0 0 0 5px;
}
.wine_time_alertX .wine_time_alertX_R{
	background-color: #ff4747;
	color: #FFFFFF;
	border-radius: 0 0 5px 0;
}
.wine_time_alertS{
	width: 100%;
	background-color: #ffffff;
	border-radius: 5px 5px 0 0;
}
