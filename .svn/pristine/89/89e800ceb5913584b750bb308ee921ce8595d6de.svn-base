@charset "utf-8";
html,body{
    width: 100%;
    min-width: 320px;
}
body{
    font-family: Tahoma,Arial,"Droid Sans","Helvetica Neue","微软雅黑","Heiti SC",sans-self;
    
}
.qianback{
	width: 100%;
	height: 102px;
	background-image:url(../img/qianback.png);
}
.touxiang img{
	width: 78px;
	height: 78px;
	margin-top: 15px;
	margin-left: 18px;
	float: left;
}
.touxiang p{
	float: left;
	font-size: 14px;
	margin-top: 22px;
	margin-left: 9px;
}
.qiandao img{
	width: 70px;
	height: 26px;
	float: right;
	margin-right:30px;
	margin-top: 45px; 
}
.guanzhu{
	width: 100%;
	height: 39px;
	background-color: #fff;
}
.guanzhu ul li{
	width: 33.3%;
	height: 39px;
	line-height: 17px;
	font-size: 12px;
	float: left;
	text-align: center;
	border-top: 1px solid #dadada;
	border-right: 1px solid #dadada;
	border-bottom: 1px solid #dadada;
}
.zhongjian{
	width: 100%;
	height: 5px;
	background-color: #e5e5e5;
}
.cont{
	width: 100%;
	height: 160px;
	background-color: #fff;
}
.cont ul li{
	width: 25%;
	height: 80px;
	font-size: 12px;
	float: left;
	text-align: center;
	border-bottom: 1px solid #dadada;
	border-top: 1px solid #dadada;
	border-right: 1px solid #dadada;
}
.cont img{
	width: 59px;
	height: 56px;
	text-align: center;
	margin-top: 3px;
}
.cont ul.list2 li{
	border-top: none;
	border-bottom: none;
}
.list2 li .tubiao{
	width: 15px;
	height: 15px;
	border-radius: 50%;
	background-color: red;
	color: #fff;
	font-size: 12px;
	text-align: center;
	position: absolute;
	margin-top: 5px;
	margin-left: -8px;
}
.zhongjian2{
	width: 100%;
	height: 7px;
	background-color: #e5e5e5;
}
