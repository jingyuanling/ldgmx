*{
	padding: 0;
	margin: 0;
}
.out_small_btn{
	position: absolute;
	right: 0;
	top: 0;
	display: none;
}
.do_task_all{
	width: 830px;
	margin: 0 auto;
	position: relative;
}
.do_task_wrap{
	width: 100%;
	height: 590px;
	background: url(../img/rw_bg.png) 0 0 no-repeat;
	background-size: 100% 100%;
	padding-top: 33px;
	box-sizing: border-box;
}
.do_task_title{
	margin: 0 auto;
	width: 465px;
	height: 40px;
	margin-bottom: 30px;
}
.do_task_title img{
	width: 100%;
	height: 100%;
}
.do_task_rules{
	width: 100%;
	height: 210px;
	background: url(../img/ruls_bg.png) 0 0 no-repeat;
	background-size: 100% 100%;
	margin-top: -29px;
	padding: 75px 35px;
	box-sizing: border-box;
}
.do_task_rules .rules_span{
	margin-left: 20px;
	font-size: 20.5px;
	color: white;
	font-family: "微软雅黑";
}
.rules_p{
	font-size: 20px;
	color: white;
	font-family: "微软雅黑";
	padding-top: 10px;
}
.do_task_wrap .do_task_wrap_line1,
.do_task_wrap .do_task_wrap_line2{
	width: 100%;
	height: 195px;
	padding-left: 60px;
	box-sizing: border-box;
	margin-bottom: 20px;
}
.do_task_wrap .do_task_wrap_line1 .line1_days,
.do_task_wrap .do_task_wrap_line2 .line2_days{
	width: 148px;
	height: 100%;
	background-size: 100% 100%;
	float: left;
	position: relative;
}
/*每天的背景图*/
.line1_one_day{
	background: url(../img/one_day_bg.png) 0 0 no-repeat;
}
.line1_two_day{
	background: url(../img/two_day_bg.png) 0 0 no-repeat;
}
.line1_three_day{
	background: url(../img/three_day_bg.png) 0 0 no-repeat;
}
.line1_four_day{
	background: url(../img/four_day_bg.png) 0 0 no-repeat;
}
.line2_one_day{
	background: url(../img/five_day_bg.png) 0 0 no-repeat;
}
.line2_two_day{
	background: url(../img/six_day_bg.png) 0 0 no-repeat;
}
.line2_three_day{
	background: url(../img/seven_day_bg.png) 0 0 no-repeat;
}
.line2_four_day{
	background: url(../img/end_price.png) 0 0 no-repeat;
}
.do_task_wrap .line_day_right{
	margin-left: 40px;
}
.do_task_wrap .do_task_bottom_text{
	width: 100%;
	text-align: left;
	padding-left: 40px;
	box-sizing: border-box;
	position: relative;
}
.do_task_wrap .do_task_bottom_text span{
	font-size: 22px;
	color: white;
	font-family: "微软雅黑";
}
.do_task_bottom_text .bottom_title_span{
	display: inline-block;
	margin-right: 90px;
}
.mouse_alert{
	position: absolute;
	top: -60px;
	display: none;
}
/*剪刀样式*/
.scissors_hide{
	margin-top: 5px;
	margin-left: -20px;
	display: none;
}
.scissors_show{
	display: block;
}
/*每天底部的按钮*/
.all_day_btn{
	width: 130px;
	height: 37px;
	margin: 0 auto;
	position: absolute;
	bottom: 10px;
	left: 50%;
	margin-left: -65px;
}
.do_task_wrap .get_btn{
	background-image: url(../img/get_btn.png);
}
.do_task_wrap .not_start{
	background-image: url(../img/not_start.png);
}
.do_task_wrap .complete_btn{
	background-image: url(../img/complete_btn.png);
}
.do_task_wrap .makeup_btn{
	background-image: url(../img/makeup_btn.png);
	z-index: 10;
}
/*灰层*/
.gray_layer{
	width: 130px;
	height: 158px;
	background-color: rgba(0,0,0,0.5);
	position: absolute;
	bottom: 10px;
	left: 50%;
	margin-left: -65px;
	border-radius: 3px;
}
/*带感叹号的灰层*/
.sigh_gray_layer{
	width: 130px;
	height: 158px;
	background-color: rgba(0,0,0,0.5);
	position: absolute;
	bottom: 10px;
	left: 50%;
	margin-left: -65px;
	border-radius: 3px;
	text-align: center;
}
.sigh{
	width: 20px;
	margin-top: 35px;
	
}
/*弥补灰层移入图片样式*/
.mouse_alert_sign{
	width: 200px;
	position: absolute;
	left: -26px;
	top: -20px;
	display: none;
}
/*弥补弹窗*/
.makeup_alert{
	width: 262px;
	height: 176px;
	position: absolute;
	z-index: 20;
	top: 50%;
	left: 50%;
	margin-top: -88px;
	margin-left: -131px;
	display: none;
}
/*弥补成功弹窗*/
.success_alert{
	background-image: url(../img/success_fill.png);
}
/*弥补失败弹框*/
.fail_alert{
	background-image: url(../img/fail_fill.png);
}
.sure_btn{
	width: 70px;
	height: 25px;
	background-color: white;
	color: #ee5952;
	font-size: 14px;
	text-align: center;
	border-radius: 3px;
	line-height: 25px;
	margin: 0 auto;
	margin-top: 120px;
	font-family: "微软雅黑";
}
/*领取任务弹窗*/
.today_task_alert{
	width: 696px;
	height: 460px;
	background-image: url(../img/today_task_alert.png);
	background-size: 100% 100%;
	position: absolute;
	z-index: 30;
	top: 50%;
	left: 50%;
	margin-top: -200px;
	margin-left: -348px;
	padding: 0 20px;
	box-sizing: border-box;
	display: none;
}
.today_task_alert_delete,.do_task_all_delete{
	width: 45px;
	height: 45px;
	background-image: url(../img/task_delete.png);
	background-size: 100% 100%;
	position: absolute;
	right: -90px;
	top: 0;
}
.today_task_alert_title{
	width: 770px;
	height: 65px;
	background-image: url(../img/tody_task_title.png);
	background-size: 100% 100%;
	margin-left: -60px;
	margin-top: -35px;
}
.today_task_alert_content{
	width: 100%;
	height: 385px;
	margin-top: 25px;
	overflow: hidden;
}
.today_task_alert_content_left{
	float: left;
	width: 280px;
	height: 100%;
	background-color: white;
}
.today_task_alert_content_right{
	float: right;
	width: 330px;
	height: 100%;
}
.today_task_right_title{
	width: 330px;
	height: 50px;
	background-image: url(../img/invite_title.png);
	margin-top: 10px;
}
.today_reward_title{
	width: 100%;
	text-align: center;
	font-size: 22px;
	color: white;
	font-family: "微软雅黑";
	margin-top: 18px;
}
.today_reward_content{
	width: 108px;
	height: 112px;
	background-image: url(../img/tody_reward.png);
	margin: 0 auto;
	margin-top: 25px;
}
.today_reward_content span{
	color: white;
	font-family: "微软雅黑";
	font-size: 18px;
	display: inline-block;
	width: 100%;
	text-align: center;
	margin-top: 50px;
}
.today_task_alert_bottom{
	width: 100%;
	height: 125px;
	overflow: hidden;
	margin-top: 25px;
	margin-right: 15px;
}
.today_task_alert_bottom div{
	float: left;
}
.today_task_alert_bottom_left{
	width: 125px;
	height: 100%;
	background-color: white;
	margin-right: 15px;
}
.today_task_alert_bottom_right{
	width: 185px;
	height: 100%;
}
.download_poster_btn{
	width: 120px;
	height: 40px;
	background-image: url(../img/down_btn.png);
	margin-top: 15px;
}
.sweep_save{
	color: white;
	font-family: "微软雅黑";
	font-size: 19px;
	margin-top: 31px;
}





/*所有任务完成*/
.complete_task_wrap{
	width: 830px;
	height: 590px;
	background: url(../img/rw_bg.png) 0 0 no-repeat;
	background-size: 100% 100%;
	position: absolute;
	top: 50%;
	left: 50%;
	margin-left: -415px;
	margin-top: -275px;
	padding-top: 33px;
    box-sizing: border-box;
    z-index: 999;
}
.complete_task_content{
	width: 196px;
	height: 273px;
	margin: 0 auto;
	margin-top: 69px;
}
.complete_task_content .complete_task_content_img{
	width: 100%;
	height: 100%;
}
.complete_task_bottom_p{
	text-align: center;
	font-size: 30px;
	color: white;
	margin-top: 40px;
}
.complete_task_bottom_span{
	display: inline-block;
	width: 100%;
	text-align: center;
	font-size: 22px;
	color: white;
	margin-top: 30px;
}
.complete_task_not_start{
	width: 173px;
    position: absolute;
    left: 328px;
    top: 351px;
}
.complete_task_not_start>img{
	width: 173px;
}
.do_task_wrap_line,.do_task_bottom_text{
	display: none;
}
