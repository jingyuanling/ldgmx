.rank-wrap{
	width: 100%;
	height: 100%;
	font-family: "黑体";
	background-color: #faf3e3;
	background-size: 100% 100%;
	overflow-y: scroll;
	position: relative;
}
.rank-wrap .xf-big-bg{
	width: 100%;
	position: absolute;
	top: 0;
}
::-webkit-scrollbar{
	display: none;
}
*{
    margin:0;
    padding:0;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
}
.rank-banner{
	width: 100%;
	position: relative;
}
.rank-banner .rank-banner-img{
	width: 100%;
}
.rank-banner .rank-bird{
	width: 2.16rem;
	height: 3.46rem;
	position: absolute;
	bottom: -0.6rem;
	z-index: 9;
}
.rank-content{
	width: 100%;
	padding: 0.3rem;
	padding-bottom: 1rem;
	position: relative;
}
.rank-content-tit{
	font-size: 0.36rem;
	text-align: center;
	color: #333333;
}
.rank-content-tit img{
	width: 1.11rem;
	height: 0.29rem;
	position: relative;
	top: -0.05rem;
	display: inline-block;
}
.rank-table-wrap{
	width: 100%;
	border: 0.02rem solid rgba(185,113,77,0.5);
	border-radius: 0.15rem;
	overflow: hidden;
	margin: 0.55rem auto;
}
.rank-table{
	width: 100%;
	table-layout:fixed;
}
.rank-table th,.rank-table td{
	text-align: center;
	color: #a95027;
}
.rank-table th{
	height: 0.9rem;
	line-height: 0.9rem;
	background-color: rgba(234,210,189,0.5);
	font-size: 0.29rem;
}
.rank-table td{
	height: 0.75rem;
	line-height: 0.75rem;
	font-size: 0.29rem;
	border-bottom: 0.01rem solid #dab298;
}
.rank-left1,.rank-right1{
	position: absolute;
}
.rank-award-set{
	width: 100%;
}
.rank-award-set{
	height: 6.12rem;
	overflow: hidden;
	padding-top: 0.1rem;
	margin: 0.2rem auto 0.55rem;
}
.rank-award{
	border: 0.01rem solid #64976d;
	padding: 0.4rem 0;
	border-radius: 0.1rem;
	width: 5.14rem;
	float: left;
	background-color: white;
}
.rank-award-tit{
	font-size: 0.24rem;
	text-align: center;
	color: #a95027;
	font-family:'Source-Han-Ligh117d16a6761b4cb';
}
.rank-award-tit2{
	color: #a95027;
	font-size: 0.16rem;
	text-align: center;
	margin: 0.13rem;
}
.rank-award-tit2 span{
	display: inline-block;
	width: 0.06rem;
	height: 0.06rem;
	border-radius: 50%;
	background-color: #a95027;
	position: relative;
	top: -0.03rem;
}
.rank-award-tit3{
	font-size: 0.14rem;
	color: #666666;
	text-align: center;
	margin: 0.3rem 0 0.1rem;
}
.rank-award .rank-prize{
	width: 4.38rem;
	height: 3.76rem;	
	margin: 0 auto;
}
.rank-ruls{
	width: 100%;
	margin: 0.4rem auto;
	border-radius: 0.15rem;
	border: 0.01rem solid #64976d;
	padding: 0.4rem;
	background-color: #fefdf9;
	position: relative;
    z-index: 9;
}
.rank-ruls p{
	font-size: 0.29rem;
	color: #666666;
	line-height: 0.45rem;
	margin-top: 0.1rem;
}
.rank-award-wrap{
	width: 16.2rem;
}
.rank-bottom-bg{
	position: absolute;
	bottom: 0;
	left: 0;
	width: 100%;
}
.rank-statement{
	font-size: 0.31rem;
	color: #3F7E4A;
	text-align: center;
	position: absolute;
	bottom: 0.5rem;
	display: inline-block;
	width: 100%;
}
.rank-statement img{
	width: 0.3rem;
	height: 0.31rem;
	display: inline-block;
	margin-right: 0.1rem;
	margin-top: -0.05rem;
}
