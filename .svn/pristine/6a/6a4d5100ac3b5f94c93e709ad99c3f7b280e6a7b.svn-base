.wine-wrap{
	padding-top: 1.05rem;
}
.balance-nav-tab tr .balance_nav_active .line-red-all{
	width: 1.04rem;
	margin-left: -0.502rem;
}
ul.order-manage-ul{
	padding-top: 0.82rem;
}
ul.order-manage-ul li{
	padding: 0 0.37rem;
	margin-bottom: 0.17rem;
	background-color: #FFFFFF;
}
.order-manage-tit,.order-return-amount{
	height: 0.81rem;
	line-height: 0.8rem;
	overflow: hidden;
	border-bottom: 0.01rem solid #eaeaea;
}
.order-return-amount{
	padding: 0 0.37rem;
	background-color: #FFFFFF;
}
.order-return-amount .right-amount{
	float: right;
	color: #d10000;
	font-size: 0.3333rem;
}
.order-manage-tit span,.order-return-amount span{
	font-size: 0.25rem;
}
.order-manage-tit .left,.order-return-amount .left{
	color: #666666;
}
.order-manage-tit .right-state{
	color: #ff7b33;
	float: right;
}
.manage-goods-list{
	height: 1.9rem;
	width: 100%;
	padding: 0.2rem 0;
}
.manage-goods-list .goodsImg{
	float: left;
	width: 1.5rem;
	height: 1.5rem;
	margin-right: 0.2rem;
}
.manage-goods-list .goodsImg img{
	width: 100%;
	height: 100%;
}
.goodsName-price span{
	color: #333333;
}
.goodsName-price .name{
	font-size: 0.2709rem;
	width: 3.85rem;
	line-height: 0.3612rem;
	float: left;
	display: -webkit-box;
	-webkit-box-orient: vertical;
	-webkit-line-clamp: 2;
	overflow: hidden;
}
.goodsName-price .price{
	font-size: 0.2917rem;
	float: right;
}
.goodsName-price,.goodsName-num{
	overflow: hidden;
	margin-top: 0.1rem;
}
.goodsName-num span{
	font-size: 0.25rem;
	color: #999999;
}
.goodsName-num .left-size{
	width: 1.25rem;
	height: 0.42rem;
	background-color: #eeeeee;
	line-height: 0.42rem;
	text-align: center;
	border-radius: 0.03rem;
}
.goodsName-num .right-num{
	float: right;
}
.order-realy-pay{
	overflow: hidden;
	height: 1.04rem;
	line-height: 1.04rem;
	border-top: 0.01rem solid #eaeaea;
}
.order-realy-pay span{
	font-size: 0.25rem;
	color: #999999;
}
.right-pay-details{
	float: right;
}
.right-pay-details .text1{
	color: #333333;
}
.right-pay-details .num{
	font-size: 0.3751rem;
	color: #d10000;
	margin-right: 0.1rem;
}
.order-manage-pay-btn{
	width: 100%;
	height: 1.25rem;
	overflow: hidden;
	padding-top: 0.225rem;
	border-top: 0.01rem solid #eaeaea;
}
.order-manage-pay-btn a{
	width: 2.15rem;
	height: 0.7rem;
	font-size: 0.3124rem;
	display: inline-block;
	line-height: 0.7rem;
	text-align: center;
	float: right;
	border-radius: 0.05rem;
}
.order-manage-pay-btn .go-cancle{
	border: 0.01rem solid #a3a3a3;
	color: #666666;
	margin-right: 0.2rem;
}
.order-manage-pay-btn .go-pay{
	background-color: #d10000;
	color: #FFFFFF;
}
/*待收货*/
.order-manage-pay-btn .reminder{
	background-color: #FFFFFF;
	color: #d10000;
	border: 0.01rem solid #d10000;
}
