.wine-wrap{
	padding-top: 1.05rem;
	padding-bottom: 0;
}
.data-upper{
	width: 100%;
	height: 2.7rem;
	margin-bottom: 0.17rem;
}
.data-upper{
	padding-top: 0.37rem;
	background-color: #FFFFFF;
}
.data-upper .user-photo{
	width: 1.25rem;
	height: 1.25rem;
	display: block;
	margin: 0 auto;
	background-color: #a0a0a0;
	border-radius: 50%;
	overflow: hidden;
}
.data-upper .user-photo img{
	width: 100%;
	height: 100%;
}
.data-upper .text-center{
	font-size: 0.2916rem;
	height: 0.85rem;
	line-height: 0.85rem;
	color: #999999;
}
.data-down{
	width: 100%;
	padding: 0 0.37rem;
	background-color: #FFFFFF;
}
.data-down li{
	width: 100%;
	height: 1.05rem;
	line-height: 1.05rem;
	border-bottom: 0.01rem solid #e4e4e4;
}
.data-down li .left{
	font-size: 0.2916rem;
	color: #333333;
}
.data-down li .right{
	font-size: 0.2916rem;
	color: #999999;
	float: right;
}
.data-down li .right img{
	width: 0.14rem;
	height: 0.29rem;
	margin-left: 0.21rem;
	position: relative;
	top: -0.03rem;
}
.change-nickname-wrap{
	width: 100%;
	height: 2.3rem;
	background-color: #FFFFFF;
	padding: 0 0.37rem;
}
.nickname{
	height: 1.05rem;
	overflow: hidden;
	line-height: 1.05rem;
	border-bottom: 0.01rem solid #e7e7e7;
}
.nickname .left{
	font-size: 0.2917rem;
	color: #333333;
	height: 100%;
	width: 80%;
}
.nickname .right{
	width: 0.38rem;
	height: 0.38rem;
	float: right;
	margin-top: 0.335rem;
	margin-right: 0.1rem;
}
.change-nickname-wrap .annotation{
	font-size: 0.2499rem;
	color: #999999;
	line-height: 0.7rem;
}
