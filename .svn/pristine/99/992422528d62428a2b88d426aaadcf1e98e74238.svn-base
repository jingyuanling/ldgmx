@charset "utf-8";

body {
    width: 100%;
    font-family: "微软雅黑";
}
*{
    margin:0;
    padding:0;
}
img{
  border:0;
  display: block;
}
.clearfix:after {
visibility: hidden;
display: block;
font-size: 0;
content: " ";
clear: both;
height: 0;

}
.hongbao-tankuangceng{
    width: 100%;
    height: 100%;
    margin:0 auto;
    background-color:rgba(0,0,0,0.5);
    position: fixed;
}
.hongbao-tankuang{
    width: 600px;
    height: 600px;
    background-color: red;
    margin:0 auto;
    top: 50%;
    margin-top: -337px;
    position: relative;
    background:url(../img/double12tankuangpc.png) no-repeat 0 0;
    background-size: 600px 600px;
    position: relative;
}
.hongbao-tankuang p.wenzineirong{
    position: absolute;
    font-size:48px;
    font-family: "微软雅黑";
    color: #e3162e;
    margin-top: 130px;
    margin-left: 200px;
}
.hongbao-tankuang img{
    position: absolute;
    margin-top: 600px;
    margin-left: 175px;

}
