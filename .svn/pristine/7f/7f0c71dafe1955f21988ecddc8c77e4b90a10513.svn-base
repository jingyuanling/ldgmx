body{
    width: 100%;
    min-width: 320px;
    max-width: 640px;
    margin: 0 auto;
    font-family: "微软雅黑";
}
html{
    background-color: #eaeaea;
}
img{
  border:0;
  display: block;
}
.clearfix:after {
visibility: hidden;
display: block;
font-size: 0;
content: " ";
clear: both;
height: 0;
}
.dingdan-box{
    width: 100%;
    height: 100%;
    background-color: #eaeaea;
}
.main-top{
    width: 100%;
    height: 39px;
    background-color: #548e11;
}
.main-top img{
    position: absolute;
    float: left;
    width: 10px;
    margin-top: 10px;
    margin-left: 13px;

}
.main-top .tit{
    letter-spacing: 1px;
    font-size: 14px;
    color: #fff;
    text-align: center;
    line-height:36px;
    padding:0 30px;
}
.main-top .goback .tit li{
    list-style: none;
    width: 25%;
    text-align: center;
    float:left;
    font-size: 14px;
    color: #fff;
}
.main-top .goback .tit li span{
    display: block;
    margin:0 auto;
    width: 60px;
}
/*.main-top .goback .tit li span{
   width: 50px;
   display: block;
   border-bottom:2px solid #fff; 
   text-align: center;
   position: relative;
   left: 50%;
   margin-left: -25px;
   margin-top: -3px;
   display: none;
}*/
.main-top .goback .tit li span.active{
    border-bottom:2px solid #fff;
  
}

.dingdan-box ul{
    display: none;
}
.dingdan-box ul.have{
    display: block;
}
.dingdan{
    width: 100%;
    margin-top: 10px;
    background-color: #fff;
    padding: 5px 15px;
}
.dingdan p{
    font-size: 14px;
    color: #000;
    line-height: 25px;
}
.dingdan p span.right{
    float: right;
    font-size: 14px;
}
.dingdan p.time{
    font-size: 12px;
    color: #a29fa2;
}
.shangpin{
    padding: 10px;
    border-top:1px solid #cccccd;
}
.shangpin p{
    width: 100%;
    line-height: 35px;
}
.shangpin img{
    float: left;
    width: 25px;
    margin-right: 10px;
    margin-top: 3px;
}
.shangpin p span.shuliang{
    color:#a29fa2;;
}
.shangpin p.zongmoney{
    border-top:1px solid #cccccd;
    width: 100%;
    margin-top: 10px;
}
.shangpin p.zongmoney span{
    float: right;
}
.bottom{
    position: fixed;
    bottom:0;
    width: 100%;
    height:47px;
    min-width: 320px;
    max-width: 640px;
    
    background-color: #fff;
}
.bottom li{
    width: 33.3%;
    float: left;
    list-style: none;
}
.bottom li img{
    margin:0 auto;
    width: 27px;
    margin-top: 6px;
}
