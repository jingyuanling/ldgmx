.jd-safe-mode-box {
    background-color: #f0f2f5;
}
		.jd-safe-mode-msg {
    height: 35px;
    border-width: 0 0 1px 0;
    border-image: url("data:image/gif;base64,") 2 0 stretch;
    -webkit-border-image: url("data:image/gif;base64,") 2 0 stretch;
    background: #fff;
    line-height: 15px;
    width: 100%;
    text-align: center;
	
}


.jd-safe-mode-msg>span {
    font-size: 0;
    display: block;
}

.jd-safe-mode-msg span.msg-safe {
    font-size: 10px;
    color: #9a9a9a;
    display: inline-block;
    vertical-align: middle;
		margin:2px 10px 0 10px;
		text-align:left;
}