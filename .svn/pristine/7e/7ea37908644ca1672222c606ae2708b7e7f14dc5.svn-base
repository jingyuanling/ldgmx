/* pages/food/food.wxss */
.life_class_nav_wrap{
	width: 100%;
  height:100%;
	position: fixed;
	top: 100rpx;
	z-index: 10;
	max-width: 750px;
	min-width: 320px;
}
.life_food_wrap{
	width: 100%;
    height: 100%;
    padding-bottom: 150rpx;
    overflow: scroll;
	background-color: #ffffff;
}
.life_class_nav{
	width: 100%;
	overflow: hidden;
	background-color: #ffffff;
	border-bottom: 1rpx solid #f0f0f0;
	max-width: 750px;
	min-width: 320px;
}
.life_border_right {
    position: absolute;
    height: 60rpx;
    width: 10rpx;
    right: 0;
    top: 20rpx;
    border-right: 1rpx solid #f0f0f0;
}
.life_class_nav_programa,.life_class_nav_address{
	font-size: 36rpx;
	width: 50%;
	float: left;
	text-align: center;
	line-height: 100rpx;
	position: relative;
}
.life_class_nav_programa span,.life_class_nav_address span{
	position: relative;
	color: #666666;
}
.life_class_nav_triangle{
	position: absolute;
	top: 50%;
	left: 100%;
	border-left: 10rpx solid transparent;
	border-top: 10rpx solid #666666;
    border-right: 10rpx solid transparent;
    border-bottom: 10rpx solid transparent;
}
.life_class_details_wrap{
	width: 100%;
  height: 100%;
  background-color: rgba(0,0,0,0.5);
	/* float: left; */
}
.life_class_details_wrap_1{
	width: 100%;
	height: 100%;
	background-color: rgba(0,0,0,0.5);
}

.transparence{
	width: 50%;
	float: left;
	height: 100%;
	overflow-y: scroll;
	background-color: transparence;
}
.life_class_details{
	width: 100%;
	overflow: hidden;
	height: 500rpx;
}
.life_class_details_left_wrap,.life_class_details_right_wrap{
	width: 50%;
	float: left;
	height: 100%;
	overflow-y: scroll;
    background-color: #f0f0f0;
}
.life_class_details_left,.life_class_details_right{
	width: 100%;
}
.life_class_details_left ul li,.life_class_details_right ul li{
	font-size: 30rpx;
	padding-left: 6%;
	line-height: 60rpx;
	overflow: hidden;
}
.life_class_details_right_wrap{
	background-color: white;
	/* display: none;	 */
}
.life_class_details_right li image{
	float: right;
    width: 39rpx;
    height: 28rpx;
    margin-top: 10rpx;
    margin-right: 20rpx;
}
/*点击事件的样式*/
.life_class_nav_programa span{
/* 	color: #ff6b6a; */
}
.life_class_nav_triangle{
    border-left: 10rpx solid transparent;
    border-top: 10rpx solid #222222;
    border-right: 10rpx solid transparent;
    border-bottom: 10rpx solid transparent;
}
.life_class_details_left ul .li_left_active{
	color: #ff6b6a;
	background-color: white;
}
.life_class_details_right ul .li_right_active{
	border-bottom: 1rpx solid #ff6b6a;	
	color: #ff6b6a;
}
.home_good_recommend{
	width: 100%;
	padding: 20rpx 30rpx;
	box-sizing: border-box;
	background-color: #ffffff;
}
.home_good_recommend_list {
	width: 100%;
}
.home_good_recommend_list .good_list_left_img{
	float: left;
	width: 160rpx;
    height: 160rpx;
}
.home_good_recommend_list .good_list_left_img img{
	width: 100%;
	height: 100%;
}
.home_good_recommend_list .good_list_right_text{
	width: 100%;
	height: 160rpx;
    padding-left: 180rpx;
	box-sizing: border-box;
}
.home_good_recommend_list ul li{
	margin-top: 20rpx;
}
.good_list_right_text .right_text_goods_name,.good_list_right_text .good_list_right_address,.good_list_right_text .good_list_right_deduction{
	height: 55rpx;
	width: 100%;
	line-height: 55rpx;
	font-size: 30rpx;
	overflow: hidden;
}
.right_text_goods_name{
	color: #000000;
	overflow: hidden;
}
.right_text_goods_name span{
	font-size: 24rpx;
	color: #9d9d9d;
	margin-left: 20rpx;
	float: left;
}
.good_list_right_address{
	position: relative;
}
.good_list_right_text .right_text_goods_name p,.good_list_right_text .good_list_right_address p{
	display: inline-block;
	max-width: 340rpx;
	overflow: hidden;
	float: left;
}
.good_list_right_address span{
	display: inline-block;
	height: 35rpx;
	line-height: 35rpx;
	margin-top: 10rpx;
	padding: 0 20rpx;
	color: #ff7800;
	border: 1rpx solid #ff7800;
	position: absolute;
    right: 30rpx;
}
.good_list_right_deduction{
	color: #ff6b6a;
}
.good_list_right_deduction span{
	font-size: 34rpx;
                                                                                                                                                               }
