html,
body{
    margin: 0 auto;
    width: 100%;
    height: 100%;
    min-width: 1300px;
    font-family: Arial,"微软雅黑";
}
ul,li,p,h1,h2,h3,h4,h5,h6,img,br,hr,table,tr,td,dl,dt,dd,form {
    margin: 0;
    padding: 0;
}

ul,li {
    list-style: none;
}
a {
    text-decoration: none;
}
h1{
    font-size: 40px;
    text-align: center;
    padding: 20px 0px;
    font-weight: normal;
}
