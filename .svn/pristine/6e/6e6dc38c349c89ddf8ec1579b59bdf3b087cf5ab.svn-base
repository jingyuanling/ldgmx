<script type="text/javascript">
	var size = 5;
	var itemSize = 5;
	$(document).ready(function(){
	   var size2 = 3;
	   for(var i=1; i<=size2; i++){
	   	  for(var j=1; j<=size; j++){
		  	 $('#style_'+i+'_'+j).addClass("on");
		  }
	   }
	});	

	function selSurvey(n,fid){
		for(var i=1; i<=size; i++){
			if($('#style_'+n+'_'+i).hasClass("on")){
				$('#style_'+n+'_'+i).removeClass("on");
			}
		}
		
		for(var i=1; i<=fid; i++){
			$('#style_'+n+'_'+i).addClass("on");
		}
	}
	
	function submit(){
		var size = 3;
		for(var i=1; i<=size; i++){
			if(!($('#style_'+i+'_'+1).hasClass("on"))){
				var name = $('#style_'+i+'_'+1).attr("val");
				alert("请给" + name + "评分!");
				return false;
			}
		}
		
		var values = "";
    	for(var i=1; i<=size; i++){
			var score = $('#score_'+i).val();
			score = score.substring(0,score.length-1).split(',');
			if(score.length>0){
				var j = 1;
        		var value = "";
        		while($('#style_'+i+'_'+j).hasClass("on")){
        			j = j + 1;
        		}
        		//value = value + score[j-2];
				value = value + score[itemSize + 1 -j];
        		if(i == 1){
        			values = values + value;
        		}else{
        			values = values + "," + value;
        		}
			}
    	}
		
		jQuery.get(
			'/user/insertSurvey.json',
			{"orderId":"11053310873","surveyId":"549656","keys":"549656X69X1827,549656X69X1828,549656X69X1829","values":values},
            function(data){
                if(data!=null){
                   if(data.flag == "1"){
				   	  $('#content').html("感谢您的评价!");
				   }else{
				   	  if(data.message && data.message != null){
					  	 $('#content').html(data.message);
					  }else{
					     $('#content').html("评价失败,请稍后再试!");
					  }
				   }
				   endTime = new Date().getTime()/1000 + 2;
				   iconShow();
				   if(data.flag == "1"){
				  	  				   }
                }
            }, "json");
	}
	
	function iconShow(){
    	var nowTime = new Date().getTime()/1000;
    	var time = Math.floor(endTime - nowTime);
    	if(time > 0){
    		setTimeout("iconShow()",1000);
			var scrollleft = window.pageXOffset|| document.documentElement.scrollLeft || document.body.scrollLeft;
        	var allWidth = document.documentElement.clientWidth || document.body.clientWidth;
            var left = scrollleft-75+(allWidth/2);
        	document.getElementById('save').style.left=left+'px';	
			var scrollheight = window.pageYOffset|| document.documentElement.scrollTop || document.body.scrollTop;
        	var allHeight = document.documentElement.clientHeight || document.body.clientHeight;
            var top = 120;
        	document.getElementById('save').style.top=top+'px';
			$('.pop-attention').show();
    	}else{
			$('.pop-attention').hide();
		}
	}
</script>
    
