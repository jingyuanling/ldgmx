.gerendetails-box{
    width: 100%;
    height: 100%;
    background-color: #f3f5f7;
    padding-top: 0.8rem;
}
.xinxi{
    width: 100%;
    border-bottom:1px solid #bbbbbb;

}
.xinxi p{
    padding: 0 1em;
    line-height: 3em;
    color: #000;
    font-size: 0.3rem;
    border-top:0.01rem solid #d7d7d7;
    background-color: #f8f8f8;
    box-sizing: border-box;
}
.xinxi p input{
    outline: none;
    border:none;
    font-size: 0.8em;
    border: 1px solid #d7d7d7;
    border-radius: 0.2em;
    padding: 0.3em 0.5em;
    width: 70%;
    background-color: #f8f8f8;
    color: #999999;
}
.xinxi p input.check{
    width: 20px;
    float: right;
}
