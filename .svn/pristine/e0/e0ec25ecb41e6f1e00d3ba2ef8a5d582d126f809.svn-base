.wine_bottom{
    width: 100%;
    max-width: 640px;
    min-width: 320px;
    height:50px;
    background-color: #fff;
    position: fixed;
    bottom: 0;
    display: flex;
    justify-content: space-around;
    border-top: 1px solid #d7d7d7;
}
.wine_bottom li{
	height: 42px;
    float: left;
    list-style: none;
}
.wine_bottom li img{
    margin:0 auto;
    height: 42px;
    margin-top: 4px;
}