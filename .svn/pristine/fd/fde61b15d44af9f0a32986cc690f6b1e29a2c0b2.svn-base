.recently-used-code{
	border-bottom: 0.02rem solid #eeeeee;
}
.recently-used-code .text-left,.upload-code .text-left{
	font-size: 0.24rem;
	color: #191919;
	margin-top: 0.3rem;
	margin-bottom: 0.5rem;
}
.used-code-ul{
	display: flex;
	justify-content: space-around;
}
.used-code-ul li{
	width: 1.56rem;
	height: 1.56rem;
	margin-bottom: 0.7rem;
	position: relative;
}
.used-code-ul li img{
	width: 100%;
	height: 100%;
}
.used-code-ul .mask_layer{
	width: 100%;
	height: 100%;
	position: absolute;
	display: block;
	top: 0;
	left: 0;
	background-color: rgba(255,255,255,0.5);
}
.upload-code {
    padding-bottom: 0.4rem;
}
.upload-code-btn{
	width: 4.6rem;
	height: 4.6rem;
	margin: 0 auto;
	position: relative;
	overflow: hidden;
}
.upload-code-btn input{
	width: 100%;
	height: 4.8rem;
	position: absolute;
	top: -0.2rem;
	font-size: 0;
	outline: none!important;
}
.details-use-btm{
	height: 0.8rem;
	width: 100%;
	position: fixed;
	max-width: 750px;
	min-width: 320px;
	/*background-color: #f03b42;*/
	background-color: #999999;
	color: #FFFFFF;
	font-size: 0.3rem;
	bottom: 0;
}
.generate_alert{
	font-size: 0;
}
.generate_alert .modal-dialog{
	width: 5rem;
	height: 5rem;
	background-color: white;
	position: absolute;
	top: 50%;
	left: 50%;
	margin-top: -2.5rem;
	margin-left: -2.5rem;
	background-color: #f03b42;
}
.generate_alert .modal-content{
	width: 100%;
	height: 100%;
	border-radius: 0;
	background-color: #f03b42;
}
.generate_alert .right-icon{
	margin-top: 0.6rem;
	width: 1.38rem;
	height: 1.38rem;
	margin-bottom: 0.45rem;
}
.generate_alert .text-center{
	color: white;
}
.generate_alert .text-center1{
	font-size: 0.33rem;
	margin-bottom: 0.15rem;
}
.generate_alert .text-center2{
	font-size: 0.28rem;
	margin-bottom: 0.5rem;
}
.generate_alert .sure_btn{
	width: 4.3rem;
	height: 0.75rem;
	background-color: white;
	color: #f03b42;
	margin-left: 0.3rem;
	font-size: 0.3rem;
    border-radius: 0.1rem;
}
#upload-code-btn img{
	width: 100%;
}
