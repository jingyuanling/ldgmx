.wine-wrap{
	height: 100%;
	overflow-y: scroll;
}
.initiate-upper{
	padding: 0 0.37rem;
	background-color: #FFFFFF;
}
.initiate-upper p{
	height: 1.05rem;
	line-height: 1.05rem;
	padding: 0 0.1rem;
	border-bottom: 0.01rem solid #e1e1e1;
}
.initiate-upper p img{
	margin-left: 0.2rem;
	width: 0.29rem;
	height: 0.14rem;
}
.initiate-upper p:nth-last-child(1){
	border: none;
}
.initiate-upper p .left,.initiate-upper p .right{
	font-size: 0.2916rem;
	color: #333333;
}
.initiate-upper p .right{
	float: right;
}
.initiate-upper p .right a{
	color: #999999;
}
.initiate-upper p .amount{
	padding-right: 0.5rem;
}
.initiate-middle,.initiate-down{
	padding: 0 0.37rem;
	margin-top: 0.17rem;
	background-color: #FFFFFF;
}
.create-eval-goods{
	height: 1.05rem;
	line-height: 1.05rem;
}
.create-eval-goods .goods-title{
	font-size: 0.2916rem;
	border-bottom: 0.01rem solid #dedede;
}
.create-eval-goods .goods-title span{
	color: #999999;
	font-size: 0.25rem;
}
.eval-goodsImg{
	padding: 0.4rem 0;
}
.initiate-submit{
	width: 100%;
	height: 1.05rem;
	color: #FFFFFF;
	background-color: #d10000;
	font-size: 0.3333rem;
	position: absolute;
	bottom: 0;
	min-width: 320px;
	max-width: 750px;
}
/*弹框*/
.initi-type-alert,.initi-reason-alert{
	width: 100%;
	height: 100%;
	background-color: rgba(0,0,0,0.5);
	position: fixed;
	top: 0;
	max-width: 750px;
	min-width: 320px;
	z-index: 9;
	display: none;
}
.initi-type-wrap,.initi-reason-wrap{
	width: 100%;
	height: 6.45rem;
	background-color: #FFFFFF;
	position: absolute;
	bottom: 0;
	color: #333333;
	padding: 0 0.37rem;
}
.initi-type-tit,.initi-reason-tit{
	font-size: 0.2916rem;
	color: #333333;
	text-align: center;
	height: 1.46rem;
	line-height: 1.45rem;
	border-bottom: 0.01rem solid #e1e1e1;
}
.initi-type-wrap li,.initi-reason-wrap li{
	height: 1.25rem;
	padding: 0.27rem 0.18rem 0 0.08rem;
	border-bottom: 0.01rem solid #e1e1e1;
}
.initi-type-wrap p.shang,.initi-reason-wrap p.shang{
	font-size: 0.2916rem;
	height: 0.34rem;
	line-height: 0.34rem;
}
.initi-type-wrap p.xia{
	height: 1.25rem;
	font-size: 0.2083rem;
	color: #999999;
}
.initi-type-wrap .pay-right-sel,.initi-reason-wrap .pay-right-sel{
	width: 0.34rem;
    height: 0.34rem;
    margin-right: 0.13rem;
    border: 0.04rem solid #7d7d7d;
    border-radius: 50%;
    float: right;
    position: relative;
}
.initi-type-wrap .pay-right-sel img,.initi-reason-wrap .pay-right-sel img{
	width: 0.24rem;
    height: 0.16rem;
    position: absolute;
    top: 0.09rem;
}
.initi-type-wrap .pay_sel_active,.initi-reason-wrap .pay_sel_active{
	border: none;
    background-color: #d10000;
}
.initi-type-wrap .pay_sel_active img,.initi-reason-wrap .pay_sel_active img{
	left: 0.05rem;
}
.initi-type-wrap .initi-type-sure,.initi-reason-wrap .initi-reason-sure{
	width: 7.5rem;
	font-size: 0.3333rem;
	color: #FFFFFF;	
	text-align: center;
	position: absolute;
	bottom: 0;
	left: 0;
	height: 1.05rem;
	background-color: #d10000;
}
.initi-reason-wrap li{
 	height: 1.05rem;
 }
.initi-reason-wrap li{
 	padding-top: 0.35rem;
 }
.initi-reason-wrap{
	height: 6.76rem;
}
.initi-reason-wrap ul{
	height: 4.2rem;
	overflow-y: scroll;
}
