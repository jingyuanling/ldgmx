html,
body{
    margin: 0 auto;
    width: 100%;
    height: 100%;
    min-width: 320px;
    max-width: 640px;
    overflow: hidden;
    font-family: Arial,"微软雅黑";
}
ul,li,p,h1,h2,h3,h4,h5,h6,img,br,hr,table,tr,td,dl,dt,dd,form {
    margin: 0;
    padding: 0;
}

ul,li {
    list-style: none;
}
img {
    border: none;
}
a {
    text-decoration: none;
    -webkit-tap-highlight-color: transparent;
}
