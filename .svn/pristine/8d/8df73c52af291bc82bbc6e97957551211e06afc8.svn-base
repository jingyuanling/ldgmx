.zhifu-box{
    width: 100%;
    background-color:#f0f0f2;
    padding: 0.8rem 0;
}
.main-top{
    width: 100%;
    height: 39px;
    background-color: #548e11;
}
.main-top img{
    position: absolute;
    float: left;
    width: 10px;
    margin-top: 10px;
    margin-left: 13px;
}
.main-top img.serch{
    position: relative;
    float: right;
    width: 18px;
    margin-top: -28px;
    margin-right: 13px;

}
.main-top p{
    width: 100%;
    letter-spacing: 2px;
    font-size: 14px;
    color: #fff;
    text-align: center;
    line-height:39px;
}
.yonghu{
    width: 100%;
    padding: 0.2rem;
    background-color: #fff;
    font-size: 0.3rem;
    color: #222222;
    border: 0.01rem solid #d7d7d7;
}
.yonghu .phone_num{
	font-size: 0.26rem;
	color: #999999;
}
.yonghu p.dizhi{
    color: #827c82;
    font-family: "黑体";
    margin-top: 0.1rem;
    position: relative;
}
.yonghu .right{
    position: absolute;
    right: 0;
    bottom: 0;
    height: 0.3rem;
}
.yonghu .right img{
	height: 100%;
}
.select{
    padding: 10px 20px;
    width: 100%;
    margin-top: 0.2rem;
    background-color: #fff;
    font-size: 0.3rem;
    border: 0.01rem solid #d7d7d7;
}
.select p{
	margin-bottom: 0.05rem;
    overflow: hidden;
}
.select p span{
    float: left;
}
.select p img{
    width: 0.35rem;
    padding-top: 0.08rem;
    margin-right: 0.2rem;
}
.select p span.money{
    color: #827c82;
    margin-left: 0.2rem;
    font-size: 0.24rem;
    margin-top: 0.05rem;
}
.select p span.xuan{
    width: 0.2rem;
    height: 0.2rem;
    border-radius: 50%;
    border:1px solid #827c82;
    float: right;
    margin-top: 0.1rem;
    margin-top: 0.1rem;
    box-sizing: border-box;
    /*选中状态*/
    /*background-color: #d9333f;*/
}
.select p span.xuan:hover{
	background-color: #d9333f;
}
.active{
	background-color: #d9333f;
}
.youhui{
    height: 40px;
    line-height: 20px;
}
.youhui p span img{
    margin-top: 1px;
}
.youhui p span.money{
    height: 20px;
    padding:8px;
    background-color: #e63c2c;
    color: #fff;
    line-height: 6px;
}
.jiesuan {
    border: 0.01rem solid #d7d7d7;
    background-color: #fff;
    margin-top: 0.2rem;
	padding: 0 0.2rem 0.2rem;
}
.jiesuan .top{
	width: 100%;
    height: 1rem;
    padding-top: 0.15rem;
    box-sizing: border-box;
    position: relative;
}
.jiesuan div p{
    font-size: 0.26rem;
}
.jiesuan .left_goods_img{
	float: left;
	width: 0.7rem;
    height: 0.7rem;
}
.jiesuan div img{
    width: 100%;
    height: 100%;
}
.jiesuan .geshu{
	color: #999999;
    margin-right: 0.2rem;
    font-size: 0.24rem;
    position: absolute;
    left: 3.8rem;
    bottom: 0;
}
.jiesuan .qian{
    font-size: 0.3rem;
    color: #d9333f;
    position: absolute;
    left: 4.5rem;
    bottom: 0;
}
.fukuan{
    position: absolute;
    bottom: 0;
    max-width: 640px;
    min-width: 320px;
    width: 100%;
    height: 0.9rem;
    background-color: #fff;
    border-top:1px solid #d7d7d7;
    border-bottom: 1px solid #d7d7d7;
}
.fukuan p{
    font-size: 0.28rem;
    width: 50%;
    float: left;
    line-height: 0.9rem;
    text-align: center;
    color: #e84e40;
}
.fukuan p:nth-child(1) a{
	color: #222222;
}
.fukuan p.anniu{
    background-color: #e84e40;
    color: #fff;

}
.jiesuan>p{
	font-size: 0.26rem;
}
.jiesuan>p span{
	color: #d9333f;
}
.wine_HB_num{
    font-size: 0.2rem;
    background-color: #d9333f;
    padding: 0.02rem 0.1rem;
    color: white;
    border-radius: 0.03rem;
    margin-left: 0.2rem;
}
/*支付成功样式*/
.wine_pay_success_wrap{
	width: 100%;
	padding: 0.8rem 0.2rem;
	background-color: #f3f5f7;
}
.wine_pay_success_logo{
	width: 100%;
	border-bottom: 1px solid #d7d7d7;
	padding: 0.7rem 0;
}
.wine_pay_success_logo img{
	border-radius: 50%;
	width: 1.3rem;
	height: 1.3rem;
	margin: 0 auto;
	margin-bottom: 0.2rem;
}
.wine_pay_success_logo p{
	text-align: center;
	font-size: 0.36rem;
}
.wine_pay_success_details{
	width: 100%;
	padding: 2em 0;
}
.wine_pay_success_details p{
	text-align: center;
	font-size: 0.36rem;
}
p.wine_pay_success_text_p2 span{
	color: #d9333f;
}
.wine_pay_success_details .wine_see_btn{
	background-color: #ffa21d;
	color: white;
	padding: 0.2rem 0.4rem;
	border-radius: 0.1rem;
}
.wine_pay_success_details .wine_return_index_btn{
	background-color: #d9333f;
	color: white;
	padding: 0.2rem 0.4rem;
	border-radius: 0.1rem;
}
.wine_pay_success_text_p1{
	margin-bottom: 0.2rem;
}
.wine_pay_success_text_p3{
	margin-top: 0.55rem;
}
/*优惠券弹出框*/
.ZF_coupon_box_wrap{
	width: 100%;
	height: 100%;
	max-width: 640px;
	min-width: 320px;
	background-color: rgba(0,0,0,0.5);
	position: absolute;
	top: 0;
	display: none;
	overflow: hidden;
}
.ZF_coupon_box{
	width: 100%;
	position: absolute;
	bottom: -100%;
	background-color: #f8f8f8;
	padding: 0 0.2rem;
	box-sizing: border-box;
	padding-bottom: 0.3rem;
}
.ZF_coupon_box_logo{
	text-align: center;
	padding: 0.5em 0;
	border-bottom: 1px solid #d7d7d7;
}
.ZF_coupon_box_logo img{
	display:inline-block;
	margin-right: 0.2rem;
	width: 0.4rem;
}
.ZF_coupon_box_logo{
	color: #d9333f;
	font-size: 0.3rem;
}
.ZF_coupon_box_title{
	width: 100%;
	border-bottom: 1px solid #d7d7d7;
	overflow: hidden;
}
.ZF_coupon_box_titleL,.ZF_coupon_box_titleR{
	float: left;
	width: 50%;
	text-align: center;
	font-size: 0.3rem;
	padding: 0.2rem 0;
}
.ZF_voucher_details li,.ZF_voucher_details2 li{
	margin-top: 0.2rem;
	font-size: 0.28rem;
}
.ZF_voucher_details li span{
	display: inline-block;
	width: 1em;
	height: 1em;
	border: 1px solid #d9333f;
	border-radius: 0.2em;
	margin: 0 1em;
}
.ZF_voucher_details li p,.ZF_voucher_details2 li p{
	padding-left: 3em;
	box-sizing: border-box;
	margin-top: 0.4em;
	color: #c5c5c5;
}
.ZF_voucher_details,.ZF_voucher_details2{
	height: 3rem;
	overflow-y: scroll;
}
.ZF_voucher_details2{
	display: none;
}
.ZF_voucher_details2{
	color: #c5c5c5;
}
.ZF_voucher_details2 li span{
	display: inline-block;
	width: 1em;
	height: 1em;
	border: 1px solid #c5c5c5;
	border-radius: 0.2em;
	margin: 0 1em;
}
