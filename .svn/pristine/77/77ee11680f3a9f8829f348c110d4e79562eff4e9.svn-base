.love520-wrap{
	width: 100%;
	min-width: 1245px;
	font-family: "微软雅黑";
	position: relative;
	overflow-x: hidden;
}
.love520-wrap-img{
	width: 100%;
	position: absolute;
	top: 0;
	left: 0;
}
.love520-banner,.love520-banner img{
	width: 100%;
}
.love520-content-wrap{
	width: 100%;
	background: url(../img/content-bg.png) 0 0 no-repeat;
	background-size: 100% 100%;
	position: relative;
	padding-bottom: 100px;
	padding-top: 30px;
}
.love520-content{
	width: 1245px;
	margin: 0 auto;
	position: relative;
}
.love520-wrap span,.love520-wrap img{
	display: inline-block;
}
.module-one{
	margin-bottom: 95px;
}
.module-big-tit{
	text-align: center;
}
.module-big-tit img{
	width: 1200px;
	margin: 0 auto;
}
.module-ruls{
	width: 1245px;
	padding-bottom: 170px;
	margin-top: 50px;
	position: relative;
}
.module-ruls-center,.module-list-ul li{
	width: 1200px;
	margin: 0 auto;
	padding: 90px 110px;
	color: #ef384e;
	border-radius: 12px;
	border: 1px solid #f6a299;
	background-color: white;
	-webkit-box-shadow:0px 0px 100px rgba(255,170,165,1) inset;
	-moz-box-shadow:0 0 100px rgba(255,170,165,1) inset;
	box-shadow:0 0 100px rgba(255,170,165,1) inset;
	position: relative;
}
.module-ruls-center h1{
	font-size: 30px;
	line-height: 32px;
	font-weight: bold;
	margin-bottom: 40px;
	margin-bottom: 35px;
}
.module-ruls-center p{
	font-size: 20px;
	margin-top: 20px;
	line-height: 30px;
}
.module-ruls-btn-wrap{
	width: 100%;
	height: 312px;
	background: url(../img/module-bg.png) 0 0 no-repeat;
	background-size: 100% 100%;
	position: absolute;
	bottom: 0;
	left: 0;
}
.module-buy-btn{
	height: 70px;
	width: 260px;
	display: block;
	color: #FFFFFF;
	font-size: 24px;
	line-height: 70px;
	text-align: center;
	margin: 180px auto 0;
	border-radius: 70px;
	position: relative;
	border: 2px solid #FFFFFF;
	padding-right: 12px;
}
.module-buy-btn span{
	font-size: 50px;
	position: absolute;
	font-weight: 100;
	top: -3px;
    margin-left: 3px;
}
.module-buy-btn:hover{
	color: #ef384e;
	background-color: #FFFFFF;
}
.module-list-ul{
	width: 1200px;
    margin: 50px auto 0;
	overflow: hidden;
}
.module-list-ul li{
	width: 384px;
	height: 482px;
	padding: 0;
	float: left;
	margin-bottom: 24px;
	margin-right: 24px;
	position: relative;
}
.leftTop-logo{
	position: absolute;
	top: 30px;
	left: 42px;
}
.module-goodsImg{
	width: 300px;
	height: 300px;
	margin-top: 4px;
	margin-left: 42px;
	border-radius: 14px;
	overflow: hidden;
}
.module-goodsImg img{
	width: 100%;
	height: 100%;
}
.module-goods-name{
	font-size: 16px;
	height: 56px;
	color: #666666;
	line-height: 28px;
	display: -webkit-box;
	-webkit-box-orient: vertical;
	-webkit-line-clamp: 2;
	overflow: hidden;
	padding: 0 30px;
	margin-bottom: 25px;
}
.module-price{
	display: block;
	margin: 0 auto;
	width: 300px;
	height: 50px;
	border-radius: 50px;
	overflow: hidden;
	border: 2px solid #ff6260;
}
.module-price span,.module-price s{
	display: inline-block;
	width: 50%;
	height: 100%;
	line-height: 50px;
	text-align: center;
}
.module-price span{
	color: #FFFFFF;
	font-weight: bold;
	background-color: #ff6260;
}
.module-price span:hover{
	background-color: #ff7f7d;
}
.module-price s{
	color: #999999;
}
.module-list-ul li:nth-child(3n){
	margin-right: 0;
}
.btm-peach-blossom,.btm-peach-blossom img{
	width: 100%;
}
.btm-peach-blossom{
	height: 100px;
	position: absolute;
	bottom: 0;
	left: 0;
}
.btm-peach-blossom img{
	position: absolute;
	bottom: 0;
	left: 0;
}
.module-ruls .hert1{
	position: absolute;
	right: -120px;
	top: 80px;
}
.love520-content-wrap .waves{
	width: 100%;
	position: absolute;
	top: 1400px;
}
.love520-content-wrap .btn-text{
	text-align: center;
	position: absolute;
	width: 100%;
	bottom: 50px;
}
.love520-content-wrap .btn-text img{
	width: 334px;
	height: 84px;
}
@media only screen and (max-width: 1245px) {
	.love520-wrap{
		overflow-x: scroll;
	}
}