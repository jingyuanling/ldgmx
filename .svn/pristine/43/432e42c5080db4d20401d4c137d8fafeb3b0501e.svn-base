html,body{
	width: 100%;
	height:100%;
}
.swiper-container {
	width: 100%;
    height: 100%;
	background:#000;
}  
.swiper-slide{
	width:100%;
	height:100%;
	background:url(../img/wrapbg.jpg) no-repeat left top;
	background-size:100% 100%;
}
img{
	left: 50%;
	display:block;
}
@-webkit-keyframes start {
	0%,30% {opacity: 0;-webkit-transform: translate(0,10px);}
	60% {opacity: 1;-webkit-transform: translate(0,0);}
	100% {opacity: 0;-webkit-transform: translate(0,-8px);}
}
@-moz-keyframes start {
	0%,30% {opacity: 0;-moz-transform: translate(0,10px);}
	60% {opacity: 1;-moz-transform: translate(0,0);}
	100% {opacity: 0;-moz-transform: translate(0,-8px);}
}
@keyframes start {
	0%,30% {opacity: 0;transform: translate(0,10px);}
	60% {opacity: 1;transform: translate(0,0);}
	100% {opacity: 0;transform: translate(0,-8px);}
}
.ani{
	position:absolute;
	}
.txt{
	position:absolute;
}
#array{
	position:absolute;z-index:999;-webkit-animation: start 1.5s infinite ease-in-out;
}
/*第七页*/
.page-seven-ul{
	position: absolute;
	top: 278px;
	left: 50%;
	margin-left: -135px;
}
.page-seven-ul li{
	margin-bottom: 10px;
}
.page-seven-ul p{
	font-size: 18px;
	color: white;
	margin-bottom: 5px;
	text-align: left;
}
.page-seven-ul input,.swiper-slide .page-seven-tellus{
	width: 270px;
	outline: none;
	height: 35px;
	color: white;
	background-color: transparent;
	border: 1px solid white;
	border-radius: 3px;
	line-height: 35px;
	font-size: 15px;
	padding: 0 5px;
}
.swiper-slide .page-seven-tellus{
	display: block;
	position: absolute;
	top: 498px;
	border: none;
	left: 50%;
	margin-left: -135px;
}
.swiper-slide .page-seven-tellus img{
	width: 100%;
	height: 100%;
	position: absolute;
	top: 0;
	left: 0;
}
@media only screen and (max-width: 330px) {
	.page-seven-ul{
		top: 220px;
	}
	.page-seven-ul input, .swiper-slide .page-seven-tellus{
		height: 29px;
		line-height: 29px;
	}
	.swiper-slide .page-seven-tellus{
		top: 415px;
	}
	.page-seven-ul p{
		font-size: 16px;
	}
}
@media only screen and (min-width: 760px) {
	.page-seven-ul{
		top: 445px;
	}
	.page-seven-ul p{
		font-size: 30px;
	}
	.page-seven-ul input, .swiper-slide .page-seven-tellus{
		width: 400px;
		height: 50px;
		line-height: 50px;
		font-size: 25px;
	}
	.swiper-slide .page-seven-tellus{
		top: 775px;
	}
	.page-seven-ul p{
		font-size: 30px;
	}
	.page-seven-ul,.swiper-slide .page-seven-tellus{
		margin-left: -200px;
	}
	.swiper-slide7 .smalltitle{
		top: 395px!important;
	}
	.page5content{
		width: 568px!important;
		left: 50%!important;
		margin-left: -284px;
	}
	.page5success{
		left: 88px!important;
	}
	#page2logo,#page4logo{
		width: 520px!important;
		left: 50%!important;
		margin-left: -260px;
	}
	.page4center{
		width: 500px!important;
		left: 50%!important;
		top: 375px!important;
		margin-left: -250px;
	}
}
@media only screen and (min-width: 1020px){
	.page-seven-ul{
		top: 600px;
	}
	.swiper-slide .page-seven-tellus{
		top: 950px;
	}
	.page4center{
		top: 500px!important;
	}
}