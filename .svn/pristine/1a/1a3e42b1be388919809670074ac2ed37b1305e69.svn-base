body{
    width: 100%;
    min-width: 320px;
    max-width: 640px;
    margin: 0 auto;
    font-family: "微软雅黑";
}
html{
     background-color: #f0f0f2;
}
img{
  border:0;
  display: block;
}
.clearfix:after {
visibility: hidden;
display: block;
font-size: 0;
content: " ";
clear: both;
height: 0;
}
.zhifuok-box{
    width: 100%;
    background-color:#f0f0f2
}
.main-top{
    width: 100%;
    height: 39px;
    background-color: #548e11;
}
.main-top img{
    position: absolute;
    float: left;
    width: 10px;
    margin-top: 10px;
    margin-left: 13px;

}
.main-top p.ok{
    width: 36px;
    float: right;
    margin-top: -40px;
    margin-right: 13px;
    color: #c6d6ba;
}
.main-top p{
    width: 100%;
    letter-spacing: 2px;
    font-size: 14px;
    color: #fff;
    text-align: center;
    line-height:39px;
}
.z-ok{
    width: 200px;
    position: relative;
    left: 50%;
    margin-left: -100px;
    height: 150px;
    padding-top:45px;
}
.z-ok img{
    float: left;
    width: 50px;
    margin-right: 10px;
}
.z-ok p{
    font-size: 14px;
    color: #000;
    line-height: 25px;
}
.z-ok p span.jine{
    color: red;
}
.check{
    /*margin-top: 10px;*/
    width: 274px;
    position: relative;
    left: 50%;
    margin-left: -137px;
}
.check p{
    margin-right: 20px;
    width:125px;
    height: 37px;
    float: left;
    background-color: #fff;
    border-radius: 8px;
    border:1px solid #cfcfcf;
    font-size: 14px;
    line-height: 35px;
    text-align: center;

}
.check p.right{
    margin-right: 0;
}