body{
    width: 100%;
    min-width: 320px;
    max-width: 640px;
    margin: 0 auto;
    font-family: "微软雅黑";
}
html{
    background-color:#ebebeb
}
img{
  border:0;
  display: block;
}
.clearfix:after {
visibility: hidden;
display: block;
font-size: 0;
content: " ";
clear: both;
height: 0;
}
.fujinmendian-box{
    width: 100%;
    height: 100%;
    background-color: #eaeaea;
}
.main-top{
    width: 100%;
    height: 39px;
    background-color: #548e11;
    font-family:"微软雅黑";
}
.main-top img{
    position: absolute;
    float: left;
    width: 10px;
    margin-top: 10px;
    margin-left: 13px;

}
.main-top img.serch{
    position: relative;
   /* position: absolute;*/
    float: right;
    width: 15px;
    margin-top: -30px;
    margin-right: 13px;

}
.main-top p{
    width: 100%;
    letter-spacing: 2px;
    font-size: 14px;
    color: #fff;
    text-align: center;
    line-height:39px;
}

.dian-tit{
    width: 100%;
    padding: 8px 19px;
    background-color: #f0f0f2;
    margin-top: 3px;
}
.dian-tit img{
    width: 69px;
    float: left;
}
.dian-tit .cont{
    padding: 5px;
    float: left;
    margin-left: 10px;
}
.dian-tit .cont p{
   color: #000;
   font-size: 14px;
   letter-spacing: 1px;
   line-height: 15px;
}
.dian-tit .cont p.p2{
   color: #8b8b8b;
   font-size: 12px;
   line-height: 20px;
}
.dian-tit .cont p.juli{
    background-color: #fff;
    color:#7ebb38;
    letter-spacing:0px;
    font-size: 12px;
    border-radius: 4px;
    text-align: center;
    margin-top: 3px;
}
.zhonglei{
    width: 100%;
    padding-left: 80px;
    margin-top: 62px;
}
.zhonglei p{
   /* border-top: 1px solid #d9d7d9;*/
    padding-top: 5px;
    margin-top: 2px;

}
.zhonglei p span{
    float: left;
    display: block;
    padding:2px 4px;
    border-radius:3px;
    font-size: 12px;
    color: #fff;
    background-color: #548e11;
    margin-right: 5px;
    margin-bottom: 3px;
}