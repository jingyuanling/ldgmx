.upload-order-wrap{
	width: 100%;
	height: 100%;
	padding-top: 0.88rem;
	padding-bottom: 1.1rem;
	color: #999999;
	overflow-y: scroll;
}
.upload-order-upper{
	width: 100%;
	padding: 0 0.3rem;
	margin-bottom: 0.34rem;
}
.upload-order-upper .upload-tit{
	font-size: 0.36rem;
	line-height: 0.8rem;
}
.upload-photo-frame{
	width: 100%;
	height: 2.77rem;
	border: 0.03rem dashed #ececec;
}
.upload-photo-frame .add-gray-img{
	width: 0.7rem;
	height: 0.7rem;
	margin: 0 auto;
	margin-top: 0.6rem;
	margin-bottom: 0.2rem;
}
.upload-photo-frame p{
	font-size: 0.22rem;
	text-align: center;
	font-weight: lighter;
}
.upload-log-inform{
	border-top: 0.02rem solid #ececec;
}
.upload-log-inform p{
	padding: 0 0.3rem;
	line-height: 0.78rem;
	overflow: hidden;
	font-size: 0.32rem;
	border-bottom: 0.02rem solid #ececec;
}
.upload-log-inform .log-choose-tit{
	font-size: 0.35rem;
}
.upload-log-inform .log-choose-tit img{
	width: 0.55rem;
	height: 0.32rem;
	float: right;
	margin-top: 0.23rem;
}
/*隐藏区域*/
/*点击上传运单号弹框*/
.upload-photo-gray,.logistics-compan-gray{
	width: 100%;
	height: 100%;
	position: fixed;
	top: 0;
	min-width: 320px;
	max-width: 750px;
	background-color: rgba(0,0,0,0.5);
	display: none;
}
.upload-photo-alert{
	width: 5.9rem;
	height: 2.95rem;
	background-color: white;
	position: absolute;
	top: 50%;
	left: 50%;
	margin-top: -1.48rem;
	margin-left: -2.95rem;
	border-radius: 0.07rem;
}
.upload-photo-alert .alert-tit{
	font-size: 0.3rem;
	overflow: hidden;
	line-height: 0.5rem;
	padding: 0 0.3rem;
	background-color: #f2f1f1;
}
.upload-photo-alert .alert-tit .alert-delect{
	width: 0.28rem;
	height: 0.28rem;
	float: right;
	margin-top: 0.11rem;
}
.upload-photo-alert .alert-con-text{
	font-size: 0.36rem;
	color: #666666;
	text-align: center;
	line-height: 1.3rem;
}
.upload-photo-alert .alert-con-text img{
	width: 0.63rem;
	height: 0.68rem;
	display: inline-block;
	margin-right: 0.15rem;
}
.upload-photo-alert .alert-two-btn{
	display: flex;
	justify-content: space-around;
}
.upload-photo-alert span{
	width: 2.4rem;
	height: 0.7rem;
	font-size: 0.34rem;
	text-align: center;
	line-height: 0.7rem;
	color: white;
	display: inline-block;
	border-radius: 0.07rem;
}
.upload-photo-alert .take-picture{
	background-color: #d65151;
}
.upload-photo-alert .local-upload{
	background-color: #bf0303;
}
/*物流公司隐藏页*/
.logistics-alert{
	width: 100%;
	background-color: white;
	position: absolute;
	bottom: 0;
}
.logistics-alert-tit{
	font-size: 0.36rem;
	color: #333333;
	padding: 0 0.3rem;
	line-height: 0.77rem;
	border-bottom: 0.02rem solid #ececec;
}
.logistics-alert-ul{
	height: 4.26rem;
	width: 100%;
	overflow: hidden;
}
.logistics-alert-ul li,.logistics-alert-more{
	color: #666666;
	font-size: 0.3rem;
	height: 0.71rem;
	line-height: 0.69rem;
	padding: 0 0.3rem;
	border-bottom: 0.02rem solid #ececec;
}
.logistics-alert-more{
	display: inline-block;
	width: 100%;
	border: none;
	margin-bottom: 0.2rem;
}

