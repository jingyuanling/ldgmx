body{
	background-color: #f3f5f7;
}
.wine_agency_wrap{
	width: 100%;
}
.wine_agency_balance{
	padding-top: 39px;
	padding-bottom: 0.7em;
	width: 100%;
	background-color: #cd222f;
}
.wine_agency_balance p{
	text-align: center;
	color: white;
}
.wine_agency_balance .wine_agency_balance_p1{
	font-size: 1.2em;
	margin-top: 1.0em;
}
.wine_agency_balance .wine_agency_balance_p2{
	font-size: 2.2em;
	margin-top: 0.5em;
}
.wine_agency_money_details{
	width: 100%;
	overflow: hidden;
}
.wine_agency_money_details div{
	width: 50%;
	float: left;
	padding: 0.3em 0;
	background-color: #cd222f;
	box-sizing: border-box;
}
.wine_agency_money_details .left{
	border-right: 1px solid white;
}
.wine_agency_money_details div p{
	text-align: center;
	color: white;
}
.wine_agency_money_details div p:nth-child(1){
	font-size: 1.2em;
}
.wine_two_btn{
	width: 100%;
	display: flex;
	justify-content: space-around;
	padding: 20px 0;
}
.wine_two_btn>div{
	width: 40%;
	text-align: center;
	font-size: 0.8em;
	line-height: 2.8em;
	border-radius: 0.4em;
	color: white;
}
.wine_two_btn .wine_withdrawal_btn{
	background-color: #e3335b;
}
.wine_two_btn .wine_commission_btn{
	background-color: #ffa21d;
}
.wine_three_div{
	overflow: hidden;
	padding: 0 0.8em;
	margin-bottom: 0.2em;
	font-size: 1.2em;
	line-height: 3.2em;
	border-top: 1px solid #d7d7d7;
	border-bottom: 1px solid #d7d7d7;
}
.wine_three_div .wine_gt,.wine_percent{
	float: right;
}
.wine_percent{
	margin-right: 0.2em;
}
.wine_three_div .wine_gt{
	font-weight: 700;
}
.wine_three_div span img{
	display: inline-block;
	margin-right: 0.5em;
	height: 1.2em;
}
